		If nColPe > 0  .And. nColTi > 0 .And.  (aCOLS[n,nColTi ] <> "M" .Or.  aCOLS[n,nColTi] <> M->TL_TIPOREG)
			aCOLS[n,nColPe] := 0.00
		EndIf
	EndIf
