			oBrw6:oBrowse:nAt := Len(aCoBrw6)
		EndIf
	EndIf
	n := oBrw6:oBrowse:nAt
	aCols := aClone(aCoBrw6)
	oBrw6:oBrowse:Refresh()
	fLoadSTL(.F.)

	oBrw4:Refresh()
	oBrw4:GoTop()

Return

