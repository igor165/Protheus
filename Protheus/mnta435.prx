								(cTRBQ400)->TQ_OK      := 'XX'
								(cTRBQ400)->TQ_CODFUNC := oBrw6:aCols[nTail,nCodIn1]
								(cTRBQ400)->TQ_SEQRELA := oBrw6:aCols[nTail,nSeqRel]

