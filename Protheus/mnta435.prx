Static Function MNTA435Mrk()

	Local aAreaTRB := (cTRBQ400)->( FWGetArea() )

	If SuperGetMV( 'MV_NG1ETPM', .F., 'N' ) == 'S'

		dbSelectArea( cTRBQ400 )
		dbSetOrder( 5 ) // TQ_MARK
