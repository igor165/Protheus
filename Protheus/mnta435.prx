Static Function MNT435CALLB( nOp )

	Local lRet  := .F.

	If ExistBlock( "MNTA4358" )
		lRet := ExecBlock( "MNTA4358",.F.,.F., { nOp,(cTRBIP) } )
		If !lRet
			Return .F.
		EndIf
	EndIf

	If nOp == 1 // "Incluir todos os insumos previstos."
