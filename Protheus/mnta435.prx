					dbSetOrder( 4 ) // TQ_ORDEM + TQ_PLANO + TQ_TAREFA + TQ_ETAPA + TQ_SEQRELA
					If msSeek( cOrdemTJ + cPlanoTJ + (cAlsExecu)->TQ_TAREFA +;
						(cAlsExecu)->TQ_ETAPA )

						If Empty( (cTRBQ400)->TQ_CODFUNC )

