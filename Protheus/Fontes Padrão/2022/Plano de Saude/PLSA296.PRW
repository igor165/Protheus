Function PLSA296

PRIVATE bDelLine := { || PLSA296Del() }

AxCadastro("BDM",Fundesc()) //"Mensagem de Carteirinha"

Return

Function PLSA296Del()
LOCAL lRet     := .T.
LOCAL BG8Name  := RetSQLName("BDL")
LOCAL cSQL     := ""

cSQL := "SELECT * FROM "+BG8Name+" WHERE "
cSQL += BG8Name+".BDL_FILIAL = '"+xFilial("BDL")+"'"
cSQL += "BDL_CODMSG = '"+BDM->BDM_CODIGO+"' AND D_E_L_E_T_ <> '*'"

TCQUERY cSQL NEW ALIAS "TrbBDL"

If ! TrbBDL->(Eof())
	lRet := .F.
	SX2->(DbSetOrder(1))
	SX2->(DbSeek("BDL"))
	MsgInfo("Nao e possivel Excluir pois este registro esta sendo utilizado ["+AllTrim(SX2->(X2NOME()))+"]")
Endif
TrbBDL->(DbCloseArea())

Return(lRet)

