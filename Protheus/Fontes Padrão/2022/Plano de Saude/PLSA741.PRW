/*/
Function PLSA741BEN(cCodInt,cCodEmp)
       
LOCAL lRet    := .T.
LOCAL aRet    := {}

BG9->(DBSetOrder(1))
BG9->(DBSeek(xFilial("BG9")+cCodInt+cCodEmp))

If BG9->BG9_TIPO == "2"
   Eval( { || aVet := PLS260EMP(cCodInt,cCodEmp),.T. })
   lRet := aVet[1]
   If lRet
      M->BFV_CONEMP := aVet[2]
   Endif         
Else
   M->BFV_CONEMP := ""
Endif

DBSelectArea("BFV")

Return(lRet)

