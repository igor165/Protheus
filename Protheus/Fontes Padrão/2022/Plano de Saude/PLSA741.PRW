/*/
Function PLSA741Cd(cCodInt)
LOCAL nRet    := 0
LOCAL nOrdBFU := BFU->(IndexOrd())

BFU->(DbSetOrder(1))

BFU->(DbSeek(xFilial("BFU")+cCodInt+Replicate("9",Len(BFU->BFU_CODSEQS)),.T.))
BFU->(DbSkip(-1))

If BFU->(BFU_FILIAL+BFU_CODINT) <> xFilial("BFU")+cCodInt
   nRet := StrZero(1,3)
Else
   nRet := StrZero(Val(BFU->BFU_CODSEQ)+1,3)
Endif

BFU->(DbSetOrder(nOrdBFU))

Return(nRet)
