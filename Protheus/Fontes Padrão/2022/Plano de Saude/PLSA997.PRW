ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß
*/
Static Function TudoOk(cAlias,nReg,nOpc,cCbo)

Local lRet 	   := .T.
Local cCodCbo  := ""
Local lAchou   := .F.

If !Empty(M->B0X_CODCBO)
	cCodCbo := M->B0X_CODCBO
Else
	cCodCbo := cCbo 
EndIf

dbSelectArea("B0X")
B0X->( dbSetOrder(1) ) // B0X_FILIAL+B0X_CODCBO

If nOpc == 3 .Or. nOpc == 4 
	If B0X->(dbSeek( xFilial("B0X") + cCodCbo))
		MsgInfo(OemToAnsi("Código CBO já cadastrado na base de dados."))
		lRet:= .F.
	EndIf
ElseIf nOpc == 5
    dbSelectArea("BAQ")
    dbSetOrder(6) //BAQ_FILIAL + BAQ_CBOS
    lAchou := BAQ->(dbSeek( xFilial("BAQ") + cCodCbo))
    If lAchou
       MsgInfo(OemToAnsi("Código CBO não pode ser excluído pois já foi utilizado no cadastro de especialidades(BAQ)."))
       lRet := .F.
    EndIf   
EndIf	

Return lRet

