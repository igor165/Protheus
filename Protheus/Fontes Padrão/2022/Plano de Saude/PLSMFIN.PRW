Function PLSE2NUM(cPrefixo)
LOCAL nNum
LOCAL nOrdSE2 := SE2->(IndexOrd())
LOCAL nRecSE2 := SE2->(Recno())
       
SE2->(DbSetOrder(1))
SE2->(DbSeek(xFilial("SE2")+cPrefixo+replicate("9",len(SE2->E2_NUM)),.T.))
       
SE2->(DbSkip(-1))
        
If SE2->(E2_FILIAL+E2_PREFIXO) == xFilial("SE2")+cPrefixo
   nNum := Val(SE2->E2_NUM)+1
Else
   nNum := 1
Endif                      
       
SE2->(DbGoTo(nRecSE2))
SE2->(DbSetOrder(nOrdSE2))

Return(StrZero(nNum,len(SE2->E2_NUM)))


