LOCAL cChaveHAT  := ""
LOCAL NQTDBENEF  := 0
LOCAL lGerPL99B  := .F.
LOCAL dDataBC3   := nil
LOCAL lProRat	 := BG1->(Fieldpos("BG1_PRORAT")) > 0 // Verifica se campo existe

DEFAULT nP20	:= 2
DEFAULT aLog	:= {}
DEFAULT lGrav	:= .F.
