		DbSelectArea(cAlias)
		cTipo  := aOpcoes[nI,3]
		cTit   := aOpcoes[nI,1]

		nPos := Ascan(aVetTemp,{|x| x[1]+x[2] == cCodFor+cChave+cAlias})

		If     cTipo == "0" // Enchoice

		ElseIf cTipo == "1" // GetDados
