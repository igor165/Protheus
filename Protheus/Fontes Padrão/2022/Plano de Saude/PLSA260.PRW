Function PLSA260IDC(cCodDoc,cObrig,cTpRg,cChave)

LOCAL aVet := {}
LOCAL lFlag := .F.
LOCAL nColTR := PLRETPOS("BCP_TIPREG",oBrwBCP:aHeader,.F.)
