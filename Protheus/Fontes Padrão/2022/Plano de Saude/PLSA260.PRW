Endif
BCP->(DbSetOrder(nOrdBCP))
BCP->(DbGoto(nRecBCP))

aVet[nColTR] := cTpRg
aVet[nColCD] := cCodDoc
aVet[nColOb] := cObrig
aVet[nColDi] := STOD("")
aVet[nColDv] := STOD("")
aVet[nColDc] := Posicione("BD2",1,xFilial("BD2")+cCodDoc,"BD2_DESCRI")
