		EndIf
		
		If !Empty(M->BCA_MOTBLO)
			
			cMotBlo := PlMotBloANS(M->BCA_NIVBLQ,M->BCA_MOTBLO)
		ElseIf !Empty(M->BC3_MOTBLO)
			cMotBlo := PlMotBloANS(M->BC3_NIVBLQ,M->BC3_MOTBLO)
