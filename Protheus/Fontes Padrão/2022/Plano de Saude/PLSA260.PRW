		If B3K->(FieldPos("B3K_CRIMAE")) > 0
			oModel:SetValue( "B3KMASTER",  'B3K_CRIMAE'	, aRetDad[39] )
		EndIf	
		If !Empty(dDatRea)
			oModel:SetValue( "B3KMASTER",  'B3K_DATREA' , dDatRea )
		EndIf
		If !Empty(cSitAns)
			oModel:SetValue( "B3KMASTER",  'B3K_SITANS', cSitAns )
		EndIf
	EndIf
	
RestArea(aAreaBTS)
RestArea(aAreaBA1)
RestArea(aAreaBA3)

Return oModel

