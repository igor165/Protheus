		AtualizaBDQ(nPosUsr,TRBBTN->BTN_CODPRO,TRBBTN->BTN_VERPRO)
				
		TRBBTN->(DbSkip())
	EndDo
	TRBBTN->(DbCloseArea())
	RestArea(aArea)

	

