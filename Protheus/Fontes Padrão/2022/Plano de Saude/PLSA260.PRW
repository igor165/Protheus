	Endif

	cSQL := ChangeQuery(cSQL)
	dbUseArea(.T.,"TOPCONN",TCGENQRY(,,cSQL),"TrbDe",.F.,.T.)

	If !TrbDe->(EOF())
		If !EMPTY(TrbDe->BA3_GRPFAM)
