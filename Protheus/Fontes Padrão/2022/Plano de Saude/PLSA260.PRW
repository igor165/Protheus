	If BVF->(MsSeek(xFilial("BVF")+M->BA3_CODINT+M->BA3_CODEMP+M->BA3_MATRIC))
		While ! BVF->(Eof()) .And. BVF->(BVF_FILIAL+BVF_CODINT+BVF_CODEMP+BVF_MATRIC) == ;
			xFilial("BVF")+M->BA3_CODINT+M->BA3_CODEMP+M->BA3_MATRIC

			BR8->(DbSetOrder(1))
