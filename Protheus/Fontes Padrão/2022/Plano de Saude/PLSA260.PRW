Local nPosIni := ""
Local nPosFin := ""
Local nQTD    := ""
Local aArea	  := GetArea()
Local nUsado  := Len(oGetBVJ:aHeader)

nPosIni := Ascan(oGetBVJ:aHeader,{|x| Alltrim(x[2])=="BVJ_VIGDE"})
nPosFin := Ascan(oGetBVJ:aHeader,{|x| Alltrim(x[2])=="BVJ_VIGATE"})
