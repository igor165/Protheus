							OemtoAnsi(STR0216)/*" deste Usuario ? "*/,"BG3",'3',BA1->BA1_DATINC,'3',M->BCA_DATPED).and. PL260REAT(),oDlgBlo:End(),nOpca:=2),If(nOpca==1,oDlgBlo:End(),.F.) }
