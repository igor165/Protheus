			oBrwBDK:FieldPut("BDK_TIPREG", M->BA1_TIPREG, nPosUsr)
	   		If lBDKTabVld
				oBrwBDK:FieldPut("BDK_TABVLD", TRBBTN->BTN_TABVLD, nPosUsr)
			EndIf
   		Endif
