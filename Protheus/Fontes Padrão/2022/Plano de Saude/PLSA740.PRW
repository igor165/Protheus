/*/
Function PLSA740Mes(cAnoMes)
LOCAL cAno := Subs(cAnoMes,1,4)
LOCAL cMes := Subs(cAnoMes,5,2)
LOCAL nInd
LOCAL cAnoRes := cAno
LOCAL cMesRes := cMes
          
For nInd := 1 To 11
    If cMesRes == "12"
       cAnoRes := StrZero(Val(cAnoRes)+1,4)
       cMesRes := "01"
    Else
       cMesRes := StrZero(Val(cMesRes)+1,2)
    Endif      
Next

Return(cAnoRes+cMesRes)
