/*/
Function PLSA740Cd(cCodInt)
LOCAL nRet    := 0
LOCAL nOrdBH9 := BH9->(IndexOrd())

BH9->(DbSetOrder(1))

BH9->(DbSeek(xFilial("BH9")+cCodInt+Replicate("9",Len(BH9->BH9_CODSEQS)),.T.))
BH9->(DbSkip(-1))

If BH9->(BH9_FILIAL+BH9_CODINT) <> xFilial("BH9")+cCodInt
   nRet := StrZero(1,3)
Else
   nRet := StrZero(Val(BH9->BH9_CODSEQ)+1,3)
Endif

BH9->(DbSetOrder(nOrdBH9))

Return(nRet)
