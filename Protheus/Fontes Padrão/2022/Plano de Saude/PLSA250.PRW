LOCAL lRet := .F.
LOCAL cSQL := ""

#IFDEF TOP
        cSQL := "SELECT COUNT(*) QTDREG FROM "+RetSQLName("BI3")+" WHERE "
        cSQL += "BI3_GRUPO = '"+BE5->BE5_CODGRU+"' AND "
        cSQL += "BI3_FILIAL = '"+xFilial("BI3")+"' AND "
        cSQL += "D_E_L_E_T_ = ''"
        PLSQUERY(cSQL,"PL250EXC")
        
        lRet := If(PL250EXC->QTDREG >= 1,.F.,.T.)
        
        PL250Exc->(DbCloseArea())

        If lRet
           AxDeleta(cAlias,nReg,nOpc)
        Else
           Help("",1,"PLS250EXC")
        Endif   
#ELSE
        MsgInfo("Opcao nao disponivel para ambiente ISAM.")
#ENDIF                           



Return(lRet)

