LOCAL cNew

BAP->(DbSetOrder(1))
BAP->(DbSeek(xFilial("BAP")+cPropri+"99"))
BAP->(DbSkip(-1))

If BAP->BAP_PROPRI == cPropri
   cNew := cPropri+StrZero(Val(Subs(BAP->BAP_CODBLO,2,2))+1,2)
Else
   cNew := cPropri+"01"
Endif

Return(cNew)

