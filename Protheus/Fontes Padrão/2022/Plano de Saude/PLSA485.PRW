/*/
Function PLSA485PRO(cCodOpe)
LOCAL nRet    := 0
LOCAL nOrdBJX := BJX->(IndexOrd())
LOCAL nTam    := Len(BJX->BJX_CODIMG)
LOCAL nReg    := BJX->(Recno())

BJX->(DbSetOrder(1))
BJX->(DbSeek(xFilial("BJX")+cCodOpe+Replicate("9",nTam),.T.))
BJX->(DbSkip(-1))
                                                           
If BJX->(BJX_FILIAL+BJX_CODOPE) <> xFilial("BJX")+cCodOpe
   nRet := StrZero(1,nTam)
Else
   nRet := StrZero(Val(BJX->BJX_CODIMG)+1,nTam)
Endif

BJX->(DbSetOrder(nOrdBJX))
BJX	->(DbGoTo(nReg))


Return(nRet)
