STATIC FUNCTION VERIMP()

LOCAL NLIN := 80

IF ARETURN[5]==2
	
	NOPC     := 1 
	#IFNDEF WINDOWS
		CCOR := "B/BG" 
	#ENDIF 

	WHILE .T.
		SETPRC(0,0)
		DBCOMMITALL()
		
		@ NLIN,054 PSAY "*"
		
		#IFNDEF WINDOWS
			SET DEVICE TO SCREEN
			DRAWADVWINDOW(STR0007,10,25,14,56) //" FORMULARIO "
			SETCOLOR(CCOR)
			@ 12,27 SAY STR0008 //"FORMULARIO ESTA POSICIONADO?"
			NOPC := MENUH({STR0009,STR0010,STR0011},14,26,"B/W,W+/N,R/W","SNC","",1) //"SIM"###"NAO"###"CANCELA IMPRESSAO"
			SET DEVICE TO PRINT
		#ELSE
			IF     MSGYESNO(STR0012) //"FOMULARIO ESTA POSICIONADO ? "
				NOPC := 1
			ELSEIF MSGYESNO(STR0013) //"TENTA NOVAMENTE ? "
				NOPC := 2
			ELSE
				NOPC := 3
			ENDIF
		#ENDIF
		
		DO CASE
		CASE NOPC==1
			LCONTINUA:=.T.
			EXIT
		CASE NOPC==2
			LOOP
		CASE NOPC==3
			LCONTINUA:=.F.
			RETURN
		ENDCASE
	ENDDO 

ENDIF

RETURN
