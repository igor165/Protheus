STATIC FUNCTION GETSTRZAN(CTIPORO , CTIPOPI , CCAMPO , CVAZIO) 
// ======================================================================= \\
// --> CHAMADA: NÃO EXISTE NESTE FONTE - DESCONTINUADA
LOCAL AAREA		:= GETAREA()                            
LOCAL CQUERY	:= ""
LOCAL LSEGUE	:= .F.
LOCAL CRET		:= "N" 

