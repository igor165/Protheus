CQUERY += " WHERE  ZAN.D_E_L_E_T_='' "
CQUERY += "   AND  ZAN.ZAN_FILIAL = '"+XFILIAL("ZAN")+"' " 
CQUERY += "   AND  ZAN.ZAN_ALEMOA = 'N' AND ZAN.ZAN_BLOQSR = 'N' "
