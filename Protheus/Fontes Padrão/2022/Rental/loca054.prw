	NCOMPZAN  := N2COMP 
	NLARGZAN  := N2LARG
	NALTUZAN  := N2ALTU
 	NPESOZAN  := N2PESO
ELSEIF CVAZIO == "C" 					// --> CARREGADO
	// DIMENS. TOT
	NCOMPZAN  := N3COMP 
