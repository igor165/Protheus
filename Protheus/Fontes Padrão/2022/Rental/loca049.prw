LOCAL LRET := .T.

IF VALTYPE(ODLGFRO)=="O"  					// SE O OBJETO JÁ FOI CRIADO
	IF ODLGFRO:NAT > 1
		IF EMPTY(ODLGFRO:ACOLS[ODLGFRO:NAT][ASCAN(ODLGFRO:AHEADER,{|X|ALLTRIM(X[2])=="FPM_DTPROG" })])
			ODLGFRO:ACOLS[ODLGFRO:NAT][ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FPM_DTPROG" })]	:= CTOD("  /  /  ")
