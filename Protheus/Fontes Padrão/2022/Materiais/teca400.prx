For nCntFor := 1 To Len(aTravas)
	dbSelectArea(aTravas[nCntFor,1])
	dbGoto(aTravas[nCntFor,2])
	MsUnLock()
Next nCntFor

dbSelectArea(aArea[1])
dbSetOrder(aArea[2])
dbGoto(aArea[3])

Return(lRet)

