If ExistBlock( "AT400BUT" ) 
	aUsButtons := ExecBlock( "AT400BUT", .F., .F. )  
	AEval( aUsButtons, { |x| AAdd( aButtons, x ) } ) 	 	
EndIf 	

Return (EnchoiceBar(oDlg,bOK,bcancel,,aButtons))
 
