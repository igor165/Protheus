	AAH->( dbSetOrder( 4 ) ) 
	If AAH->( dbSeek( xFilial( "AAH" ) + AAP->AAP_GRPATE ) ) 
		Help( " ", 1, "AT150VLCO" ) // Este grupo esta sendo utilizado em um contrato de manutencao 
		lRetorno := .F. 
	EndIf 
	
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Valida a existencia de grupos na amarracao produto x ocorrencia  ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	
	If lRetorno 
		AA7->( dbSetOrder( 2 ) ) 
		If AA7->( dbSeek( xFilial( "AA7" ) + AAP->AAP_GRPATE ) ) 
			Help( " ", 1, "AT150VLPR" ) // Este grupo esta sendo utilizado em uma amarracao produto x ocorrencia 
			lRetorno := .F. 
		EndIf 
	EndIf 	
EndIf

Return( lRetorno )

