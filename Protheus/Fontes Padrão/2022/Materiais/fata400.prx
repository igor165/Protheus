dbSelectArea("ADB")
dbSetOrder(1)
If MsSeek(xFilial("ADB")+M->ADA_NUMCTR+aCols[n][nPItem])
	If ADB->ADB_QTDEMP >= ADB->ADB_QUANT .Or. IIf("ADB_QUANT"$cReadVar,&cReadVar,aCols[n][nPQuant])<ADB->ADB_QTDEMP
