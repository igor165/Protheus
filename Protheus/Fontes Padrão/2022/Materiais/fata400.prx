Function Ft400FldOk()

Local aArea    := GetArea()
Local lRetorno := .T.
Local nPItem   := aScan(aHeader,{|x| AllTrim(x[2])=="ADB_ITEM"})
Local nPQuant  := aScan(aHeader,{|x| AllTrim(x[2])=="ADB_QUANT"})
Local cReadVar := ReadVar()
//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
