Function At630Valid( )

LOCAL lRet      := .T.
LOCAL cConteudo := &( ReadVar() )

If cConteudo >= dDataBase
	
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Se nao existia data original, grava ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	
	If Empty( M->ABE_DTORIG ) .And. cConteudo <> ABE->ABE_DATA
		M->ABE_DTORIG := ABE->ABE_DATA  
	EndIf	
		
Else
	lRet := .F. 
EndIf                          
Return( lRet ) 

