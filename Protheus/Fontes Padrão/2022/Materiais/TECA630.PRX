Function At630Del(cAlias,nReg,nOpcx)
     
Local cSeek := ""
Local aArea := GetArea()

If ( Empty( ABE->ABE_NUMOS ) )
   
	Begin Transaction
		cSeek := ABE->ABE_CODFAB+ABE->ABE_LOJAFA+ABE->ABE_CODPRO+ABE->ABE_NUMSER
		AxDeleta(cAlias,nReg,nOpcx)
	
		dbSelectArea("ABE")
		dbSetOrder(1)
		If ( !MsSeek(xFilial("ABE")+cSeek) )
			dbSelectArea("AA3")
			dbSetOrder(4)
			If ( MsSeek(xFilial("AA3")+cSeek) )
				RecLock("AA3")
				AA3->AA3_PLANO := ""
				AA3->AA3_DTEXP := Ctod("")
				MsUnLock()
			EndIf
		EndIf
	
	End Transaction

Else

	Help(" ",1,"NODELETA")

EndIf
RestArea(aArea)
Return(Nil)
