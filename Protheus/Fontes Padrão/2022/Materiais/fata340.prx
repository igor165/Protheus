Function Ft340RmvAc( cString ) 
                 
LOCAL cNewString := ""
LOCAL cAddChar   := ""
LOCAL cChar      := ""

LOCAL nLoop      := 0 

For nLoop := 1 To Len( cString ) 

	cChar := SubStr( cString, nLoop, 1 ) 
	
	If Asc( cChar ) < 32 .Or. Asc( cChar ) > 127 
		cAddChar := "_"		
	Else 				
		cAddChar := cChar	
	EndIf 
                     
	cNewString += cAddChar 

Next nLoop 

Return( cNewString )