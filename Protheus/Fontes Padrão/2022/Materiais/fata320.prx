Function LimSemana( dData ) 

Local dDataIni                           
Local dDataFim 

Local nDia := Dow( dData )

dDataIni := dData - ( nDia - 1 ) 
dDataFim := dData + ( 7 - nDia ) 

Return( { dDataIni, dDataFim } ) 

