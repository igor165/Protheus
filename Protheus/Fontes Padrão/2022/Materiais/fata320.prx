Function Ft320AFixe(cAlias,aCposSim,aCposNao)

Local aFixe			:= {}
Local aArea			:= GetArea()
Local aAreaSX3  	:= SX3->(GetArea() )

Default aCposSim	:= {}
Default aCposNao	:= {}

/*
[n][1]=>DescriГЦo do campo
[n][2]=>Nome do campo
[n][3]=>Tipo
[n][4]=>Tamanho
[n][5]=>Decimal
[n][6]=>Picture
*/

DbSelectArea("SX3")
DbSetOrder(1)
DbSeek(cAlias)

While !SX3->(Eof()) .AND. SX3->X3_ARQUIVO == cAlias

	If	(Len(aCposNao)>0) .AND. (aScan(aCposNao,{|x|AllTrim(Upper(x)) == AllTrim(SX3->X3_CAMPO) })<>0)
		SX3->(DbSkip())
		Loop		
	Endif

	If (SX3->X3_BROWSE = 'S' .AND. SX3->X3_CONTEXT <> "V" .AND. SX3->X3_TIPO <> "M") .OR.;
		(If((Len(aCposSim)>0),(aScan(aCposSim,{|x|AllTrim(Upper(x)) == AllTrim(SX3->X3_CAMPO) })<>0),.F.))
	
		AAdd(aFixe,{	SX3->X3_TITULO	,;
						SX3->X3_CAMPO	,;
						SX3->X3_TIPO	,;
						SX3->X3_TAMANHO	,;
						SX3->X3_DECIMAL	,;
						SX3->X3_PICTURE	})
	EndIf

	SX3->(DbSkip())

End

RestArea(aAreaSX3)
RestArea(aArea)

Return aFixe
