Function Ft320TOK()
Local lRet 		:= .T.
Local lFT320TDK := ExistBlock("FT320TDK")

If lFT320TDK
   lRet := Execblock("FT320TDK",.F.,.F.)
	If ValType(lRet) <> "L"
		lRet := .T. // Reinicializa a valiavel
	EndIf	
EndIf

Return ( lRet )

