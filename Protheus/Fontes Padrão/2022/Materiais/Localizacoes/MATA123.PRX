Function A123Del(o)

Local lDeleted := .F.

If ValType(aCols[n,Len(aCols[n])]) == "L"  /// Verifico se posso Deletar
	lDeleted := aCols[n,Len(aCols[n])]      /// Se esta Deletado
End

If !MaFisFound("IT",n)
	MaFisAdd("","",0,0,0,"","",,0,0,0,0,0)
EndIf

MaFisDel(n,aCols[n][Len(aCols[n])])
Eval(bRefresh)

Return .T.

