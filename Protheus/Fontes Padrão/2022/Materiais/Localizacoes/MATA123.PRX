		If aArrayF4[oQual:nAt,1]
			aArrayF4[oQual:nAt,1] := !aArrayF4[oQual:nAt,1]
		Endif
	Endif

	If !aArrayF4[oQual:nAt,1]
		If SimpleLock("SC1")
			SC1->(MsRUnlock())
		Endif
	Endif
Endif

RestArea(aArea)

Return

