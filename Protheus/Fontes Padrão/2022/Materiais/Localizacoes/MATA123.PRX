dbSelectArea("SC1")
dbSetOrder(1)

If MsSeek(xFilial()+aArrayF4[oQual:nAt,2]+aArrayF4[oQual:nAt,3])

	cFlag := A123Flag()

	If SoftLock("SC1") .And. Empty(cFlag)
		aArrayF4[oQual:nAt,1] := !aArrayF4[oQual:nAt,1]
		SimpleLock("SC1")
	ElseIf !Empty(cFlag)
