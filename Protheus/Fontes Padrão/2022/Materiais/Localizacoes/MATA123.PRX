		aadd(aCabec,{"C7_DESPESA" ,0})
		aadd(aCabec,{"C7_SEGURO"  ,0})
		aadd(aCabec,{"C7_DESC1"   ,0})
		aadd(aCabec,{"C7_DESC2"   ,0})
		aadd(aCabec,{"C7_DESC3"   ,0})
		aadd(aCabec,{"C7_MSG"     ,""})
		aadd(aCabec,{"C7_REAJUST" ,""})
		For nX := 1 To 30
