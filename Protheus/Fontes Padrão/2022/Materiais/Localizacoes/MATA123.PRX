	ConOut(PadC("Teste de exclusao",80))
	ConOut("Inicio: "+Time())
	MATA123(1,aCabec,aItens,5)
	If !lMsErroAuto
		ConOut("Exclusao com sucesso! "+cDoc)
	Else
		ConOut("Erro na exclusao!")
	EndIf
	ConOut("Fim  : "+Time())
	ConOut(Repl("-",80))
EndIf
RESET ENVIRONMENT

Return(.T.)

