Local nReg 		:= AE5->(Recno())
Local lOk		:= .T.
Local cCodGrp 	:= AE5->AE5_GRPCOM
Local aArea := getArea()
                       
dbSelectArea("AE1")
dbSetOrder(1)
DbSeek(xFilial("AE1"))

While AE1->(!EOF())
	If AE1->AE1_GRPCOM == cCodGrp
		lOk := .F.
		MsgAlert(STR0007 + Alltrim(AE1->AE1_COMPOS) + STR0008) // "Este Grupo de composição está em uso pela Composição " ". Não será possível Alterar ou Excluir seu código para manter a integridade dos dados."
		Exit	
	Endif
	AE1->(DbSkip())
ENDDO

RestArea(aArea)

If lOk
	AxDeleta("AE5",nReg,5,cTudoOk)
Endif

Return
