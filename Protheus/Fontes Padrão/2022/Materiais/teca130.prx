oModel := MPFormModel():New('TECA130',/*bPreValidacao*/,/*bPosValidacao*/,bCommit,/*bCancel*/)
oModel:AddFields('AADMASTER',/*cOwner*/,oStruAAD,/*bPreValidacao*/,/*bPosValidacao*/,/*bCarga*/)
oModel:SetDescription(STR0001)


Return(oModel)

