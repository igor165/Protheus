Function At140Cmm(oMdl)

Local aBlock := {}
Local bNoTTS := {||.T.}

FWFormCommit( oMdl )

If ExistBlock("TEA1401")
	aBlock := ExecBlock("TEA1401")
	If ValType(aBlock)=="A"
		If Len(aBlock) > 3 .AND. ValType(aBlock[4])=="B"
			bNoTTS := aBlock[4]
		EndIf
	EndIf
EndIf

Return .T.

