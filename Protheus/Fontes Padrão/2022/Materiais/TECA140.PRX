Local oStruAAE := FWFormStruct(2,'AAE')

oView := FWFormView():New()
oView:SetModel(oModel)
oView:AddField('VIEW_AAE', oStruAAE,'AAEMASTER')
oView:CreateHorizontalBox('TELA',100)
oView:SetOwnerView('VIEW_AAE','TELA')

Return(oView)

