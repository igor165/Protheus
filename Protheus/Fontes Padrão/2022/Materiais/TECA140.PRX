Function At140Pos(oMdl)

Local aBlock := {}
Local bOk    := {||.T.}
Local bTTS   := {||.T.}

If ExistBlock("TEA1401")
	aBlock := ExecBlock("TEA1401")
	If ValType(aBlock)=="A"
		If Len(aBlock) > 1 .AND. ValType(aBlock[2])=="B"
			bOK := aBlock[2]
		EndIf
		
		If Len(aBlock) > 2 .AND. ValType(aBlock[3])=="B"
			bTTS := aBlock[3]
		EndIf
	EndIf
EndIf

Return .T.
