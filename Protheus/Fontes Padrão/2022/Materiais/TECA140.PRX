/*/
Function At140Pre(oMdl)

Local aBlock := {}
Local bPre   := {||.T.}

If ExistBlock("TEA1401")
	aBlock := ExecBlock("TEA1401")
	If ValType(aBlock)=="A"
	
		If Len(aBlock) > 0 .AND. ValType(aBlock[1])=="B"
			bPre := aBlock[1]
		EndIf
	EndIf
EndIf

Return .T.

