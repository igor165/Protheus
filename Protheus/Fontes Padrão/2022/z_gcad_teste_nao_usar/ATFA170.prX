	Endif
	If nDepAcum5 < SN3->N3_VORIG5
		nValDepr5 := Round((SN3->N3_VORIG5 * nTaxa) / 1200, X3Decimal("N3_VORIG5"))
		nDiferenca := SN3->N3_VORIG5 - (nValDepr5 + nDepAcum5)
		If nDiferenca <= 0
			nValDepr5 := nDiferenca
		Endif
		nDepAcum5 +=  nValDepr5
	Endif
