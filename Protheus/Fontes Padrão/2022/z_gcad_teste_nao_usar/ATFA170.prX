	dbSetOrder(1)
	Reclock("SN4",.T.)
	SN4->N4_FILIAL := xFilial("SN4")
	SN4->N4_CBASE  := SN3->N3_CBASE
	SN4->N4_ITEM   := SN3->N3_ITEM
	SN4->N4_TIPO   := SN3->N3_TIPO
	SN4->N4_OCORR  := IIf(nDifDepAc1 > 0, "10", "11")   // 10 aCeleracao Positiva
