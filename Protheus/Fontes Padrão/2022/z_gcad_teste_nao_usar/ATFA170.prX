nIndex := RetIndex("SN3")
dbSelectArea("SN3")
#IFNDEF TOP
	dbSetIndex(cIndex+OrdBagExt())
#ENDIF
dbSetOrder(nIndex+1)
DbGoTop()
If BOF() .and. EOF()
	RetIndex("SN3")
	dbSetOrder(nSavOrd1)
	Set Filter to
	FErase (cIndex+OrdBagExt())
	Return
EndIf

nQtdBens := 0	  // quantidade de Bens,mostrado no rodap?do browse
dbGotop()

nOpca := 0
aF170Marca("SN3",cMarca)
DEFINE MSDIALOG oDlg1 TITLE OemToAnsi(STR0011) From 9,0 To 28,80 OF oMainWnd //"Calculo de Depreciao Acelerada"
@1.9,.8 Say OemToAnsi(STR0012) //"Tot de Bens a Depre..."
