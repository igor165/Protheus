LOCAL lSavTTS
LOCAL nRec
LOCAL cAliasAnt := Alias()

lSavTTS := __TTSInUse
__TTSInUse := .F.

dbSelectArea(cAlias)
nRec := Recno()
DbGoTop()
While !Eof()
	 RecLock("SN3")
	 SN3->N3_OK := cMarca
