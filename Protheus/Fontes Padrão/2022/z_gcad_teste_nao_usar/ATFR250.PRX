			N1_DESCRIC, N1_AQUISIC, N1_PATRIM, N1_CHAPA,
			%Exp:cCpoCTB%
		FROM 
			%table:SN4% SN4, %table:SN3% SN3, %table:SN1% SN1, %Exp:cFrom%
		WHERE
			SN4.N4_FILIAL = %xfilial:SN4% AND
