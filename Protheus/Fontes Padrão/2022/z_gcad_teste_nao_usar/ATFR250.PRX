			SN3.N3_ITEM   = SN4.N4_ITEM AND 
			SN3.N3_TIPO   = SN4.N4_TIPO AND
			SN3.%notDel% AND
			SN1.N1_FILIAL = %xfilial:SN1%  AND
			SN1.N1_CBASE = SN4.N4_CBASE AND
			SN1.N1_ITEM  = SN4.N4_ITEM AND
			SN1.%notDel% AND
			%Exp:cWhere%
		ORDER BY %Exp:cChave%
	EndSql
