	Endif
	Li ++
	@Li,034 PSAY (cAliasSN3)->N3_VRCACM1 Picture cPicVRCACM1
	@Li,052 PSAY (cAliasSN3)->N3_VRDACM1 Picture cPicVRDACM1
	@Li,070 PSAY (cAliasSN3)->N3_VRCDA1  Picture cPicVRCDA1
	@Li,088 PSAY (cAliasSN3)->N3_AMPLIA1 Picture cPicAMPLIA1
	Li ++
	(cAliasSN3)->(dbSkip())
EndDo

If	li != 80
	@li,00 Say repl("*",132)
	@++li,00 PSAY OemToAnsi(STR0010)+Space(23) // "T O T A I S"
	If	lMoeda1
		@Prow(),Pcol() PSAY nTotal1 Picture cPicVORIG1
	Endif
	If	lMoeda2
