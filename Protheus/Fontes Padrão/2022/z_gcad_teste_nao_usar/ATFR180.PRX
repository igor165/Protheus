	If	lMoeda1
		@Prow(),Pcol() PSAY (cAliasSN3)->(N3_VORIG1+N3_AMPLIA1) 	Picture cPicVORIG1
	Endif
	If	lMoeda2
		 @Prow(),Pcol() PSAY (cAliasSN3)->(N3_VORIG2+N3_AMPLIA2) 	Picture cPicVORIG2
