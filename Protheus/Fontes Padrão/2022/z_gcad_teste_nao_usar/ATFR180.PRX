	@Li,052 PSAY nTotalDeprAc Picture cPicVRDACM1
	@Li,070 PSAY nTotalCrDpAc Picture cPicVRCDA1
	@Li,088 PSAY nTotalAmplia Picture cPicAMPLIA1
    
	@++li,00 PSAY Replicate("*",132)
