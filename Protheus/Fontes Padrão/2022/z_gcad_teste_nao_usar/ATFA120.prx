Static Function AcertaSXD(lAuto)
Local aArea	:= GetArea()

If !lAuto
	SXD->(DbSetOrder(1))
	If !SXD->(DbSeek("ATFA120"))
		DbSelectArea("SXD")
		RecLock("SXD",.T.)
		Replace XD_TIPO 		with "P"
		Replace XD_FUNCAO 	with "ATFA120"
		Replace XD_PERGUNT 	with "AFA120"
		Replace XD_PROPRI 	with "S"
		Replace XD_TITBRZ 	with "Refaz saldos"
		Replace XD_TITSPA 	with "Rehace Saldos"
		Replace XD_TITENG 	with "Redo Balances"
		Replace XD_DESCBRZ 	with "Este programa tem o objetivo reconstruir o arquivo de saldos"
		Replace XD_DESCSPA 	with "El objetivo de este programa es reconstruir el archivo de saldos"
		Replace XD_DESCENG 	with "This program has the purpose of rebuilding the accounting balances files"
		MsUnLock()
	EndIf
EndIf
RestArea(aArea)
Return

