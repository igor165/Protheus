Static Function VldATFR339()

Local aSaveArea	:= GetArea()
Local lRet := .T.

If Empty(MV_PAR01)
	lRet := .F.
	Help(" ",1,"NOEXEC",,STR0041,1,0)	 //"Exercício não preenchido "
ElseIf Empty(MV_PAR02)
	lRet := .F.
	Help(" ",1,"NOPERIODIN",,STR0042,1,0)	 //"Período inicial de tributação não preenchido"
ElseIf Empty(MV_PAR03)
	lRet := .F.
	Help(" ",1,"NOPERIODFN",,STR0043,1,0)	 //"Período final de tributação não preenchido "
EndIf

If Alltrim(Str(Year(MV_PAR02))) != Alltrim(MV_PAR01)
	lRet := .F.
	Help(" ",1,"PERIODINC",,STR0044,1,0)	 //"Informe o período de acordo com o exercício"
EndIf

RestArea(aSaveArea)
Return lRet


