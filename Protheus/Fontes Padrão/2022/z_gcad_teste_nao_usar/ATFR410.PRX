Local nRec
Local cBase
Local cItem
Local cTipo
Local cBemAnt   := " "
Local cIndice
Local CbTxt
Local cbCont
Local nMoeda   	:= 0
Local nVOrig	:= 0
Local cFilDe 	
Local cFilAte	
Local nFilValor := 0
Local cUserFilter := aReturn[7]
Local cAuxFilter := cUserFilter
Local cQuery := ""
Local aR410Campos := {}
Local i := 0
Local cValCampo := ""
Local cChave	:= ""
Local cFiltro	:= ""
Local TRB		:= ""
Local lQuery	:= .T. // Esta function trabalha Query
Local cFilOld	:= cFilAnt

// Verificação da classificação de Ativo se sofre depreciação
