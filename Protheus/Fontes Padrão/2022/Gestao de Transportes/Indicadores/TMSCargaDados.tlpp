        RecLock("A1P",.T.)
        For nAux := 1 To Len(aRet[nCount])
            FieldPut(FieldPos(aRet[nCount][nAux][1]), aRet[nCount][nAux][2])
        Next
        A1P->(MsUnlock())

        CargaA1Q( nCount , cIDAIP , cTitulo )
    Next  
      
   
EndIf 

Return lRet 

