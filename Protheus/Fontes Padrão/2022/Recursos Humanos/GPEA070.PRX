	aCposKey	:= { "RT_TIPPROV", "RT_VERBA", "RT_ITEM", "RT_CLVL"  }
Else
	aCposKey	:= { "RT_TIPPROV", "RT_VERBA" }
EndIf

IF !( GdCheckKey( aCposKey , 4 ) )
	lRet := .F.
EndIf
Return lRet

