/*/{Protheus.doc} GP070EXC
Exclusão de cálculo da provisão
@author Allyson Mesashi
@since 18/08/2021
/*/
Function GP070EXC( lRobo )

Local aArea 		:= GetArea()
Local aAreaSX1 		:= SX1->( GetArea() )
Local aButtons 		:= {}
Local aLog			:= {}
Local aLogTitle 	:= { Upper(OemToAnsi(STR0108)) } //'Log de ocorrências da exclusão provisão'
Local cPergSX1		:= "GPM070EXCL"
Local lTemSX1		:= .F.
Local nCont			:= 0
Local nOpca			:= 0

Local aAdvSize		:= {}
Local aInfoAdvSize	:= {}
Local aObjCoords	:= {}
Local aObjSize		:= {}

Private aLogFile  	:= {}
Private lAbortPrint := .F.

Default lRobo		:= .F.

SX1->( dbSetOrder(1) )
lTemSX1	:= SX1->( dbSeek(cPergSX1) )

If !lTemSX1
	fMsgExc()
Else
	If !lRobo
		aAdvSize		:= MsAdvSize(,.T.,280)
		aInfoAdvSize	:= { aAdvSize[1] , aAdvSize[2] , aAdvSize[3] , aAdvSize[4] , 5 , 5 }
		aAdd(aObjCoords , { 000 , 000 , .T. , .T. })
		aObjSize := MsObjSize(aInfoAdvSize , aObjCoords)

		DEFINE FONT oFont NAME "Arial" SIZE 7,-11 BOLD
		DEFINE MSDIALOG oDlg TITLE OemToAnsi(STR0109) From aAdvSize[7],0 TO aAdvSize[6],aAdvSize[5] PIXEL//"Exclusão do cálculo da provisão"
			@ aObjSize[1,1] , aObjSize[1,2] GROUP oGroup TO aObjSize[1,3], aObjSize[1,4] LABEL OemToAnsi("") OF oDlg PIXEL
			oGroup:oFont:= oFont
			@ aObjSize[1,1]+15	,aObjSize[1,2]+10 SAY   STR0110 Size  500,008 OF oDlg PIXEL FONT oFont//"Exclusão do cálculo de provisão de férias e 13º salário"
			@ aObjSize[1,1]+25	,aObjSize[1,2]+10 SAY   STR0111 Size  500,008 OF oDlg PIXEL FONT oFont//"Será processado de acordo com os parâmetros selecionados pelo usuário."

			bParam := {|| Pergunte(cPergSX1,.T.) }
			AAdd(aButtons, {OemToAnsi(STR0112), bParam 	, OemToAnsi(STR0112), OemToAnsi(STR0112) }  )//"Parâmetros"

			bSet15	:= {|o| nOpca := 1,If(GPA070Ok(),oDlg:End(),nOpca:=0)}
			bSet24	:= {|o| oDlg:End() }
		ACTIVATE MSDIALOG oDlg ON INIT EnchoiceBar(oDlg, bSet15 , bSet24 , Nil , aButtons)  CENTERED
	Else
		nOpca := 1
	EndIf
EndIf

If nOpca == 1
	If !lRobo
		ProcGpe({|lEnd| GP070Excl()},,,.T.)
	Else
		GP070Excl( .T. )
	EndIf
EndIf

If !Empty(aLogFile)
	aAdd(aLog, {})
	For nCont := 1 To Len(aLogFile)
		aAdd( aLog[1], aLogFile[nCont] )
	Next nCont
	fMakeLog( aLog, aLogTitle, "GPEA070", NIL, FunName(), STR0108 )//'Log de ocorrências da exclusão provisão'
EndIf

RestArea( aAreaSX1 )
RestArea( aArea )

Return

