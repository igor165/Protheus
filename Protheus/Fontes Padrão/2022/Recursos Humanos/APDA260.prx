Local cAlias
Local nF3

If Type("aHeader") <> "A" .and. Type("oGdRdb:aHeader") = "A"
   Private aCols 	:= aClone(oGdRdb:aCols)
   Private aHeader 	:= aClone(oGdRdb:aHeader)
