	mvRet:=Alltrim(ReadVar())			 // Iguala Nome da Variavel ao Nome variavel de Retorno
EndIF

dbSelectArea("SX5")
If dbSeek(cFilial+"00RO")
   cTitulo := Alltrim(Left(X5Descri(),35))
Endif
If dbSeek(cFilial+"RO")
	CursorWait()
		While !Eof() .And. SX5->X5_Tabela == "RO"
