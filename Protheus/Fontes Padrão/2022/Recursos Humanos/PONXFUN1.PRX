					 Grava o ontrole de exportacao para o TSA - Usuario           
					*/
					oObjRep:WSUpdRHExp( "SR8" )

				Endif
				DelClassIntf()

			EndIf

		Endif
		If ( lRegua )
			oProcess:IncRegua2(STR0003 + Str(++nReg) + STR0004 + Str(nTotReg) )// Registro : 'x' # De : 'y'
		EndIf

		(cAliasSR8)->(dbSkip())
	Enddo
	DelClassIntf()
Endif

(cAliasSR8)->(DbCloseArea())

Return

