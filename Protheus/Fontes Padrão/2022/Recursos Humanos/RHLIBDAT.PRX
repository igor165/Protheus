			cMes := StrZero( Val(Substr( cMesAno, 4, 2 )), 2)
			cAno2 := StrZero(Val(Substr( cDtMenor, 1, 2)), 2)
			cMes2 := StrZero( Val(Substr( cDtMenor, 4, 2 )), 2)		
