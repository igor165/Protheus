Function Last_Day( dDate )

Local nMonth
Local nYear

IF ( ValType( dDate ) == "C" )
	dDate := CToD( dDate , "DDMMYYYY" )	
EndIF

nMonth	:= ( Month( dDate ) + 1 )
nYear	:= Year( dDate )
IF ( nMonth > 12 )
	nMonth := 1
    ++nYear
EndIF

dDate := CToD( "01/" + Month2Str( nMonth ) + "/" + Year2Str( nYear ) , "DDMMYYYY" )
dDate -= 1

Return( Day( dDate ) )

