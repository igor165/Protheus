Function ConflictDate( dFirstChk , dLastChk , dFirstDate , dLastDate )

Begin Sequence

	IF (;
			lConflictDate := (;
								( dFirstChk <= dFirstDate );
								.and.;
								( dLastChk >= dLastDate );
							);
		)
		Break
	EndIF						

	IF (;
			lConflictDate := (;
								( dFirstChk >= dFirstDate );
								.and.;
								( dLastChk <= dLastDate );
							);
		)
		Break
	EndIF						

	IF (;
			lConflictDate := (;
			   					( dFirstChk >= dFirstDate );
			   					.and.;
			   					( dFirstChk <= dLastDate );
			   					.and.;
			   					( dLastChk >= dLastDate );
			   				);
		)
		Break
	EndIF				   				
	
	IF (;
			lConflictDate := (;
								( dFirstChk <= dFirstDate );
								.and.;
								( dLastChk >= dFirstDate );
								.and.;
								( dLastChk <= dLastDate );
							);
			   	)
		Break
	EndIF

End Sequence

Return( lConflictDate )

