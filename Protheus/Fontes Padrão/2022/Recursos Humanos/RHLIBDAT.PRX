Function fChkSitFol()
	Local lRet := .F.
	
	If Inclui
		If (IsInCallStack("GPEA010") .And. !IsBlind()) .And. !(Empty(M->RA_SITFOLH))
			lRet := .T.
		EndIf
	EndIf
Return lRet

