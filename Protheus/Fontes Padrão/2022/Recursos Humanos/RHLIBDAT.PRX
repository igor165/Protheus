		ElseIf nTam == 6
			cAno := StrZero(Val(Substr( cMesAno, 1, 4)), 4)
			cMes := StrZero( Val(Substr( cMesAno, 5, 2 )), 2)
		ElseIf nTam == 7
			cAno := StrZero(Val(Substr( cMesAno, 1, 4)), 4)
			cMes := StrZero( Val(Substr( cMesAno, 6, 2 )), 2)
		EndIf
	ElseIf (nTipo == 3 .OR. nTipo == 4 ) .AND. !Empty(cDtMenor)
