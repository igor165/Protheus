			cMes := StrZero(Val(Substr( cMesAno, 1, 2)), 2)
			cAno := StrZero( Val(Substr( cMesAno, 4, 4 )), 4)
		EndIf
	ElseIf nTipo == 2
		If nTam == 4
			cAno := StrZero(Val(Substr( cMesAno, 1, 2)), 2)
			cMes := StrZero( Val(Substr( cMesAno, 3, 2 )), 2)
		ElseIf nTam == 5
