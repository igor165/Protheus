Function SomaMesAno( uData )

Local nMes, nAno, cRet
Local lData := If( ValType(uData)=="D",.T.,.F. )

nMes := If( lData,Month( uData ),Val(Right( uData,2 )) )
nAno := If( lData,Year( uData ),Val(Left( uData,4 )) )

nMes++
If nMes == 13
	nMes := 1
	nAno++
EndIf

uData := StrZero(nAno,4) + StrZero(nMes,2)
cRet  := uData

Return( cRet )

