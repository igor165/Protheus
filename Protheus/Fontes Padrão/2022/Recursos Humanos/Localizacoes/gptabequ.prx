Static Function Tab001( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S001','INSTRUCCIONES','01','CODIGO','CODIGO','C',2,0,'@!','NAOVAZIO()','','003','2',} )
