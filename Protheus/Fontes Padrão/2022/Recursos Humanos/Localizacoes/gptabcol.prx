Static Function Tab055( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S091','Mensajes','01','Codigo','Código','C',2,0,'@!','','001','S','1','','',} )
aAdd( aRCBItens, { '','S091','Mensajes','02','Sec','Secuencia','C',1,0,'@!','','001','S','1','','',} )
aAdd( aRCBItens, { '','S091','Mensajes','03','Mensaje','Mensaje','C',250,0,'@!','','001','S','2','','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])
If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIf

If cCliVersao != cVersao
	fGravaTab( aRCBHeader, aRCBItens )
EndIf

Return( NIL )

