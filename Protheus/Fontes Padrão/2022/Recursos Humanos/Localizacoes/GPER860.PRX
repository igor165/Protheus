Static Function fFechaCuatr(nFechCua)
Local lRet 	  := .T.
Local cAnioCuat := ""
Local cCuatri	  := ""
Local cHelp	  := ""

cHelp 		:= OemToAnsi(STR0023)
cAnioCuat	:= alltrim(str(nFechCua))
If Len(cAnioCuat) = 5
cCuatri := substr(cAnioCuat, 5,1)
	If cCuatri $ "1/2/3" 
		lRet := .T.
	Else
		lRet := .F.
	EndIf
Else
lRet := .F.
EndIf

If lRet == .F.
Help( ' ' , 1 , "FECHCUATR" , , cHelp , 1 , 0 )
EndIf

Return( lRet ) 


                                                                        
