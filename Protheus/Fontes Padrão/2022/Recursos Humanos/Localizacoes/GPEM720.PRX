Local nX 		:= 0
Local nY 		:= 0
Local aAreaRCV 	:= RCV->( GetArea() )

Begin Transaction
	dbSelectArea("RCV")
	dbSetOrder(1)            
	//If dbSeek(xFilial("RCV")+M->RCV_MAT+M->RCV_RFC+M->RCV_ANO+M->RCV_MESINI+M->RCV_MESFIN)
	If  RCV->(dbSeek(xFilial("RCV")+M->RCV_MAT+M->RCV_RFC+M->RCV_ANO)) //THGXPI
