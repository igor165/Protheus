Static Function Tab037( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S043','Formulario Anual de Ganancias','01','CODIGO','CСdigo','C',2,0,'','','','001','','','',} )
aAdd( aRCBItens, { '','S043','Formulario Anual de Ganancias','02','DESCRICAO','Descricao','C',130,0,'','','','001','','','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])
If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIf

If cCliVersao != cVersao
	fGravaTab( aRCBHeader, aRCBItens )
EndIf

Return( NIL )

