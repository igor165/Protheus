Static Function Tab038( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S044','F1357- Liq Imp A Las Ganancias','01','CODIGO','Rubro','C',4,0,'','','','002','1','S','',} )
aAdd( aRCBItens, { '','S044','F1357- Liq Imp A Las Ganancias','02','DETALLE','Descripción','C',185,0,'','','','002','2','S','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])
If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIf

If cCliVersao != cVersao
	fGravaTab( aRCBHeader, aRCBItens )
EndIf

Return( NIL )