Static Function Tab001( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S001','RИgimen','01','CODREG','RИgimen','N',4,0,'9999','NAOVAZIO()','','001','1','','',} )
aAdd( aRCBItens, { '','S001','RИgimen','02','FCHDES','Fecha Desde','N',6,0,'999999','','','001','2','','',} )
aAdd( aRCBItens, { '','S001','RИgimen','03','FCHHAS','Fecha Hasta','N',6,0,'999999','','','001','2','','',} )
aAdd( aRCBItens, { '','S001','RИgimen','04','DESCRP','DescripciСn','C',30,0,'@!','','','001','2','','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])
If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIf

If cCliVersao != cVersao
	fGravaTab( aRCBHeader, aRCBItens )
EndIf

Return( NIL )

