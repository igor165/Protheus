Static Function fBuscaOficio(dData)

Local nCont  := 0
Local cTipoF := ""

aOficio := {}


aSort( aFuncoes,,, { |x,y|dtos(x[3]) + x[6] < dtos(y[3]) + y[6]})

For nCont := 1 to Len(aFuncoes)
	
	nPos := Ascan(aFuncoes, { |X| X[1] = dData})
	
	If aFuncoes[nCont][10] = "C"
		cTipoF := "Comunes"
	Elseif aFuncoes[nCont][10]	= "P"
		cTipoF := "Privilegiados"
	Elseif aFuncoes[nCont][10]	= "I"
		cTipoF := "Insalubres Docentes"
	Elseif aFuncoes[nCont][10]	= "D"
		cTipoF := "Docentes al frente de grado"
	Endif                  
	
	cTipoF := "Comunes"
	
	AADD(aOficio,{aFuncoes[nCont][7],cTipoF})
	
	EXIT
	
Next nCont

Return .T.

