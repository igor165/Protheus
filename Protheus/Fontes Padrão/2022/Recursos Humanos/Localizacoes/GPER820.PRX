Static Function fBuscaAVerbas(cVerbasProv)

Local ns 			:= 0
Local lQuery		:= .F.
Local cAcessaSRV	:= &( " { || " + ChkRH( "GPER820" , "SRV" , "2" ) + " } " )

#IFDEF TOP
	lQuery	:= .T.
	
	cSQL := " SELECT         "
	cSQL += "	RV_FILIAL,   "
	cSQL += "	RV_COD,      "
	cSql += "  	RV_TIPOCOD , "
	cSql += "      RV_CODFOL,    "
	cSql += "      RV_REMUNE  "
	cSQL += " FROM " +	RetSqlName("SRV")
	cSQL += " WHERE "
	cSQL += " RV_TIPOCOD  ='" + cTipoVerba + "'"
	cSQL += " AND "
	
	If TcSrvType() != "AS/400"
		cSQL += "  D_E_L_E_T_ <> '*' "
	Else
		cSQL += "  @DELETED@  <> '*' "
	Endif
	
	cSQL 		+= " ORDER BY RV_FILIAL, RV_TIPOCOD "
	cSQL     	:= ChangeQuery(cSQL)
	aStruSRV 	:= SRV->(dbStruct())
	SRV->( dbCloseArea() ) //Fecha o SRV para uso da Query
	dbUseArea(.T.,"TOPCONN",TcGenQry(,,cSQL),"SRV1",.F.,.T.)
	For nS := 1 To Len(aStruSRV)
		If ( aStruSRV[nS][2] <> "C" )
			TcSetField("SRV1",aStruSRV[nS][1],aStruSRV[nS][2],aStruSRV[nS][3],aStruSRV[nS][4])
		EndIf
	Next nS
	cQrySRV := "SRV1"
#ELSE
	
	cQrySRV := "SRV"
	DbSelectArea(cQrySRV)
	(cQrySRV)->(DbGotop())
	(cQrySRV)->(dbSeek(xFilial()+cTipoVerba))
	
#ENDIF

WHILE !EOF()   .And. (cQrySRV)->RV_FILIAL + (cQrySRV)->RV_TIPOCOD == xFilial() + cTipoVerba
	
	If !lQuery
		IF ((cQrySRV)->RV_FILIAL <> cFilDe) .AND. ((cQrySRV)->RV_TIPOCOD <> cTipoVerba )
			(cQrySRV)->(dbSkip())
			Loop
		ENDIF
	EndIF
	
	If !((cQrySRV)->RV_FILIAL $ fValidFil()) .Or. !Eval(cAcessaSRV)       
		(cQrySRV)->(dbSkip())
		Loop
	EndIf
	
    If cRoteiro== fGetCalcRot('1')
		cVerbasProv += IIF(!EMPTY( (cQrySRV)->RV_COD )   .AND.   (! (cQrySRV)->RV_CODFOL ==  "0072")   .AND.   ( (cQrySRV)->RV_REMUNE  == "S")   ,  (cQrySRV)->RV_COD +"/","")				
	Else
		cVerbasProv += IIF(!EMPTY( (cQrySRV)->RV_COD )   .AND.   (! (cQrySRV)->RV_CODFOL ==  "0072")   ,  (cQrySRV)->RV_COD +"/","")							    
    Endif
    
	(cQrySRV)->(dbSkip())
	
ENDDO

(cQrySRV)->( dbCloseArea() )

Return .T.

