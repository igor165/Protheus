		Next nCont
		                                                                  
		For nCont := 1 to Len(aRetAcm)
			//If aRetAcm[nCont,1] $ cVerbasProv
			If aRetAcm[nCont,1] $ cListaVerbas
			    If  aRetAcm[nCont,1] $ FGETCODFOL("0813") + "/" + FGETCODFOL("0221")
			
					nPos := Ascan(aRetAcmImp, { |X| X[1] = aRetAcm[nCont][4]})
					                   
					IF (nPos = 0)
						AADD(aRetAcmImp, {Substr(aRetAcm[nCont][4],1,6),aRetAcm[nCont][3]} )
