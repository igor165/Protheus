	cListaVerbas  := FGETCODFOL("0813") + "/" + FGETCODFOL("0221") + "/" + cVerba
	aRetAcm  	    := fBuscaACM(cListaVerbas,,STOD(cDataAcmDe),STOD(cDataAcmAte),,,,cRoteiro,.T.)
    aRetAcmImp      := {}
    aRetAcmDia      := {}
    aRetAcmZ      := {}
    
	If Len(aRetAcm) > 0

		cAno 	:= Substr(aRetAcm[1][4],1,4)
		aAdd(aAno,cAno)
		nQtdeAnos := 1
		
		For nCont := 1 To Len(aRetAcm)
			
			IF Val(cAno) <> Val(Substr(aRetAcm[nCont][4],1,4))
