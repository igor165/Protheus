							cAnoTab  := (Substr(aRetAcm[nCont][4],1,4))
							//valor modulo previsional      >> utiliza nas funcoes adjacentes
						 	nHabMinGar := If(FTABELA("S026",1,4, CtoD("01/"+cMesTab+"/"+cAnoTab)) <> Nil, FTABELA("S026",1,4, CtoD("01/"+cMesTab+"/"+cAnoTab )) , 0 )
							//jubilacion       >> utiliza nas funcoes adjacentes     
					 		nMaxEm := If(FTABELA("S001",1,10, CtoD("01/"+cMesTab+"/"+cAnoTab)) <> Nil, FTABELA("S001",1,8, CtoD("01/"+cMesTab+"/"+cAnoTab)) , 0 )
					 		nMaxEm	*=	nHabMinGar
						Endif
						If  (aRetAcmImp[nPos,2] + aRetAcm[nCont][3]) < nHabMinGar        
							aRetAcmImp[nPos,2]:= (aRetAcmImp[nPos,2] + aRetAcm[nCont][3])
						Else
							aRetAcmImp[nPos,2]:= nHabMinGar 
