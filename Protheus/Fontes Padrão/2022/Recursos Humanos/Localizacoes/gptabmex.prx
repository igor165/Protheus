Static Function Tab036( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S081','Intereses Préstamos','01','PERIODO','Periodo','C',6,0,'@!','','2','S','004','','',} )
aAdd( aRCBItens, { '','S081','Intereses Préstamos','02','PAGO','Pago','C',2,0,'@!','','2','S','004','','',} )
aAdd( aRCBItens, { '','S081','Intereses Préstamos','03','INTERES','Interés FA','N',10,6,'999.999999','','2','S','004','','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])
If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIf

If cCliVersao != cVersao
	fGravaTab( aRCBHeader, aRCBItens )
EndIf

Return( NIL )