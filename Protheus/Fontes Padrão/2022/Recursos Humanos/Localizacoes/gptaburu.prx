Static Function Tab001( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local lGrava := .T. 
Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S001','TICKET ALIMENTACAO VALOR FIXO','01','SALATE','A SUELDO BASE','N',12,2,'@E 9,999,999.99','POSITIVO()','','003','1',} )
