/*/{Protheus.doc} GpCrgRUS
Definition of the Tables
@author E.Moskovkina
@since 31/10/2017
@version 1.0
/*/
//-------------------------------------------------------------------

Function GpCrgRUS()

	Local aRCCHeader	:= {}
	Local aTabMigr	:= { 'S001','S002','S003','S004','S005','S006','S007','S008','S009','S010','S011','S012','S013','S014','S015', ;
						'S016','S017','S018','S019','S020','S021','S022','S023','S024','S025','S026','S027','S028','S029','S030', ;
						'S031','S032','S033','S034','S035','S036','S037','S038','S039','S040','S041','S042'}
	Local bExecFunc
	Local cNameFunc	:= ""
	Local lRet	  	:= .T.
	Local nX 		:= 0
	Local cFil  	:= Alltrim(Str(FWGETTAMFILIAL)) 
	
	/*/
	--------------------------------------------------------------
	RCC Header                                            
	--------------------------------------------------------------/*/
	
	aAdd(aRCCHeader, { 'Branch'			,'RCC_FILIAL',''		,cFil ,'0',''										,'ЂЂЂЂЂЂЂЂЂЂЂЂЂЂ ','C','SM0   '	,' ','                                                                                                                                '	,'','',' ','',' ','.F.' })
	aAdd(aRCCHeader, { 'Code'			,'RCC_CODIGO','@!'		,'4'  ,'0','EXISTCPO("RCB")'						,'ЂЂЂЂЂЂЂЂЂЂЂЂЂЂ ','C','      '	,'R','                                                                                                                                '	,'','','V','','' ,'.F.' })
	aAdd(aRCCHeader, { 'Table Branch'	,'RCC_FIL'	 ,''		,'8'  ,'0',''										,'ЂЂЂЂЂЂЂЂЂЂЂЂЂЂ ','C','XM0   '	,' ','                                                                                                                                '	,'','',' ','','' ,'.F.' })
	aAdd(aRCCHeader, { 'Month / Year'	,'RCC_CHAVE' ,'999999'	,'6'  ,'0','IIF(EMPTY(M->RCC_CHAVE),.T.,(ENTRE'+;
																		   '("01","12",Substr(M->RCC_CHAVE,1,2))'+;
																		   ' .AND. LEN(ALLTRIM(M->RCC_CHAVE))==6))'	,'ЂЂЂЂЂЂЂЂЂЂЂЂЂЂ ','C','      '	,' ','                                                                                                                                '	,'','',' ','','' ,'.F.' })
	aAdd(aRCCHeader, { 'Sequence'		,'RCC_SEQUEN','@!'		,'3'  ,'0',''										,'ЂЂЂЂЂЂЂЂЂЂЂЂЂЂ ','C','      '	,'R','                                                                                                                                '	,'','','V','','' ,'.F.' })
	aAdd(aRCCHeader, { 'Content'		,'RCC_CONTEU','@!'		,'250','0',''										,'ЂЂЂЂЂЂЂЂЂЂЂЂЂЂ ','C','      '	,' ','                                                                                                                                '	,'','',' ','','' ,'.F.' })
	aAdd(aRCCHeader, { 'Alias WT'		,'RCC_ALI_WT',''		,'3'  ,'0',''										,'ЂЂЂЂЂЂЂЂЂЂЂЂЂЂ ','C','RCC'	,'V',''																																  	,'','','V','','' ,'.F.' })
	aAdd(aRCCHeader, { 'Recno WT'		,'RCC_REC_WT',''		,'10' ,'0',''										,'ЂЂЂЂЂЂЂЂЂЂЂЂЂЂ ','N','RCC'	,'V',''																																	,'','','V','','' ,'.F.' })
	
	/*/
	--------------------------------------------------------------
	Validate the Structure of the Table RCC                                           
	--------------------------------------------------------------/*/
	lRet := fNewOldSx3(aRCCHeader, NIL, "RCC", NIL)
	
	If lRet
		For nX := 1 To 42
			cNameFunc := "{ || Tab" + SubStr(aTabMigr[nX],2) + "( aRCCHeader )" + " }"
			bExecFunc := &cNameFunc
			Eval(bExecFunc)
		Next nX
	EndIf

Return( NIL )

