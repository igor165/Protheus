lRet := fNewOldSx3(aRCAHeader, NIL, "RCA", NIL, .F.)

If lRet
		If MV_MODFOL == "2"
			GpexOper(aMnemo)
		EndIf
		If MV_MODFOL == "1"
			AMnem(aMnemo)
		EndIf
		CMnem(aMnemo)
		DMnem(aMnemo)
		LMnem(aMnemo)
		NMnem(aMnemo)
		If MV_MODFOL == "1"
			OMnem(aMnemo)
		EndIf
		If MV_MODFOL == "1"
			UMnem(aMnemo)
		EndIf

	nPosMnemo := GdFieldPos ( "RCA_MNEMON" , aRCAHeader )
	aSort( aMnemo ,,, { |x, y| Upper( AllTrim( x[nPosMnemo] )  ) < Upper( AllTrim( y[nPosMnemo] ) ) } )

EndIf

Return( NIL )

