Static Function GeraCaso(cFil,cRegp,cAnio,cMat,cCon,dFecAcc,cRec,cTipRie,cConsec,nPorIPP,cDefun,dFecter,cObs,cTip,aMovs,cNcerin)
//Guarda en RIT
//Guarda en RS2 el contenido de aMovs
dbSelectArea("RIT")

RecLock("RIT", .T.)
RIT->RIT_FILIAL := cFil
RIT->RIT_CODRPAT:= cRegp
RIT->RIT_ANIO   := cAnio
RIT->RIT_MAT    := cMat
RIT->RIT_ITEM   := cCon
RIT->RIT_REC    := cRec
RIT->RIT_FECACC := dFecAcc
RIT->RIT_TIPRIE := cTipRie
RIT->RIT_CONSEC := cConsec
If Alltrim(cDefun) == 'D' .and. Len(Alltrim(cNcerin)) == 0
	RIT->RIT_DIAS := 0
Else
	RIT->RIT_DIAS := dFecter-dFecAcc+1
EndIf
RIT->RIT_PORINC := nPorIPP
RIT->RIT_DEFUNC := cDefun
If cTip != '2' .and. cTip != '3'
	RIT->RIT_FECALT := dFecter
EndIf
RIT->RIT_OBSERV := cObs
MsUnlock()

dbSelectArea("RS2")
RecLock("RS2", .T.)
RS2_FILIAL := aMovs[1]
RS2_CODRPA := aMovs[2]
RS2_ANIO   := aMovs[3]
RS2_MAT    := aMovs[4]
RS2_ITEM   := aMovs[5]
RS2_NCERIN := aMovs[6]
RS2_DATAIN := aMovs[7]
RS2_DURACA := dFecter-dFecAcc+1
If cTip != '2' .and. cTip != '3'
	RS2_DATAFI := aMovs[9]
EndIf
RS2_CONTIN := aMovs[10]
RS2_PRORSC := aMovs[11]
RS2_TIPORS := aMovs[12]
RS2_RESINC := aMovs[13]
RS2_CONINC := aMovs[14]
MsUnlock()
Return

