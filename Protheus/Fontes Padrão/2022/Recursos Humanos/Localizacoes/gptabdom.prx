Static Function Tab018( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local lGrava := .T. 
Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','01','www','PAGINA WEB','C',30,0,'@!','','002','1','',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','02','Email','CORREO ELECTRONICO','C',30,0,'@!','','002','2','',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','03','Parque','PARQUE','C',30,0,'@!','','002','2','NAOVAZIO()',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','04','ValorRDS','VLR INSTALACIONES (RD$)','N',14,2,'@E 999,999,999.99','','002','2','NAOVAZIO()',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','05','Inspector','NOMBRE DEL INSPECTOR','C',30,0,'@!','','002','2','NAOVAZIO()',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','06','Fechatemp','FECHA INICIO TEMPORADA','D',8,0,'','','002','2','NAOVAZIO()',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','07','Duractemp','DURACION DE LA TEMPORADA','N',2,0,'99','','002','2','NAOVAZIO()',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','08','PorcExt','% EXTRANJEROS','N',3,0,'999','','002','2','NAOVAZIO()',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','09','Seccion','SECCION','C',30,0,'@!','','002','2','',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','10','Ocupacion','OCUPACION EMPRESA','C',60,0,'@!','','002','2','',} )
aAdd( aRCBItens, { '','S112','DATOS DEL EMPLEADOR','11','PolizaAcc','POLIZA CONTRA ACCIDENTE','C',10,0,'@!','','002','2','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])
If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIF

IF ( Val(cCliVersao) < Val(aRCBItens[ 1 , GdFieldPos( 'RCB_VERSAO' , aRCBHeader ) ]) ) .Or. (cCliVersao == '__New_Version__')
	If !Empty(aPaisTab) 
		If aScan( aPaisTab, { |x| x ==  aRCBItens[1, nPosCodigo] } ) == 0 
			lGrava := .F. 
		EndIf 
	EndIf 
	If lGrava 
		fGravaTab( aRCBHeader, aRCBItens )
	EndIf
EndIF

Return( NIL )