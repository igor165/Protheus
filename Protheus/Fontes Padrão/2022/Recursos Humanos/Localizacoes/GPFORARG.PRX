Function AddARG0110( aRc2Header , aRc3Header , aSYPHeader )

Local aRc2Cols    := {}
Local aRc3Cols    := {}
Local aItensSYP   := {}

Local cFilRC2     := xFilial('RC2')
Local cCliVersao  := ''

Local lFound      := .F.

Local nRc2Origem	:=  GdFieldPos( 'RC2_ORIGEM' , aRC2Header )
Local nRc2Codigo	:=  GdFieldPos( 'RC2_CODIGO' , aRC2Header )
Local nRc2Versao	:=  GdFieldPos( 'RC2_VERSAO' , aRC2Header )

aAdd(aRc2Cols, { '','S_','814SIC','APORTES INSSJP','SICOSS','','','007','1','2',} )

aAdd(aRc3Cols, { '','S_','814SIC','000001','','','','FINAL','SI_IGUAL','','NOCALCULA','','1','','',} )
aAdd(aRc3Cols, { '','S_','814SIC','000002','','','','NAUX_11','MULTIPLICA','','REM05','','APINSSJP','','',} )
aAdd(aRc3Cols, { '','S_','814SIC','000003','','','','NAUX_11','DIVIDE','','NAUX_11','','100','','',} )
aAdd(aRc3Cols, { '','S_','814SIC','000004','','','','IMPORTE','SUMA','','NAUX_11','','INCIMPINC','','',} )
aAdd(aRc3Cols, { '','S_','814SIC','000005','','','','FINAL','SI_IGUAL','','IMPORTE','','0','','',} )
aAdd(aRc3Cols, { '','S_','814SIC','000006','','','','UNIDADES','MUEVE','','','','APINSSJP','','',} )
aAdd(aRc3Cols, { '','S_','814SIC','000007','','','','','ETIQUETA','','FINAL','','','','',} )

dbSelectArea("RC2")
RC2->( dbSetOrder( RetOrdem('RC2','RC2_FILIAL+RC2_ORIGEM+RC2_CODIGO') ) )
lFound := RC2->( dbSeek( cFilRC2 + aRc2Cols[ 1 , nRc2Origem ] + aRc2Cols[ 1 , nRc2Codigo ] ) )
If ( lFound )
	cCliVersao := RC2->RC2_VERSAO
Else
	cCliVersao := '__New_Version__' 
EndIf
If ( Val(cCliVersao) < Val(aRc2Cols[ 1 , nRc2Versao ]) ) .Or. (cCliVersao == '__New_Version__') .Or. ( lFilRPO .and. Val(cCliVersao) == Val(aRc2Cols[ 1 , nRc2Versao ]) )
	fFormGrava( aRc2Cols , aRc3Cols , aRC2Header , aRC3Header , aSYPHeader, aItensSYP )
EndIf

