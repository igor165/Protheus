Static Function Tab001( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local lGrava := .T. 
Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S001','PROVINCIAS','01','CODIGO','CODIGO','C',2,0,'@!','NAOVAZIO()','','003','2',} )
aAdd( aRCBItens, { '','S001','PROVINCIAS','02','DESCRICAO','DESCRICAO','C',30,0,'@!','NAOVAZIO()','','003','2',} )
aAdd( aRCBItens, { '','S001','PROVINCIAS','03','SIGLA','SIGLA','C',3,0,'@!','NAOVAZIO()','','003','2',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])
If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIF

If cCliVersao != cVersao 
	If !Empty(aPaisTab) 
		If aScan( aPaisTab, { |x| x ==  aRCBItens[1, nPosCodigo] } ) == 0 
			lGrava := .F. 
		EndIf 
	EndIf 
	If lGrava 
		fGravaTab( aRCBHeader, aRCBItens )
	EndIf
EndIF

Return( NIL )

