Function Rd4ItemInit()

Local cLastRd4Item := ""
Local cNumRd4Item
Local cKeyAux
Local cRdkCodigo
Local nPosItem	:= GdFieldPos( "RD4_ITEM" 	, aHeader )
Local nDeleted 	:= GdFieldPos( "GDDELETED" 	, aHeader )
Local c			:= 1

IF IsMemVar( "RDK_CODIGO" )
	cRdkCodigo := GetMemVar( "RDK_CODIGO" )
Else
	cRdkCodigo := RDK->RDK_CODIGO
EndIF
cKeyAux		:= ( xFilial( "RD4" , xFilial( "RDK" ) ) + cRdkCodigo )

/*
* Essa alteraГЦo se deve ao fato de a numeraГЦo automАtica estar falhando ao utilizar 
* SIGAAPD -> AtualizaГУes -> Cadastros -> VisУes  
*/		
If Empty(cRd4ItemInit) //IsInCallStack( "APDA010ALT" , "__EXECUTE/RUNAPP/SIGAAPD/" )
	cLastRd4Item	:= Replicate( "0" , GetSx3Cache( "RD4_ITEM" , "X3_TAMANHO" ) )
Else
	//cLastRd4Item	:= Rd4ItemGet()
	For c := Len(aCols) To 1 Step -1		
		If !Empty(aCols[c, nPosItem]) .AND. !aCols[c, nDeleted] 
			cLastRd4Item := aCols[c, nPosItem]
			Exit
		EndIf
	Next
EndIF

IF ( StrTran( cLastRd4Item , "0" , "" ) == "" )
	cLastRd4Item := Soma1( cLastRd4Item )
EndIF

IF IsInGetDados( { "RD4_ITEM" } )
	cNumRd4Item := GdNumItem( "RD4_ITEM" , cLastRd4Item )
EndIF

DEFAULT cNumRd4Item := cLastRd4Item
cNumRd4Item		:= GetNewCodigo("RD4","RD4_ITEM",GetSx2Unico("RD4"),{ || cNumRd4Item := Soma1( cNumRd4Item ) },.F.,.F.,cKeyAux,NIL,cNumRd4Item)

Rd4ItemSet( cNumRd4Item )

Return( cNumRd4Item )

