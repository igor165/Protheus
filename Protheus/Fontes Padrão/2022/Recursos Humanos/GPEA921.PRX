Static Function ModelDef()
	Local oModel		:= Nil
	Local oStruRFR 	:= FWFormStruct( 1, "RFR")

	oModel     := MPFormModel():New('GPEA921')
	oModel:AddFields( 'RFRMASTER',, oStruRFR )

	oModel:SetDescription( OemToAnsi(STR0001) ) //"Agente de IntegraГЦo EstАgio"
	oModel:GetModel( 'RFRMASTER' ):SetDescription( OemToAnsi(STR0001) ) //"Agente de IntegraГЦo EstАgio"

Return( oModel )

