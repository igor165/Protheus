Function GPEA921()
	Local oMBrowse
	Local cFiltraRh
	
	oMBrowse := FWMBrowse():New()
	oMBrowse:SetAlias("RFR")
	oMBrowse:SetDescription(OemToAnsi(STR0001)) //"Agente de IntegraГЦo EstАgio"

	//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
