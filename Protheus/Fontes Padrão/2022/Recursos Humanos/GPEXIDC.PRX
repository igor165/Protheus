		cCliVersao := RCN->( RCN_VERSAO )
	Else
		cCliVersao := ""
	EndIf
	If cVersao <> cCliVersao
		Fp_GravaIDCal( aCodFol, cVersao )
	EndIf

	/*/
	ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	³ Transferir as verbas para os identificadores     			    ³
	ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ/*/
	aLog[1] := {}
	aLog[2] := {}
	dbSelectArea("SRV")
	DbSetOrder(2)
	DbSeek(cFil + "0001",.T.)
	If !Eof()
		While !Eof() .And. SRV->RV_FILIAL == cFil .And. Val(SRV->RV_CODFOL) > 0 .And. Val(SRV->RV_CODFOL) <= Len (aCodfol)
			nElem := Val(SRV->RV_CODFOL)
			aCodfol[nElem, 1] := SRV->RV_COD
			DbSkip(1)
		EndDo

		/*/
		ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		³ Verificar se as verbas obrigatorias estao OK     			    ³
		ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ/*/
		nElem := Len(aCodFol)
		For nY = 1 to nElem
			If ( nPosPais := aScan( aCodFol[ nY, 4 ], cPaisLoc ) ) == 0
				nPosPais := aScan( aCodFol[ nY, 4 ], "ALL" )
			EndIf
			If nPosPais > 0
				If aCodFol[nY, 1] = Space(3) .And. Substr(aCodFol[nY, 4, nPosPais],4,1) == "*"
					//Ids do eSocial - Brasil
					If cPaisLoc == "BRA" .And. !IsBlind() .And. StrZero(nY, 4) $ "0123/0124/0625/1420/1432/1433/1434/1435/1436/1437/1438/1439/1440/1441/1442/1443/1444/1445/1446/1447/1449/1450/1451"
						If lFirst
							lFirst := .F.
							aAdd( aLog[2], "Conforme alertado anteriormente, os identificadores de cálculo abaixo tornaram-se obrigatórios a partir do dia 16/05/2018:" )
						EndIf
						aAdd( aLog[2], StrZero(nY, 4) + " - " + aCodFol[ nY, 2] )
					ElseIf cPaisLoc == "BRA" .And. StrZero(nY, 4) $ "1562"
						aAdd( a1562, StrZero(nY, 4) + " - " + aCodFol[ nY, 2] )
					ElseIf cPaisLoc == "BRA" .And. StrZero(nY, 4) $ "1628/1629/1630/1631/1632/1633/1634/1635/1636/1637"
						aAdd( a1628, StrZero(nY, 4) + " - " + aCodFol[ nY, 2] )
					ElseIf cPaisLoc == "BRA" .And. StrZero(nY, 4) $ "1639/1640/1641/1642/1643/1644/1645/1646/1647/1648"
						aAdd( a1639, StrZero(nY, 4) + " - " + aCodFol[ nY, 2] )
					ElseIf cPaisLoc == "BRA" .And. nY = 1731
						aAdd( a1731, StrZero(nY, 4) + " - " + aCodFol[ nY, 2])
					Else
						aAdd( aLog[1], StrZero(nY, 4) + " - " + aCodFol[ nY, 2] )
					EndIf
				
				// Retirar após a migração do MP
				ELSEIF lGestPubl .AND. cPaisLoc == "BRA" .AND. aCodFol[nY, 1] = Space(3) .And. StrZero(nY, 4) $ "1662/1663/1664/1665"
					aAdd( a1662, StrZero(nY, 4) + " - " + aCodFol[ nY, 2] )
				EndIf

				If cPaisLoc == "BRA" .And. nY = 1882 .And. Empty(aCodFol[nY,1])	.And. !IsBlind()		
					If Date() <= cToD("31/01/2022")  //Até a data apresentar mensagem
						fMsg1882(StrZero(nY, 4) + " - " + aCodFol[ nY, 2])

					Else //Gera Log
						aAdd( a1882, StrZero(nY, 4) + " - " + aCodFol[ nY, 2] )
					EndIf
				EndIf
			EndIf
		Next
	Else
		dbSelectArea("SRV")
		dbSetOrder( 1 )
		RestArea( aArea )
		lRet := .F.
		If lMsg .And. !IsBlind()
			aAdd( aLog[1], STR1001 )             //TABELA DE VERBAS VAZIA
			MsAguarde( { || fMakeLog( aLog , { } , "GPEXIDE" , NIL , "IDCALLOG" , STR0998 ) } ,  STR0998 ) //"Log de Ocorrencias nos Identificadores de Calculo"
			aLog[1] := {}
		EndIf
		Break
	EndIf

End Sequence

If !Empty(a1562)
	aAdd( aLog[1], a1562[1])
	aAdd( aLog[1], "Para mais informações consulte http://tdn.totvs.com/pages/viewpage.action?pageId=394210801")
	aAdd( aLog[1], "")
EndIf

If !Empty(a1628)
	For nCont := 1 To Len(a1628)
		aAdd( aLog[1], a1628[nCont])
	Next nCont
	aAdd( aLog[1], "Para mais informações consulte http://tdn.totvs.com/pages/viewpage.action?pageId=403745443")
	aAdd( aLog[1], "")
EndIf

If !Empty(a1639)
	For nCont := 1 To Len(a1639)
		aAdd( aLog[1], a1639[nCont])
	Next nCont
	aAdd( aLog[1], "Para mais informações consulte http://tdn.totvs.com/pages/viewpage.action?pageId=425435514")
	aAdd( aLog[1], "")
EndIf

If !Empty(a1662)
	For nCont := 1 To Len(a1662)
		aAdd( aLog[1], a1662[nCont])
	Next nCont
	aAdd( aLog[1], "")
EndIf

If !Empty(a1731)
	aAdd( aLog[1], a1731[1])
	aAdd( aLog[1], "Para mais informações consulte https://tdn.totvs.com/pages/viewpage.action?pageId=528924226")
	aAdd( aLog[1], "")
EndIf

//Validação deve ser excluída após 31/12/2021, devido entrada em vigor do eSocial, quando as verbas passarão a ser obrigatórias//
If !Empty(a1882)
	aAdd( aLog[1], a1882[1])
	aAdd( aLog[1], "Para mais informações consulte https://tdn.totvs.com/x/rJvXJg")
	aAdd( aLog[1], "")
EndIf

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Faz tratamento de LOG                                                 ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
If !Empty ( aLog[1] ) .Or. !Empty ( aLog[2] )
	aLogErr := If(Empty(aLog[1]),aLog[2],aLog[1])
 	If lMsg .And. !IsBlind()
		If !Empty ( aLog[2] )
			aAdd( aLog[2], "")
			aAdd( aLog[2], "Para mais informações consulte http://tdn.totvs.com/pages/viewpage.action?pageId=353275183")
		EndIf
		/*/
		ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		³Mostrar o Log                                                          ³
		ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ*/
		MsAguarde( { || fMakeLog( aLog , { STR0999, STR1004 } , "GPEXIDE" , NIL , "IDCALLOG" , STR0998 ) } ,  STR0998 ) //"Log de Ocorrencias nos Identificadores de Calculo"##"Identificadores de cálculo abaixo tornaram-se obrigatórios a partir do dia 16/05/2018"
	Else

		AddLogExecRot(STR0999)//"Log de Ocorrencias nos Identificadores de Calculo"
		AddLogExecRot("")
		/*/
		ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		³Adicionar Log da execucao de roteiros                                  ³
		ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ*/
		For nY := 1 To Len( aLog[1] )
			AddLogExecRot( aLog[1, nY] )		// adiciona no Log de Erros
		Next

	EndIf
	lRet := .F.
	If lApaga
		aCodFol := {}
	EndIf

ElseIf Empty( __aCodFol )

	__aCodFol := aClone( aCodFol )
	__cFilial := cFil

EndIf

dbSelectArea("SRV")
