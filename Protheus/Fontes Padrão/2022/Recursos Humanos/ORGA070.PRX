Static Function Orgm10Hist()

	Local nRecno  := PosIdent("1")
	Local aAreaRBT:= {}
		
	If nRecno > 0

		aAreaRBT := RBT->( GetArea() )

		dbSelectArea("RBT")	
		dbSetOrder(1)		
		RBT->( dbSeek( xFilial("RBT")+SQB->QB_DEPTO,.F.) )	
	
		ORGM10Oper("RBX",nRecno)     
	Else
		Aviso("",OemToAnsi(STR0015),{"OK"})
	Endif
	
	If Len( aAreaRBT ) > 0
		RestArea( aAreaRBT )
	EndIf
	
Return
          
