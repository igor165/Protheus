	Local nRecno := PosIdent("1")
	Local cRD4Chave:= RTRIM(RD4->RD4_CHAVE)

	If nRecno > 0
		AxAlM010()
		
		While Len(cRD4Chave) > 0
			While (nPos := Ascan(aDados, { |aBusca| RTRIM(aBusca[10]) == cRD4Chave} ) ) > 0
				ADel(aDados, nPos)
				ASize(aDados, Len(aDados)-1)
