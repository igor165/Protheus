Function fValidAgrup()

Local lRet	:= .T. 

	If M->RC0_AGRUPA == "3"
		MsgAlert( OemToAnsi(STR0072), OemToAnsi(STR0060) ) 	//"Opção não disponível. Favor selecionar a opção por Centro de Custo" ##"Atenção"
		lRet := .F.  
	EndIf
	
	//Verifica se é Imposto de Renda
	If ((aLbxT[3,1]),oTpRetIR:Enable(), oTpRetIR:Disable())
	
	If !Empty(aGerBen)
		If M->RC0_AGRUPA == "4" .And. nTitSelect == 4 
			oGerBen:enable()
		Else
			oGerBen:disable()
		EndIf
	EndIf
				
Return lRet	


