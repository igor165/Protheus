		   lRet:= .F.
		   break
		Endif 
		If !Pn330VRESERVI(Pn330ValidCpo("M->PW_RESERVI")) .OR. !Pn330VOUTRDOC(Pn330ValidCpo("M->PW_OUTRDOC")) .OR. !Pn330VHABILIT(Pn330ValidCpo("M->PW_HABILIT")) .OR.;
