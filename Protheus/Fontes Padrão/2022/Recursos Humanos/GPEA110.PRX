Function gp110Retorno()
Local cCpoRet	:= ""
Local cNomeCpo	:= readvar()

If "RK_PERINI" $ cNomeCpo
	cCpoRet := "RCH->RCH_PER"
ElseIf "RK_NUMPAGO" $ cNomeCpo
	cCpoRet := "RCH->RCH_NUMPAG"
ElseIf "RK_DTVENC" $ cNomeCpo
	cCpoRet := "RCH->RCH_DTINI"
EndIf

cCpoRet := If (Empty(&(cCpoRet)), "",  &(cCpoRet))

Return(cCpoRet)

