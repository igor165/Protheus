Function fChkSR0(cFilFunc,cMatFunc,cTipoVale,lAgrup)
Local aArea		:= GetArea()
Local lRet		:= .T.

DEFAULT cFilFunc  := xFilial("SR0")
DEFAULT cMatFunc  := SRA->RA_MAT
DEFAULT cTipoVale := If(cTipoRot == "8","0",(If(cTipoRot == "D","1","2"))) //0-Vale Transporte / 1-Vale Refei็ใo / 2-Vale Alimenta็ใo
