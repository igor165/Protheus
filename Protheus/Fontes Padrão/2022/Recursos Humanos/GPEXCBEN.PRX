/*/{Protheus.doc} fCarrAdias
Carrega os dias do m๊s validando em quais dias devem ser pagos os benefํcios
@author cicero.pereira
@since 30/09/2017
@param dInicio, date, Inicio do perํodo
@param dFim, date, Fim do perํodo
@param cPeriod, caracter, C๓digo do perํodo que serแ carregado
/*/
Static Function fCarrAdias(dInicio, dFim, cPeriod)

	Local nI, nJ		:= 0
	Local lPropAdm		:= .F. //APERGUNTE[1,3] == 1	// Se calcula proporcional a admissใo
	Local lDesAfast		:= .F. //APERGUNTE[2,3] <> 2	// Se desconta f้rias/astamentos
	Local lDesFerProg	:= .F. //APERGUNTE[3,3] == 1	// Se calcula proporcional a f้rias programadas
	Local lDesFaltas	:= .F. //APERGUNTE[4,3] == 1	// Se desconta faltas
	Local cTipoAfas		:= ""
	Local aAfast		:= {}
	Local aPerAnt		:= {}
	//Local nAfaFer	:= If(cTipoRot == "I", 0, If(cTipoRot == '8', aPergunte[3,3], aPergunte[2,3]))
	Static _aDias		:= {}

	Default cPeriod := ""
	DEFAULT __lMemCalc	:= cPaisLoc == "BRA" .And. fMemCalc() // Mem๓ria de Cแlculo

	If cTipoRot == "8"
		lPropAdm	:= APERGUNTE[2,3] == 1	// Se calcula proporcional a admissใo
		lDesAfast	:= APERGUNTE[3,3] <> 2	// Se desconta f้rias/astamentos
		lDesFerProg	:= APERGUNTE[4,3] == 1	// Se calcula proporcional a f้rias programadas
		lDesFaltas	:= APERGUNTE[5,3] == 1	// Se desconta faltas
	ElseIf cTipoRot $ "D*E"
		lPropAdm	:= APERGUNTE[1,3] == 1	// Se calcula proporcional a admissใo
		lDesAfast	:= APERGUNTE[2,3] <> 2	// Se desconta f้rias/astamentos
		lDesFerProg	:= APERGUNTE[3,3] == 1	// Se calcula proporcional a f้rias programadas
		lDesFaltas	:= APERGUNTE[4,3] == 1	// Se desconta faltas
	EndIf

	If __lMemCalc
		fAddMemLog("Proporcional a f้rias : " + If(lDesFerProg,"Sim","Nใo") ,1,1)
		fAddMemLog("Proporcional a admissใo : " + If(lPropAdm,"Sim","Nใo") ,1,1)
		fAddMemLog("Desconta faltas  : " + If(lDesFaltas,"Sim","Nใo") ,1,1)
		fAddMemLog("Desconta afastamentos  : " + If(lDesAfast,"Sim","Nใo") ,1,1)
	EndIf

	If !Empty(cPeriod) // Se estiver preenchido estแ recalculando o m๊s anterior
		fCarPeriodo( cPeriod , cRot, @aPerAnt, , , ! IsBlind(), )
		fCarDiasMes(@_aDias, lPropAdm, aPerAnt)
	Else
		fCarDiasMes(@_aDias, lPropAdm)
	EndIf

	If lDesAfast
		If cTipoRot == "8"
			If aPergunte[2,3] > 1
				cTipoAfas := If(aPergunte[2,3] == 3, '4','123')
			EndIf
		Else
			If aPergunte[2,3] > 1
				cTipoAfas := If(aPergunte[3,3] == 3, '4','123')
			EndIf
		EndIf

		fRetAfas(dInicio, dFim, , , ,cTipoAfas , @aAfast)
		For nI := 1 To Len(aAfast)
			For nJ := 1 To Len(_aDias)
				If _aDias[nJ][1] >= aAfast[nI][3] .And. (_aDias[nJ][1] <= aAfast[nI][4] .Or. Empty(aAfast[nI][4]))
					_aDias[nJ][4] := .F.
				EndIf
			Next
		Next
	EndIf

	If lDesFerProg
		fVlsFerPrg(dInicio, dFim, @_aDias, .T.)
	EndIf

	For nI := 1 To Len(_aDias)
		Aadd(_aDias[nI], 0)
	Next

Return

/*/{Protheus.doc} AtuSM7Aut
Grava Registro na SM7
@author cicero.pereira
@since 09/10/2017
@version 12.1.17
@param cTpVale, characters, Tipo do vale: 1 Vale Refei็ใo; 2 Vale Alimenta็ใo
@param cCodBen, characters, C๓digo do benefํcio
/*/
Static Function AtuSM7Aut(cTpVale, cCodBen)

	Local aArea	:= GetArea()

	dbSelectArea("SM7")

	RecLock("SM7", .T.)
		SM7->M7_FILIAL := xFilial("SM7", SRA->RA_FILIAL)
		SM7->M7_MAT := SRA->RA_MAT
		SM7->M7_TPCALC := "2"	// Cแlculo automแtico
		SM7->M7_TPVALE := cTpVale
		SM7->M7_CODIGO := cCodBen
		SM7->M7_COMPL := "2"
	SM7->(MSUnlock())

	RestArea(aArea)

Return

/*/{Protheus.doc} fVlDemMs
Verifica demissao mes seguinte para pagamento de beneficios
@author Esther de Viveiro
@since 03/10/2017
@version P12.1.14
/*/
Function fVlDemMs()
	Local aArea		:= GetArea()
	Local lGpem040 	:= FunName() == "GPEM040" .or. FunName() == "GPEM040B"

	If !lGpem040 //Se esta sendo calculado pela rescisใo, a data de demissใo jแ esta preenchida, mas a SRG ainda nใo foi gravada.
		dDataDem	:= StoD("//")

		//verifica se ha calculo de rescisao para o periodo de processamento do beneficio
		SRG->( DbSetOrder(1) )	// Ordem 1 - RetOrdem( "SRG", "RG_FILIAL+RG_MAT+DTOS(RG_DTGERAR)")
		If SRG->(DbSeek( SRA->RA_FILIAL + SRA->RA_MAT))
			While SRG->(!Eof()) .and. SRG->(RG_FILIAL+RG_MAT) == SRA->(RA_FILIAL + RA_MAT)
				If SRG->RG_EFETIVA == 'S' .AND. AnoMes(SRG->RG_DATADEM) <= cPeriodo
					dDataDem := (SRG->RG_DATADEM)
				EndIf
				SRG->(dbSkip())
			EndDo
		EndIf

		RestArea(aArea)
	EndIf
Return Nil

