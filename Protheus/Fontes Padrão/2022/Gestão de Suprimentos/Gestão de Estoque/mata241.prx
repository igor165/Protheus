Local bSeekFor	  := {|| D3_ESTORNO <> "S" .And. dA241Data == SD3->D3_EMISSAO .And. (D3_TM == cTm .Or. (D3_TM <> cTm .And. !Empty(D3_ESTORNO))) .And. If(lEstornaIt .Or. Empty(D3_DOC), Recno() == nReg, .T.)}
Local bSeekWhile
Local cSeek	   	  := ""
Local cMvPar      := "mv_par04"
Local xMvParBkp
Local nPosRec     := 0
Local aBtnBack    := {}
Local lIntegDef   :=  FWHasEAI("MATA241")
Local aRetInt	  := {}
