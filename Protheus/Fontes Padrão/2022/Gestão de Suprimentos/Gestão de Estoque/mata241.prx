PRIVATE aRatVei   := {}
PRIVATE aRatFro   := {}
PRIVATE aArraySDG := {}
PRIVATE lEstornaIt:= .F.

// -- Protecao quando rotina for executada em modo automatico
If Type("aCtbDia") == "U"
   Private aCtbDia := {}
EndIf

l241Inc := IIf( Type( "l241Inc" ) == "L", l241Inc, .F. )

If !GETNEWPAR("MV_DCLNEW",".F.")
	AADD(aNoFields,"D3_OBS")
Endif

//здддддддддддддддддд©
//Ё Percentual FCI   |
//юдддддддддддддддддды
AADD(aNoFields,"D3_PERIMP")
