*/
Function A270Mark()
Local aAreaAnt	:= GetArea()
Local cMarca    := ThisMark()
If !Empty(SB7->B7_CONTAGE)
	If IsMark("B7_OK",cMarca)
		Reclock("SB7",.F.)
		Replace B7_OK With Space(Len(B7_OK))
		MsUnlock()
	Else
		Reclock("SB7",.F.)
		Replace B7_OK With cMarca
		MsUnlock()
		If mv_par04 == 1
			MarcaSimil(cMarca)
		EndIf
	EndIf
EndIf
RestArea(aAreaAnt)
MarkBRefresh()
Return .T.

