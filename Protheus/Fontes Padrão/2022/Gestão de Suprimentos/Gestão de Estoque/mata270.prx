			If SBF->(dBSeek(xFilial("SBF")+SB7->B7_LOCAL+SB7->B7_LOCALIZ+SB7->B7_COD+SB7->B7_NUMSERI+SB7->B7_LOTECTL+SB7->B7_NUMLOTE)).And. (SBF->BF_DINVENT == SB7->B7_DATA)
				lLocaliDt := .T.
