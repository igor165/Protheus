		dbSetOrder(2)  // seleciona o indice por produto
		dbSeek(aFiliais[nBusca]+cCod)
		dbSetOrder(1)  // retorna ao indice principal do arquivo
		If Found()
			lRet := .F.
			Exit
		EndIf
	Next
EndIf

If lRet
