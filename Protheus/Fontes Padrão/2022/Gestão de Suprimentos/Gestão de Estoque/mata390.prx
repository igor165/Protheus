Function M390dValid(dDataValid)
Local lRet	:= .T.

If dDataValid < dDataBase .And. dDataValid <> SB8->B8_DTVALID
	Help(" ",1,'M390DTVALID',,STR0035 + DtoC(dDataValid) + STR0036 + DtoC(dDataBase),1,0) //"Data informada: "#" И menor que "
	lRet := .F.
EndIf

Return lRet

