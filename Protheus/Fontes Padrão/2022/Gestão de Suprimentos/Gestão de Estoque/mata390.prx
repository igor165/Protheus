Static Function a390AfterC(nOpc,nSaldoB8,lEmpPrev,aRecno)

Local lRet := (nOpc<>2)
If lRet
	Reclock("SB8",.F.)
	AADD(aRecno,Recno())
	nSaldoB8 += SB8SALDO(,,,,,lEmpPrev,,,.T.)
EndIf
Return (lRet)

