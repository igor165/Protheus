	FillGetDados(2,"SB8",If(mv_par01==1,3,5),cSeek,bSeekWhile,{||.T.},,aYesFields,,,,,,,{|| a390AfterC(nOpc,@nSaldoB8,lEmpPrev,@aRecno)} )
