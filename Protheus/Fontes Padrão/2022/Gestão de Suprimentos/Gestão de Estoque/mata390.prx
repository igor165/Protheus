Function A390Preco(cAlias,nReg,nOpc)

Local aRecno     := {}
Local aObjects   := {},aPosObj  :={}
Local aSize      := MsAdvSize()
Local aInfo      := {aSize[1],aSize[2],aSize[3],aSize[4],3,3}

