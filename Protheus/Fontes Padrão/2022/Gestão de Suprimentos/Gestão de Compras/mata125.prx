Function A125ComView()

Local nPosCod := aScan(aHeader,{|x| AllTrim(x[2]) == "C3_PRODUTO"})

MaFisSave()
MaFisEnd()

If !AtIsRotina("MACOMVIEW")
	If !Empty(aCols[n][nPosCod])
		MaComView(aCols[n][nPosCod])
	EndIf
EndIf

MaFisRestore()

Return
