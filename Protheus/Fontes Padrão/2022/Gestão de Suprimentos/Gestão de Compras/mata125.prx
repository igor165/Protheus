							ConFirmSX8()
						EndDo
					End Transaction
				EndIf
			Else
				lGravaOk := A125Grava(l125Deleta,aRefImpos)
				If !lGravaOk
					Help(" ",1,"A125NAOREG")
					While ( GetSX8Len() > nSaveSX8 )
