		nDupTotal := Round(nDupTotal + aCols[n][nPosVl],MsDecimais(nMoedaNF))
	endif
Next n
If Abs(nBaseDup2-nDupTotal) > GetMV("MV_LIMPAG")
	HELP(" ",1,"A100VALDUP",,GETMV("MV_SIMB"+AllTrim(Str(nMoedaCor)))+" "+Str(Abs(nBaseDup2-nDupTotal)),3,0)
	lRet:=.F.
EndIf
Return lRet

