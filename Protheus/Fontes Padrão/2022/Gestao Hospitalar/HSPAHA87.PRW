// Funcao de Exclusao
Function HS_Exc87()                         
 Local lRet := .T.
 DbSelectArea("GA7")
 DbSetOrder(4)
 If DbSeek(xFilial("GA7") + M->X5_CHAVE)
  HS_MsgInf(STR0010, STR0012, STR0013) //"Existe este Porte Anestesico no Cabecalho de Procedimentos (GA7). Impossivel Exclui-lo!"###"Atencao"###"Exclusão não Permitida"
  lRet := .F.
 Endif

 DbSelectArea("GA3")
 DbSetOrder(2)
 If DbSeek(xFilial("GA3") + M->X5_CHAVE)
  HS_MsgInf(STR0011, STR0012, STR0013) //"Existe este Porte Anestesico na amarração Tabela de Procedimento e Porte Anestesico (GA3). Impossivel Exclui-lo!"###"Atencao"###"Exclusão não Permitida"
  lRet := .F.
 Endif      
 
 DbSelectArea(cAlias)
 If lRet
  nOpca := 1
  oDlg:End()          
 EndIf 
Return(lRet)


