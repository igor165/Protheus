Function HS_VExcA13()
 Local aArea := GetArea()
 Local lRet  := .T.
 
 DbSelectArea("GDY")
 DbSetOrder(2) //GDY_FILIAL + GDY_CODDIE + GDY_ATIVO
 If DbSeek(xFilial("GDY") + M->X5_CHAVE)
  HS_MsgInf(STR0010, STR0011, STR0001) //"Dieta ja utilizada em atendimento. Exclusao nao permitida!"###"Atencao"###"Dietas"
  lRet := .F.
 Endif
 RestArea(aArea)
Return(lRet)


