Function HS_ABCExc()

Local aArea := GetArea()
Local lRet  := .T.

dbSelectArea("GEO")
dbSetOrder(3)
If dbSeek(xFilial("GEO")+GEN->GEN_CODIGO)
	HS_MsgInf(STR0004, STR0005, STR0006) //"Existe este fator de risco no cadastro de Notificacao. Impossivel Exclui-la!"###"Atencao"####"Validação de exclusão"
	lRet := .F.
EndIf

RestArea(aArea)

Return( lRet )