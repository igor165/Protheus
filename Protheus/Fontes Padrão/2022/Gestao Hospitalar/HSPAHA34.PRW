// Funcao de Exclusao
Function HS_Exc34()
 DbSelectArea("GAI")
 DbSetOrder(5)
 If DbSeek(xFilial("GAI") + PADR(M->X5_CHAVE,2) )
  HS_MsgInf(STR0010, STR0009, STR0012) //"Existe este Codigo de Armazem no Arquivo de Solicitacoes (GAI). Impossivel Exclui-lo!"###"Atenção"###"Inclusao/Alteracao/Visualizacao"
  Return(.F.)
 Endif   
 DbSelectArea("GCS")
 DbSetOrder(1)
 DBGotop()
 Do While !Eof()
  If GCS->GCS_ARMSET == Padr(M->X5_CHAVE,2)
   HS_MsgInf(STR0011+AllTrim(GCS->GCS_NOMLOC)+STR0013, STR0009, STR0012) //"Este Codigo Esta relacionado no Cadastro de Setor: "###" ...Impossivel exclui-lo."###"Atenção"###"Inclusao/Alteracao/Visualizacao"
   Return(.F.)
  Endif
  DbSkip()
 End         
 nOpca := 1
 oDlg:End()          

Return(.T.)


