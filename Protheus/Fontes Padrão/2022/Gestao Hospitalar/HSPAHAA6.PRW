FUNCTION HSPAHAA6()
 Private cCadastro := STR0001 //"Cadastro de carater do atendimento"
 Private aRotina := MenuDef()

 DbSelectArea("GD1")
 DbSetOrder(1)

 mBrowse( 6,1,22,75, "GD1")
Return(Nil)

Function HS_VEAA6()
 Local lRet := .T., cAliasOld := Alias()
 
 DbSelectArea("GCY")
 DbSetOrder(6)
 
 If !(lRet := !DBSeek(xFilial("GCY") + GD1->GD1_CARATE))
  Help(" ", 1, "HAA6RELGCY",, STR0007, 1) //"Registro encontrado no atendimento e não pode ser excluido"
 EndIf  
  
 DbSelectArea(cAliasOld)
Return( lRet )

Function HS_AA6(cAlias, nReg, nOpc)
 Local nOpcA := 0
 
 Private cDelFunc := "HS_VEAA6()"

 If nOpc == 3
  nOpcA := axInclui("GD1",, nOpc,,,, /*Função para validar  a inclusao*/) 
 ElseIf nOpc == 5
  nOpcA := axDeleta("GD1", nReg, nOpc)
 EndIf                            
Return(nOpcA)

Function HS_VldAA6()
	Local lRet 	:= .T.
	Local aArea	:= GetArea()
	
	/* Codigo do SUS */
	If ReadVar() == "M->GD1_CATAIH"
		If !Empty(M->GD1_CATAIH)		
			DbSelectArea("GD1")
			DbSetOrder(3)
			If DbSeek(xFilial("GD1") + M->GD1_CATAIH)
				If (M->GD1_CARATE # GD1->GD1_CARATE)
					HS_MsgInf(STR0008, STR0009, STR0010) //"Código do SUS já cadastrado!"###"Atenção"###"Validação dos Campos"
					lRet := .F.
				EndIf
			EndIf
		EndIf
 EndIf
	
	RestArea(aArea)
Return(lRet)


