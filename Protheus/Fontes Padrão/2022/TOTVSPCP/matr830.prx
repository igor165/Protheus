Else
	cMaqAnt := cFilial+cMaquina
	While !eof() .and. H2_FILIAL+H2_RECPRIN == cMaqAnt
		If H2_TIPO == "S"
			cMaqSec += H2_RECALTE+" "
		EndIf
		dbSkip()
	EndDo
	dbSelectarea("SH1")
	Return cMaqSec
EndIf

