Function A790Deleta(cAlias,nReg,nOpc)
Local nOpcA
Local oDlg,i
Local aObjects := {}                  
Local aSize    := {} 
Local aInfo    := {}
Local lModNG := nModulo == 19 .Or. nModulo == 95 // SIGAMNT e SIGAGFR
Default lAutoMacao := .F.

If Bof() .Or. Eof()
	Help(" ",1,"ARQVAZIO")
	Return
EndIf

If (H9_ORIGEM == "1") .and. !lModNG
	Help(" ",1,"NGMT790FER")
	Return
Endif	

If (H9_ORIGEM # "1") .and. lModNG
	Help(" ",1,"NGMT790FER")
	Return
Endif	
//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Monta a entrada de dados do arquivo                          ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
Private aTELA[0][0],aGETS[0]

nOpcA:=0
dbSelectArea(cAlias)
bCampo := {|nCPO| Field(nCPO) }
For i := 1 TO FCount()
	M->&(EVAL(bCampo,i)) := FieldGet(i)
Next i

SoftLock(cAlias)

aSize := MsAdvSize()       
aObjects := {}
AAdd( aObjects, { 100, 100, .T., .T. } )
aInfo    := { aSize[1], aSize[2], aSize[3], aSize[4], 3, 3 } 
aPosObj := MsObjSize( aInfo, aObjects ) 

IF !lAutoMacao
	DEFINE MSDIALOG oDlg TITLE cCadastro FROM aSize[7],0 TO aSize[6],aSize[5] OF oMainWnd PIXEL
	nOpcA:=EnChoice( cAlias, nReg, nOpc,,,,aAcho,{aPosObj[1,1],aPosObj[1,2],aPosObj[1,3],aPosObj[1,4]})
	ACTIVATE MSDIALOG oDlg ON INIT EnchoiceBar(oDlg,{|| nOpca := 2,oDlg:End()},{|| nOpca := 1,oDlg:End()})

	IF nOpcA == 2
		Begin Transaction
		dbSelectArea(cAlias)
		RecLock(cAlias,.F.,.T.)
		dbDelete()
		End Transaction
	Else
		MsUnLock()
	Endif
ENDIF

dbSelectArea(cAlias)
Return

