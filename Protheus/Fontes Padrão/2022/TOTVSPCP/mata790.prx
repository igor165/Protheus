Local nOpca
Local lModNG := nModulo == 19 .Or. nModulo == 95 // SIGAMNT e SIGAGFR
Default lAutoMacao := .F.

IF !lAutoMacao
	nOpca := AxInclui(cAlias,nReg,nOpc,aAcho,,,"A790TudoOk()")
