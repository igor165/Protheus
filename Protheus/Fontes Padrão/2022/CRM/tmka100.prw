Static Function TMKA100Cmt(oMdl)
Local lRet := .T.
Local nOperation := oMdl:GetOperation()

if nOperation == 5
	DbSelectArea("AGI")
	DbSelectArea("AGJ")
	DbSetOrder(1)
	
	If DbSeek(xFilial("AGJ") + AGI->AGI_COD)
	    lRet := .F.
		msgAlert(STR0002)		//Esse grupo DAC não pode ser excluido porque está associado à um Grupo de Atendimento
		Return(lRet)
	EndIf
	
EndIf

FWModelActive(oMdl)
FWFormCommit(oMdl)

Return(lRet)


