Static Function MTA335Perg(lTela)
Local ni      := 0

Default lTela := .T.

If lTela
	If pergunte("MTA335",.T.)
		For ni := 1 to 4
			a335ParamZX[ni] := &("mv_par"+StrZero(ni,2))
		Next ni
		dDataAjust := mv_par05
	Else
		Return .F.
	EndIf      
Else
	Pergunte("MTA335",.F.)
	For ni := 1 to 04
		a335ParamZX[ni] := &("mv_par"+StrZero(ni,2))
	Next ni
	dDataAjust := mv_par05
EndIf

Return .T.
