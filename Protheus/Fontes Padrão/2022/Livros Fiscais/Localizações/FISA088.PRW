Function FISA088()
	
	Local aGetArea 	:= GetArea()
	Local aSays   	:= { }
	Local aButtons	:= { }
	Local nOpca   	:= 0
	Local cPerg   	:= "FISA088"
	Local cCadastro	:= STR0001 // "Generacin de la Declaracin Informativa de Operaciones con Terceros"
	
	Private cMes    := ''
	Private cAnio   := ''
	Private cMesAno := ''
	Private cSucur  := ''
	Private dFecIni := CToD("  /  /  ")
	Private dFecFin := CToD("  /  /  ")
	Private lAutomato := isblind()
	
	aAdd(aSays, STR0002) // "Esta rutina genera la informacin necesaria para la presentacin de  DIOT."
	aAdd(aSays, STR0003) // "Despus de este proceso, se podr ejecutar la Generacin del Archivo y los Informes"
	aAdd(aSays, STR0004) // "necesarios."

	aAdd(aButtons, {5, .T., {|| Pergunte(cPerg, .T. ) } } )
	aAdd(aButtons, {1, .T., {|o| nOpca := 1, IIf(TodoOK(cPerg),FechaBatch(), nOpca := 0)}})
	aAdd(aButtons, {2, .T., {|o| FechaBatch() }} )

	If !lAutomato
		FormBatch( cCadastro, aSays, aButtons )

		If nOpca == 1 //Ejecuta el proceso
			Processa({|| FS088GERA() },OemToAnsi(STR0005)) //"Procesando..."
		EndIf
	Else
		If TodoOK(cPerg)
			FS088GERA()
		Endif
	EndIf

	RestArea(aGetArea)

Return

/*


ͻ
Funcao    TodoOK    Autor  M.Camargo            Data   21/04/14   
͹
Desc.     Validacion de los datos antes de Ejecutar el proceso        
͹
Uso                                                                   
ͼ


*/
Static Function TodoOK(cPerg)
	
	//Ŀ
	// Variaveis utilizadas para parametros         
	// MV_PAR01 // Mes y Ao del periodo a procesar 
	// MV_PAR02 // Sucursales a procesar            
	//
	
	Pergunte(cPerg,.F.)
	
	cMesAno := MV_PAR01
	cSucur  := MV_PAR02
	cMes    := StrZero(Val(Left(cMesAno,2)),2)
	cAnio   := Right(cMesAno,4)
	cLisSuc := AllTrim(cSucur)

	If Val(cMes) < 1 .Or. Val(cMes) > 12
		MsgInfo(STR0006) // "El mes debe ser de 1 a 12!"
		Return .F.
	EndIf

	If Val(cAnio) < 1900
		MsgInfo(STR0007) // "El ao debe ser mayor a 1900!"
		Return .F.
	EndIf

	If !Empty(cLisSuc)
	    cSucur := FS088Traduce(cLisSuc)
	Else
		MsgInfo(STR0035) // "Seleccione al menos una sucursal "
		Return .F.
	EndIf

	dFecIni := Ctod("01/"+ cMes+ "/" +Substr(cAnio,3,2)+"/")
	dFecFin := Ctod(StrZero(f_UltDia(dFecIni), 02) + "/" + cMes + "/" + SubStr(cAnio, 3, 2))

Return .T.

