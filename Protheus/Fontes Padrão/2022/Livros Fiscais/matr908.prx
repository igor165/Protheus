Local lHouveMov 	:= .F.
Local lQuery    	:= .F.
Local lImprime		:= .F.
Local lPublico  	:= .T.
Local lF3_Reciss 	:= SF3->(FieldPos ("F3_RECISS"))>0
Local lDocCanc		:= .T.
Local lDocCupom		:= .T.
Local lPula			:= .F.

Local dDatAtu  		:= ""
Local dDatAnt   	:= ""

Local cModNot		:= GetNewPar("MV_M908MOD","")
Local cModelo		:= ""
Local aLay   	 	:= Array(24)
Local aStru			:= {}
Local aObservacao   := {}
Local aObserva2  	:= {}
