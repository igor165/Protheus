	Static Function A941Reg61(aTotReg61)
	Local cRegistro, i

	For i := 1 to len( aTotReg61 )
		cRegistro:="61"
		cRegistro+=A941FILL(,28) 										// brancos
		cRegistro+=A941FILL(aTotReg61[i,3],08)						// Data de emissao dos doc. fiscais
