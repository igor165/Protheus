#IFDEF TOP
static function FormatIN(cRet)
Local nX 	:=	0 
Local cNovo	:=	""

For nX := 1 to Len(cRet)
		If SubStr(cRet,nX,1)$"0123456789"
			cNovo += SubStr(cRet,nX,1)
		Else 
			If SubStr(cRet,nX+1,1)$"0123456789"
				cNovo := Alltrim(cNovo) + "','"
			Endif
		Endif                                                          
	Next
	cNovo	:= "('" + Alltrim(cNovo) + "')"
Return(cNovo)
#ENDIF
