*/
STATIC FUNCTION R966Cabec(nLin,lFirst)
lFirst	:=	IIf(Empty(lFirst),.f.,lFirst)
If nLin>=55 .and. !lEnd
    If !lFirst
        @ ++nLin,117 PSAY STR0006 //"( Continua... )"
        lFirst	:=	.f.
    Endif
    Cabec(titulo,cabec1,cabec2,wnrel,nTamanho)
    nLin	:=	PRow()
    @ ++nLin,00 PSAY Replic("*",132)
    nLin++
Endif
Return (nLin)
