Function MNTR760()

	WNREL      := "MNTR760"
	LIMITE     := 080
	cDESC1     := STR0001 //"O relatório apresenta a quantidade de eventos apurados "
	cDESC2     := STR0002 //"pelo jurídico, tabulando por tipo de acidente"
	cDESC3     := " "
	cSTRING    := "TRJ"

	Private NOMEPROG := "MNTR760"
	Private TAMANHO  := "P"
	Private aRETURN  := {STR0003,1,STR0004,1,2,1,"",1} //"Zebrado"###"Administracao"
	Private TITULO   := STR0005 //"Relatório de Eventos Apurados pelo Jurídico por Período"
	Private nTIPO    := 0
	Private nLASTKEY := 0
	Private CABEC1,CABEC2
	Private aVETINR := {}
	Private cPERG := "MNR760"
	Private aPerg :={}

	SetKey( VK_F9, { | | NGVersao( "MNTR760" , 1 ) } )

	Pergunte(cPERG,.F.)
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Envia controle para a funcao SETPRINT                        ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	WNREL:=SetPrint(cSTRING,WNREL,cPERG,TITULO,cDESC1,cDESC2,cDESC3,.F.,"")
	If nLASTKEY = 27
		Set Filter To
		DbSelectArea("TRJ")
		Return
	EndIf
	SetDefault(aReturn,cSTRING)
	RptStatus({|lEND| MNTR760IMP(@lEND,WNREL,TITULO,TAMANHO)},STR0008,STR0009) //"Aguarde..."###"Processando Registros..."
	Dbselectarea("TRJ")

Return .T.

