/*/
 Function A755CHAVE()

	Local cFilVld := A755FILIAL(M->TSJ_PLACA)

	If !Empty(M->TSJ_CODBEM) .And. !Empty(M->TSJ_PLACA) .And. !Empty(M->TSJ_DTINIC)

		dbSelectArea("ST9")
		dbSetOrder(14)
		dbSeek(M->TSJ_PLACA)

		dbSelectArea("TSJ")
		dbSetOrder(01)
		dbSeek(cFilVld+M->TSJ_CODBEM, .F.)

		While cFilVld == TSJ->TSJ_FILIAL .And. M->TSJ_CODBEM == TSJ->TSJ_CODBEM
			If M->TSJ_DTINIC == TSJ->TSJ_DTINIC
				HELP(" ",1,STR0019,,STR0020+CHR(13)+STR0021,3,1) //"ATEN«√O"##"J‡ existe um registro com a mesma"##"informaÁ„o de VeÌculo + Dt.Inic.Leasing."
				Return .F.
			EndIf
			dbSkip()
		EndDo
	EndIf

 Return .T.

