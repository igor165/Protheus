 Function MNTA755GRA()

	Local aOldArea := GetArea()
	Local cFilBem  := Space(FWSizeFilial())

	cFilBem := A755FILIAL(M->TSJ_PLACA,3)

	dbSelectArea("ST9")
	dbSetOrder(01)
	If dbSeek(cFilBem+M->TSJ_CODBEM)

		RecLock("ST9",.F.)

		If Empty(M->TSJ_DTQULE)
			ST9->T9_STATUS := cNGLEASP
		Else
			ST9->T9_STATUS := cNGLEASQ
		Endif
		MsUnlock("ST9")

	EndIf

	RestArea(aOldarea)

 Return .T.

