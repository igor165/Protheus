 Function M755VLDSRA()

	Local cFilVld := A755FILIAL( M->TSJ_PLACA, 2 )

	Local cQuery     := ""
	Local cAliasQry  := "T9SRA"
	Local lReturn    := .T.

	cQuery := ' SELECT DISTINCT 1 AS "Possui"'
	cQuery += "   FROM " + RetSQLName("SRA")
	cQuery += "  WHERE RA_MAT = " + ValToSql(&(ReadVar()))
	cQuery += "   AND " + NGMODCOMP("SRA","SRA", "=", , cFilVld) // FunÁ„o que monta o AND RA_FILIAL com base no compartilhamento de tabelas.

	cQuery := ChangeQuery(cQuery)
	dbUseArea( .T., "TOPCONN", TCGENQRY(,,cQuery),cAliasQry, .F., .T.)

	dbSelectArea(cAliasQry)
    cPossui := (cAliasQry)->Possui

	If Empty(cPossui)
		// Se a Query n„o encontrou nada, o arquivo retorna vazio, e isso significa
		// que a matricula n„o existe ou n„o tem permiss„o para ser respons·vel pelo veÌculo.
		HELP(" ",1,"REGNOIS")
		lReturn := .F.
	EndIf

	(cALIASQRY)->(dbCloseArea())

 Return lReturn

