Function Sg050Classe()
Local aClasse := {}
Local aOldArea := GetArea() // Guarda variaveis de alias e indice

DbSelectArea("TA8")
DbSetOrder(1)
DbSeek(xFilial("TA8"))

Do While !Eof() .and. xFilial("TA8") == TA8->TA8_FILIAL
	aAdd( aClasse,{ TA8->TA8_LIMMIN,TA8->TA8_LIMMAX,TA8->TA8_DESCRI, TA8->TA8_CODCLA } )
	DbSkip()
EndDo      
                                    
RestArea(aOldArea)     
Return aClasse

