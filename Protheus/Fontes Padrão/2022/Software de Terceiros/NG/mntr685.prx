/*/
Static Function ReportPrint(oReport)
	Local oSection1 := oReport:Section(1)
	Local oSection2 := oReport:Section(2)

	Private nordem
	Private tipo
	Private nproduto
	Private nnome
	Private cBloq := "S"
	Private cDest := Space(1)


	If mv_par01 = 1          // PRODUTOS
		tiporeg  := 'P'
		calias   := 'SB1'
		tipo     := STR0007 //"Produto"
	Elseif mv_par01 = 2   // FUNCIONARIO
		tiporeg  := 'M'
		calias   := 'ST1'
		tipo     := STR0008 //"Funcionario"
	Elseif mv_par01 = 3   // FERRAMENTAS
		tiporeg  := 'F'
		calias   := 'SH4'
		tipo     := STR0009 //"Ferramenta"
	Elseif mv_par01 =4   // TERCEIROS
		tiporeg  := 'T'
		calias   := 'SA2'
		tipo     := STR0010 //"Terceiros"
	Else
		tiporeg  := 'E'
		calias   := 'ST0'
		tipo     := STR0014//"Especialidade"
	Endif

	#IFDEF TOP
	fMNTR685ST() //Monta TRB a partir de consulta SQL

	dbSelectArea((cTRB))
	dbGoTop()
	oReport:SetMeter(LastRec())
	While !Eof()
		If (cTRB)->TJ_TIPOOS = "B" .or. (cTRB)->TJ_TIPOOS = "L"
			nproduto := (cTRB)->TL_CODIGO

			Dbselectarea(calias)
			Dbsetorder(01)
			If Dbseek(xFilial(calias)+alltrim(nproduto))
				If mv_par01 = 1       // PRODUTOS
					nnome  := sb1->b1_desc
				Elseif mv_par01 = 2   // FUNCIONARIO
					nnome  := st1->t1_nome
				Elseif mv_par01 = 3   // FERRAMENTAS
					nnome := sh4->h4_DESCRI
				Elseif mv_par01 = 4   // TERCEIROS
					nnome := sa2->a2_nome
				Else                  //  ESPECIALIDADE
					nnome := st0->t0_nome
				Endif
			Endif


			oReport:SkipLine(2)
			oSection1:Init()
			oSection1:PrintLine()
			oSection2:Init()

			While !Eof() .And. (cTRB)->TL_CODIGO == nproduto
				nnome := ""

				oReport:IncMeter()

				nordem := (cTRB)->TL_ORDEM

				cBloq := STR0016 //"Sim"
				cDest := Space(1)

				If (cTRB)->TL_REPFIM = "N"
					cBloq := STR0017 //"Não"
				Endif

				If (cTRB)->TL_TIPOREG = "P"
					If (cTRB)->TL_DESTINO == "T"
						cDest := STR0018 //"Troca"
					Elseif (cTRB)->TL_DESTINO == "A"
						cDest := STR0019 //"Apoio"
					Elseif (cTRB)->TL_DESTINO == "S"
						cDest := STR0020 //"Substituição"
					Endif
				Endif

				If (cTRB)->TJ_TIPOOS = "B"
					If NGIFDBSEEK("ST9",(cTRB)->TJ_CODBEM,1)
						cNomeBem := SubStr(ST9->T9_NOME,1,20)
					Else
						cNomeBem := Space(Len(ST9->T9_NOME))
					EndIf
				ElseIf (cTRB)->TJ_TIPOOS = "L"
					If NGIFDBSEEK("TAF","X2"+(cTRB)->TJ_CODBEM,7)
						cNomeBem := SubStr(TAF->TAF_NOMNIV,1,20)
					Else
						cNomeBem := Space(Len(TAF->TAF_NOMNIV))
					EndIf
				EndIf

				If NGIFDBSEEK("ST4",(cTRB)->TJ_SERVICO,1)
					cNomeSer := SubStr(ST4->T4_NOME,1,20)
				Else
					cNomeSer := Space(Len(ST4->T4_NOME))
				EndIf

				oSection2:PrintLine()
				Dbselectarea((cTRB))
				Dbskip()
			End
			oSection1:Finish()
			oSection2:Finish()
		Else
			Dbselectarea((cTRB))
			Dbskip()
		Endif
		Dbselectarea((cTRB))
	End

	fMNTR685DE() //Deleta Tab. Temp.

	#ELSE
	Dbselectarea("STL")
	Dbsetorder(02)
	Dbseek(xFilial('STL')+tiporeg+"0  ")
	oReport:SetMeter(LastRec())
	While !Eof() .and. stl->tl_filial == xFilial("STL") .And.;
	stl->tl_tiporeg ==  tiporeg  .And. Alltrim(stl->tl_seqrela) = "0"

		Dbselectarea("STJ")
		Dbsetorder(01)
		If Dbseek(xFilial('STJ')+stl->tl_ordem+stl->tl_plano)
			If stj->tj_tipoos = "B" .or. stj->tj_tipoos = "L"
				nproduto := stl->tl_codigo

				Dbselectarea(calias)
				Dbsetorder(01)
				If Dbseek(xFilial(calias)+alltrim(nproduto))
					If mv_par01 = 1       // PRODUTOS
						nnome  := sb1->b1_desc
					Elseif mv_par01 = 2   // FUNCIONARIO
						nnome  := st1->t1_nome
					Elseif mv_par01 = 3   // FERRAMENTAS
						nnome := sh4->h4_DESCRI
					Elseif mv_par01 = 4   // TERCEIROS
						nnome := sa2->a2_nome
					Else                  //  ESPECIALIDADE
						nnome := st0->t0_nome
					Endif
				Endif

				oReport:SkipLine(2)
				oSection1:Init()
				oSection1:PrintLine()
				oSection2:Init()

				While !Eof() .and. stl->tl_filial == xFilial("STL") .And.;
				stl->tl_tiporeg ==  tiporeg  .And. Alltrim(stl->tl_seqrela) = "0";
				.And. stl->tl_codigo == nproduto

					If stl->tl_ordem >= mv_par03 .And. stl->tl_ordem <= mv_par04 .And. stl->tl_plano = mv_par02
						nnome := ""

						oReport:IncMeter()

						nordem := STL->TL_ORDEM

						Dbselectarea("STJ")
						Dbsetorder(01)
						Dbseek(xFilial("STJ")+nordem+MV_PAR02)

						cBloq := STR0016 //"Sim"
						cDest := Space(1)

						If STL->TL_REPFIM = "N"
							cBloq := STR0017 //"Não"
						Endif

						If STL->TL_TIPOREG = "P"
							If STL->TL_DESTINO == "T"
								cDest := STR0018 //"Troca"
							Elseif STL->TL_DESTINO == "A"
								cDest := STR0019 //"Apoio"
							Elseif STL->TL_DESTINO == "S"
								cDest := STR0020 //"Substituição"
							Endif
						Endif

						oSection2:PrintLine()
					Endif
					Dbselectarea("STL")
					Dbskip()
				End
				oSection1:Finish()
				oSection2:Finish()
			Else
				Dbselectarea("STL")
				Dbskip()
			Endif
		Endif
		Dbselectarea("STL")
	End
	#ENDIF

	//³ Devolve a condicao original do arquivo principam             ³
	RetIndex("STJ")
	RetIndex("STL")
	Set Filter To

	MS_FLUSH()

Return .T.

