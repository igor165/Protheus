/*/{Protheus.doc} MDTC340QU
Monta um browse com os questionarios do cliente

@type    function
@author  Andre Perez Alvarez
@since   20/11/2007
@sample  MDTC340QU()
@return  Nil, Sempre nulo
/*/
//-------------------------------------------------------------------
Function MDTC340QU()

	Local aArea	:= GetArea()
	Local oldROTINA := aCLONE(aROTINA)
	Local oldCad := cCadastro
	cCliMdtPs := SA1->A1_COD+SA1->A1_LOJA

	aRotina := { { STR0001, "AxPesqui"  , 0 , 1 }, ; //"Pesquisar"
			 	 { STR0002, "NGCAD01"   , 0 , 2 }, ; //"Visualizar"
				 { STR0003, "MDC340QUE" , 0 , 3 }  } //"Questoes"

	cCadastro := OemToAnsi(STR0004) //"Questionario"

	//  Endereca a funcao de BROWSE
	dbSelectArea("TMG")
	Set Filter To TMG->(TMG_CLIENT+TMG_LOJA) == cCliMdtps
	dbSetOrder(2)

	mBrowse( 6, 1,22,75,"TMG")

	//  Recupera a Ordem Original do arquivo principal
	dbSelectArea("TMG")
	Set Filter To

	aROTINA := aCLONE(oldROTINA)
	RestArea(aArea)
	cCadastro := oldCad

Return Nil

//---------------------------------------------------------------------
/*/{Protheus.doc} MDT340RES
Função para mostrar o resultado.

@type    function
@author  Jean Pytter da Costa
@since   04/04/2014
@sample  MDT340RES()
@return  lRet, Lógico, Verdadeiro se possuir alguma pergunta marcada
/*/
//---------------------------------------------------------------------
Function MDT340RES()

	Local lRet := .T. // Variável para mostrar o resultado, caso possua alguma pergunta marcada.
	Local nPer := 0   // Variável para verificar se possui pergunta marcada no MarkBrowse.
	Local nCan := 0   // Variável para verificar se pergunta foi cancelada.
	Local oTempTRB

	lTEMMARK := .F.
	dbSelectArea("TRBH")
	DbGoTop()

	Do While TRBH->(!EoF()) //Verifica se algum item foi marcado no MarkBrwose.
		If !Empty(TRBH->TMH_OK)
			lTEMMARK := .T.
			nPer := 1 //Caso alguma pergunta estiver marcada.
			Exit
		EndIf
		DbSelectArea("TRBH")
		dbSkip(1)
	End

	If !lTEMMARK //Caso o MarkBrowse não possuir marcações.
		dbSelectArea("TRBH")
		dbGoTop()
		lRet := .F.
	EndIf

	//  Variaveis utilizadas para parametros
	//  mv_ch1     // De  Data
	//  mv_ch2     // Ate Data
	If !Pergunte("MDT340", lRet)//Se possuir algum item marcado no MarkBrowse Pergunta.
		dbSelectArea("TRBH")
		lRet := .F.
		nCan := 1 //Se pergunta foi cancelada.
	EndIf

	If lRet
		aDBF := { { "QUESTA"  , "C", 03      , 0 } ,;
			      { "PERGUN"  , "C", nSizeTMH, 0 } ,;
				  { "SIM"     , "N", 10      , 0 } ,;
				  { "NAO"     , "N", 10      , 0 } }

		oTempTRB := FWTemporaryTable():New( "TRB", aDBF )
		oTempTRB:AddIndex( "1", {"QUESTA"} )
		oTempTRB:Create()

		DbSelectArea("TRBH")
		DbGoTop()

		Do While !EoF()

			If !Empty(TRBH->TMH_OK)

				If lSigaMdtps   // Prestador

					DbSelectArea("TMI")
					Dbsetorder(2)
					If Dbseek(xFilial('TMI')+TRBH->TMH_QUESTI)

						Do While !EoF() .And. TMI->TMI_FILIAL == xFilial("TMI")  .And.;
							TMI->TMI_QUESTI == TRBH->TMH_QUESTI

							dbSelectArea("TM0")
							dbSetOrder(1)
							dbSeek(xFilial("TM0")+TMI->TMI_NUMFIC)
							If TM0->(TM0_CLIENT+TM0_LOJA) <> cCliMdtps
								DbSelectArea("TMI")
								dbSkip(1)
								Loop
							EndIf

							If TMI->TMI_DTREAL >= MV_PAR01 .And. TMI->TMI_DTREAL <= MV_PAR02;
									.And.TMI->TMI_QUESTA = TRBH->TMH_QUESTA

								dbSelectArea("TRB")
								if !dbSeek(TMI->TMI_QUESTA)
									TRB->(DbAppend())
									Replace TRB->QUESTA with TMI->TMI_QUESTA
									Replace TRB->PERGUN with TRBH->TMH_PERGUN
								EndIf

								If TMI->TMI_RESPOS = '1'
									Replace TRB->SIM with TRB->SIM + 1
								else
									Replace TRB->NAO with TRB->NAO + 1
								EndIf

							EndIf

							DbSelectArea("TMI")
							dbSkip(1)
						End

					EndIf

				Else // Não-prestador

					DbSelectArea("TMI")
					Dbsetorder(2)
					If Dbseek(xFilial('TMI')+TRBH->TMH_QUESTI)

						Do While !EoF() .And. TMI->TMI_FILIAL == xFilial("TMI")  .And.;
							TMI->TMI_QUESTI == TRBH->TMH_QUESTI

							If TMI->TMI_DTREAL >= MV_PAR01 .And. TMI->TMI_DTREAL <= MV_PAR02;
									.And.TMI->TMI_QUESTA = TRBH->TMH_QUESTA

								dbSelectArea("TRB")
								If !dbSeek(TMI->TMI_QUESTA)
									TRB->(DbAppend())
									Replace TRB->QUESTA with TMI->TMI_QUESTA
									Replace TRB->PERGUN with TRBH->TMH_PERGUN
								EndIf

								If TMI->TMI_RESPOS = '1'
									Replace TRB->SIM with TRB->SIM + 1
								ElseIf TMI->TMI_RESPOS = '2'
									Replace TRB->NAO with TRB->NAO + 1
								EndIf

							EndIf
							DbSelectArea("TMI")
							dbSkip(1)

						End

					EndIf

				EndIf

			EndIf

			DbSelectArea("TRBH")
			dbSkip(1)

		End

		DbselectArea('TRB')
		Dbgotop()
		cTitulo := STR0007+Alltrim(TMG->TMG_NOMQUE)+' ( '+dtoc(mv_par01)+' a '+dtoc(mv_par02)+' )' //'Questionario - '

		DEFINE DIALOG oDlg1 FROM 6,0 TO 34,100 TITLE cTitulo

		@ 04,020 BUTTON STR0008 OF oDlg1 SIZE 40,09 PIXEL ACTION (MDC340GRA()) //"&Grafico"
		@ 04,060 BUTTON STR0009 OF oDlg1 SIZE 40,09 PIXEL ACTION (MDC340IMP()) //"&Imprimir"

		@ 01,0.3 LISTBOX FIELDS QUESTA                                ,;
								PERGUN                                ,;
								PADL(Transform(SIM,"@E 999,999"),12)  ,;
								PADL(Transform(NAO,"@E 999,999"),12)   ;
							FieldSizes 40,200,70,70                    ;
							Size 388,195                               ;
							HEADERS STR0005                           ,; //"Pergunta"
									STR0010                           ,; //"Nome Pergunta"
									STR0011                           ,; //"Sim"
									STR0012                              //"Nao"
		ACTIVATE DIALOG oDlg1
	Else
		If nPer == 0 //Se não possuir pergunta marcada.
			ShowHelpDlg(STR0036,{STR0037},1,{STR0038},1)//"ATENÇÃO"##"Não possui pergunta marcada."##"Favor selecionar no minímo uma pergunta para verificar o resultado."
		EndIf
	EndIf

	If nCan == 0 .And. nPer == 1 //Se não foi cancelado###Se foi marcado alguma pergunta
		oTempTRB:Delete()
	EndIf
	DbSelectArea("TRBH")
	dbgotop()

Return lRet
