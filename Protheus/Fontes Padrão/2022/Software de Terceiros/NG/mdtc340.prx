/*/{Protheus.doc} c340INVERT
Inverte marcações

@type    function
@author  Paulo Pego
@since   09/11/1999
@sample  c340INVERT()
@return  Nil, Sempre nulo
/*/
//-------------------------------------------------------------------
Function c340INVERT()

	Local nREG := TRBH->(RECNO())

	DBSELECTAREA("TRBH")
	DBGOTOP()
	DO WHILE !EoF()
		TRBH->TMH_OK := IIF(TMH_OK == "  ",cMARCA,"  ")
		dbSkip()
	EndDo

	TRBH->(DBGOTO(nREG))
	lREFRESH := .T.

Return Nil

