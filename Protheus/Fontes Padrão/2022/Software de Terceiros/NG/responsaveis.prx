Function Sg100CarQdj(aQdGDoc)
Local cNomeFun := ''

DbSelectArea("TAK")
TAK->(DbSetOrder(1))
TAK->(DbGotop())
While TAK->(!Eof()) 
	If !Empty( TAK->TAK_CODEST )
		DbSelectArea("SRA")
		DbSetOrder(1)
		If DbSeek(xFilial("SRA")+TAK->TAK_MAT)                              
			cNomeFun := SRA->RA_NOME
		EndIf	    
		If aScan( aQdgDoc,{|x| x[5] == TAK->TAK_MAT } ) == 0
	  		Aadd(aQdGDoc,{"","",TAK->TAK_CODFUN,'S',TAK->TAK_MAT,cNomeFun})
	  	Endif	
	EndIf	                                                 
	TAK->(DbSkip())
End

Return 