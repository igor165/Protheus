Function SgRespGrav()  
Local i,lDelete := .f.
   
For i:= 1 To Len( aQDGDoc )     

    DbSelectArea('TAK')
	 DbSetOrder(1)
	 If DbSeek( xFilial('TAK')+'001'+cLocal+aQdgDoc[i][5])    
    	 RecLock('TAK',.f.)
       lDelete := .t.  	  
	 Else	  	 	
   	  RecLock('TAK',.t.)
	 EndIf	  
   
	 If aQdgDoc[i][4] == 'S'
  	  	 TAK->TAK_FILIAL := xFilial('TAK')
  	  	 TAK->TAK_CODEST := '001'
  	  	 TAK->TAK_CODNIV := cLocal
  	  	 TAK->TAK_CODFUN := aQdgDoc[i][3]  
  	  	 TAK->TAK_MAT    := aQdgDoc[i][5]  
    Else
	  	 If lDelete  
	   	 DbDelete()	
	    EndIf	 
	 EndIf	 	   
    MsUnLock('TAK')
Next i

Return .t.

