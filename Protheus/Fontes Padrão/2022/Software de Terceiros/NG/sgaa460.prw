Function SG460WHEN(cCampo)
Local lRet := .T.

If cCampo == "TD1_TIPFON"
	If M->TD1_ESCOPO == "1"
		If Empty(M->TD1_TIPFON)
			M->TD1_TIPFON := "1"
		Endif
		lRet := .T.
	ElseIf M->TD1_ESCOPO == "2"
		M->TD1_TIPFON := "6"
		lRet := .F.
	Else
		lRet := .F.
		M->TD1_TIPFON := Space(1)
	Endif
Endif

Return lRet

