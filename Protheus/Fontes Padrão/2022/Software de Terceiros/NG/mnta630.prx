If !MNA630VH(cID)
	Return .F.
EndIf

If !Empty(M->TQL_HRINIC) .And. !Empty(M->TQL_HRFIM)


		If !MNTQUETTV('Q')
			Return .f.
		Endif
		If MNTQUETTV('V') //Verifica se tem virada
			Return .t.
		Endif


	If lRETVH .AND. cHoraI >= cHoraF
		HELP(" ",1,STR0007,,STR0009,3,1) //"ATENÇÃO"###"Hora Final não pode ser menor ou igual à Hora Inicial."
		lRETVH := .F.
	Endif

EndIf

Return lRETVH
