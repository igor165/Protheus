Static Function NGCALMED(nQUANTID)

	Local nConMedia

	If nCONTACUM > 0
		If MV_PAR03 = 1
			nConMedia := nCONTAD/nQUANTID
		Else
			nConMedia := nQUANTID/nCONTAD
		EndIf
		Return Transform(nConMedia,"@E 9999,999.99")
	EndIf

Return Space(8) + "N/D"
