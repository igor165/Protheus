	Local aNGBEGINPRM := NGBEGINPRM()
	Local oReport
	Local aArea := GetArea()

	Private cTRB  := GetNextAlias()
	Private cPerg := PadR( "MNT290", Len(Posicione("SX1", 1, "MNT290", "X1_GRUPO")) )
	Private lPRI1 := .T., lPRI2 := .T.
