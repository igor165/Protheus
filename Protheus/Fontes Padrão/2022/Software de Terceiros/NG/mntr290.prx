Static Function ReportPrint(oReport)

	Local oSection1 := oReport:Section(1)
	Local oARQTR290

	Private aVETINR := {}

	oReport:SetLandScape()

	aDBFR290 := {{"CODBEM" ,"C",16,0},;
				 {"FAMILIA","C",06,0},;
				 {"DATAINI","D",08,0},;
				 {"DATAFIM","D",08,0},;
				 {"HORA"   ,"C",05,0},;
				 {"CONTAD1","N",09,0},;
				 {"CONTAC1","N",12,0},;
				 {"CONTAD2","N",09,0},;
				 {"CONTAC2","N",12,0},;
				 {"QUANTID","N",09,2},;
				 {"UNIDADE","C",03,0}}

	//Cria Tabela Temporária
	oARQTR290 := NGFwTmpTbl(cTRB,aDBFR290,{{"CODBEM","DATAINI","HORA"}})

	Processa({|lEND| MTR290TJTS()},STR0010)

	DbSelectArea(cTRB)
	DbGotop()
	oReport:SetMeter(RecCount())
	oSection1:Init()
	While !Eof() .And. !oReport:Cancel()

		lPRI1 := .T.
		lPRI2 := .T.

		cCODBIM := Space(16)
		cCODBEM := (cTRB)->CODBEM
		DbSelectArea(cTRB)
		While !Eof() .And. !oReport:Cancel() .And. (cTRB)->CODBEM == cCODBEM

			oReport:IncMeter()
			If cCODBIM == cCODBEM
				oSection1:Cell("(cTRB)->CODBEM"):Hide()
				oSection1:Cell("T9_NOME"):Hide()
				oSection1:Cell("(cTRB)->FAMILIA"):Hide()
			Else
				oSection1:Cell("(cTRB)->CODBEM"):Show()
				oSection1:Cell("T9_NOME"):Show()
				oSection1:Cell("(cTRB)->FAMILIA"):Show()
			EndIf

			cCODBIM := (cTRB)->CODBEM
			oSection1:PrintLine()

			nCONTACUM := (cTRB)->CONTAC1
			DbSelectArea(cTRB)
			Dbskip()
		End
		oSection1:Cell("(cTRB)->CODBEM"):Show()
		oSection1:Cell("T9_NOME"):Show()
		oSection1:Cell("(cTRB)->FAMILIA"):Show()
	End
	oSection1:Finish()

	//Deleta o arquivo temporario fisicamente
	oArqTR290:Delete()

Return .T.
