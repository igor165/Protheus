/*/
Function MDT750AREA(cTLN_AREA)
Local aArea := GetArea()

lRet := ExistCpo('TLS',cTLN_AREA)
If lRet
	lRet := ExistChav('TLN',M->TLN_CC+cTLN_AREA)
	If lRet
		dbSelectArea("TLS")
		dbSetOrder(1)
		If dbSeek(xFilial("TLS")+cTLN_AREA)
			M->TLN_MEMO1 := MSMM(TLS->TLS_CODMEM,80)
		Endif
	Endif
Endif

RestArea(aArea)
Return lRet