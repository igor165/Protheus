Function MNTA255() 

Local cORDEMTQA, cPLANOTQA
Local lOk := .T.

If "A" $ Alltrim(SuperGetMv("MV_NGLANEX",.F.,"-1"))
	MsgStop(STR0004,STR0003)//"Parâmetro MV_NGLANEX restringe lançamentos do tipo Contador."
	lOk := .F.
EndIf

If lOk .And. Pergunte("MNT255",.T.)
   cORDEMTQA := MV_PAR01
   
   dbSelectArea("TQA")
   dbsetorder(01)
   If dbSeek(xFilial("TQA")+cORDEMTQA) 
      
      cPLANOTQA := TQA->TQA_PLANO
      
      //Exclui os lancamentos de contador 1 e 2
      If !Empty(TQA->TQA_HORAC1) .And. TQA->TQA_POSCON > 0 
         MNT470EXCO(TQA->TQA_CODBEM,TQA->TQA_DTLEI1,TQA->TQA_HORAC1,1)
      EndIf 
        
      If !Empty(TQA->TQA_HORAC2) .And. TQA->TQA_POSCO2 > 0 
         MNT470EXCO(TQA->TQA_CODBEM,TQA->TQA_DTLEI2,TQA->TQA_HORAC2,2)
      EndIf 
         
      //Reabre o plano
      dbSelectArea("STO")
      dbSetOrder(01)  
      If dbSeek(xFilial("STO")+cPLANOTQA) .And. STO->TO_TERMINO == "S"
         RecLock("STO",.F.)
         STO->TO_TERMINO := "N"
         STO->TO_USUARIO := " "
         STO->(MsUnlock())
      EndIf 
                  
      dbSelectArea("TQA")
      RecLock("TQA",.F.)
      TQA->TQA_SITUAC := "L"  
      TQA->TQA_TERMIN := "N"
      TQA->TQA_POSCON := 0
      TQA->TQA_DTLEI1 := CTOD("  /  /  ")
      TQA->TQA_HORAC1 := "  :  "
      TQA->TQA_POSCO2 := 0
      TQA->TQA_DTLEI2 := CTOD("  /  /  ")
      TQA->TQA_HORAC2 := "  :  "
      TQA->TQA_RETORN := "N"
      TQA->(MsUnlock())
   EndIf    
EndIf

Return Nil  

