Function MNTA460EXC(cAlias,nReg,nOpcx)
DbSelectArea("STJ")
nInd460STJ := IndexOrd()

If STJ->TJ_ORDEM == STJ->TJ_ORDEPAI .and. nREGISTR > 1
   Help(" ",1,"NGMOSRNEXC")
   Return
EndIf

nRET := NG400EXC(cAlias,nReg,nOpcx)

If nRET = 2
   nREGISTR := nREGISTR - 1
Endif

DbSelectArea("STJ")
If Type("aInd460R") = "A"
   bFiltraBrw := {|| FilBrowse("STJ",@aInd460R,@ccond460R) }
   Eval(bFiltraBrw)
Else
   DbSetOrder(nInd460STJ)
   lREFRESH := .T.
EndIf

Return Nil

