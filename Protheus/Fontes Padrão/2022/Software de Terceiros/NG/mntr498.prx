FUNCTION MNTR498()
LOCAL WNREL   := "MNTR498"
LOCAL LIMITE  := 132
LOCAL cDESC1  := STR0001 //"Relatorio de analise de similaridade de pneus para formacao de pares."
LOCAL cDESC2  := " "
LOCAL cDESC3  := " "
LOCAL cSTRING := "ST9"

PRIVATE NOMEPROG := "MNTR498"
PRIVATE TAMANHO  := "M"
