				TNC->TNC_CLIENT+TNC->TNC_LOJA } )
			Endif
			
			DbSelectArea("TNC")
			DbSkip()
		End
		aSort(aDadosTNC,,,{|x,y| x[2]+x[6] < y[2]+y[6] })
		
		For nXX := 1 To Len(aDadosTNC)
