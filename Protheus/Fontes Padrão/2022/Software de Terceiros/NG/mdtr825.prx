			cCodInss := aDadosTNC[nXX,2]
			If !Empty(cCodInss)
				nRegValido := nXX
				For nYY := nXX To Len(aDadosTNC)
					If cCodInss == aDadosTNC[nYY,2]
						nRegValido := nYY
