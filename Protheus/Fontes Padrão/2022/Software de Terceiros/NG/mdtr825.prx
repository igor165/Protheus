/*/   
Static Function MDT825GRAVA(cTNC_CC,cTMT_CID)

DbSelectArea("TRBA")
If DbSeek(&cSeek1)
	RecLock("TRBA",.f.)
	TRBA->QTOBITOS   += nObito
	TRBA->QTCASOS    += nCasos
	TRBA->QTTRANSF   += nTrans
	TRBA->QTINCAPA   += nIncap
	MsUnlock()
Else
	TRBA->(DbAppend())
	If lSigaMdtps
		TRBA->CLIENTE    := Substr(cTNC_CC,1,nSizeTD)
	Else
		TRBA->CLIENTE    := Substr(cTNC_CC,1,nTa1)
	Endif
	TRBA->CODCID		:= cTMT_CID
	TRBA->QTOBITOS   := nObito
	TRBA->QTCASOS    := nCasos
	TRBA->QTTRANSF   := nTrans
	TRBA->QTINCAPA   := nIncap
EndIf

DbSelectArea("TRBC")
If DbSeek(&cSeek2)
	RecLock("TRBC",.f.)
	TRBC->QTOCOR  += 1
	MsUnlock()
Else
	TRBC->(DbAppend())
	If lSigaMdtps
		TRBC->CLIENTE  := Substr(cTNC_CC,1,nSizeTD)
	Else
		TRBC->CLIENTE  := Substr(cTNC_CC,1,nTa1)
	Endif
	TRBC->CODCID	  := cTMT_CID
	TRBC->CCUSTO   := cTNC_CC
	TRBC->QTOCOR   := 1
EndIf

Return  .t.
