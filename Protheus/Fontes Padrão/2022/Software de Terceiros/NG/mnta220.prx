      oPrint:Say(nL08,nCOLP2  ,aARQTQ1[xl,nESQ5],oFont11)
      nUTLIN := Max(nUTLIN,1450)

   ElseIf aARQTQ1[xl][1] = 6
      MNTA220BX(oPrint,@i,@lin,nL03-20,85,200,3)
