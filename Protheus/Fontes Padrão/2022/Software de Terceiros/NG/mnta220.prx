/*/
Static Function MNT220CSX3(xz,lINCL)

	Local nCSX3   := 0
	Local nTamTot := 0
	Local nInd    := 0
	Local nTam    := 0
	Local aStruct := {}
	Local cCampo  := ""
	Local cUso    := ""
	Local cX3NVL  := ""
	Local cTipo   := ""

	If !lINCL
		nCSX3 := If(lGFrota,3,2)
	EndIf

	dbSelectArea("TQ1")
	aStruct := DBSTRUCT()
	nTamTot := Len(aStruct)

	For nInd := 1 To nTamTot

		cCampo := aStruct[nInd,1]
		cUso   := Posicione("SX3",2,cCampo,"X3_USADO")
		cX3NVL := Posicione("SX3",2,cCampo,"X3_NIVEL")
		nTam   := TAMSX3(cCampo)[1]
		cTipo  := aStruct[nInd,2]

		If fLimpaCpo(cUso, cCampo, cX3NVL, lINCL)
			nCSX3++
			If cTipo == "C"
				aCOLS[xz][nCSX3] := Space(nTam)
			ElseIf cTipo == "N"
				aCOLS[xz][nCSX3] := 0
			ElseIf cTipo == "D"
				aCOLS[xz][nCSX3] := dDataBase
			ElseIf cTipo == "M"
				aCOLS[xz][nCSX3] := ""
			Else
				aCOLS[xz][nCSX3] := .F.
			EndIf
		Endif

		dbSkip()
	End

	nCSX3              := Len(aCols[xz])-2
	aCols[xz][nCSX3]   := "TQ1"
	aCols[xz][nCSX3+1] := TQ1->(Recno())
	aCOLS[xz][nCSX3+2] := .F.

Return .T.

