      nUTLIN := Max(nUTLIN,2150)

   ElseIf aARQTQ1[xl][1] = 10
      MNTA220BX(oPrint,@i,@lin,nL01-20,85,200,5)
      MNTA220EI1(oPrint,@i,@lin,nL01+65,nL01+130,2,4,90,nLISOMA)
