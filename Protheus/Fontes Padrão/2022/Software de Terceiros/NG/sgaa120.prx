	If Len( cMemoFor )  >=  nPula
	//	cMemoFor += Chr(10)
		nPula += 70
	EndIf
	If cOperacao == Nil
		cMemoFor +=  '#'+cCbx1+'# '
	ElseIf cOperacao $ "1234567890"
		If SubStr(cMemoFor,Len(cMemoFor)-1,1) $ "1234567890"
