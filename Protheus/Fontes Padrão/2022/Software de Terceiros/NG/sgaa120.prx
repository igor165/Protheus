		While !Eof() .And. xFilial("TA3") == TA3->TA3_FILIAL

			aAdd(aForm,TA3->TA3_CODFOR)

			dbSelectArea("TA3")
			dbSkip()

		EndDo

		nPos := aScan(aForm,cForm)

		If nPos == 0 .Or. nPos > 5
			Return .T.
		ElseIf nPos == 1
			cField := "TAB->TAB_RESULT"
		Else
			cField := "TAB->TAB_RESUL"+cValToChar(nPos)
		EndIf

		dbSelectArea("TAB")
		dbSetOrder(1)
		dbSeek(xFilial("TAB")+Space(Len(TA3->TA3_CODFOR)),.T.)
		While !Eof() .And. xFilial("TAB") == TAB->TAB_FILIAL

			If &cField > 0
				cError :=  NGSX2NOME("TAB") + " (TAB)"
				HELP(" ",1,"NGINTMOD",,cError,5,1)
				RestArea(aArea)
				Return .F.
			EndIf

			dbSelectArea("TAB")
			dbSkip()

		EndDo

	//NGVALSX9("TA3")
		RestArea(aArea)

Return .T.

//---------------------------------------------------------------------
/*/{Protheus.doc} SgBckSpc
Exclui o ultimo termo inserido na fórmula

@param cMemoPar

@author Juliani Schlickmann Damasceno
@since 15/10/2013
@version VER110
@return Nil
/*/
//---------------------------------------------------------------------
Static Function SgBckSpc(cMemoOrg)

	Local cMemoFrm := Trim(cMemoOrg)
	Local nPosTok

	//Verifica se o último caractere da fórmula é sustenido
	If Right(cMemoFrm, 1) == "#"
		If ( nPosTok := RAT("#", SubStr(cMemoFrm,1, Len(cMemoFrm) - 1 ) ) ) > 0 //Se for vai procurar o penúltimo e apagar o conteúdo nesse meio
			cMemoFor := SubStr(cMemoFrm, 1, nPosTok - 1 )
		Endif

	//Se não entrar na primeira consição cerificará se o último caractere é um digito, se for apagará de um em um.
	ElseIf isDigit(RIGHT(cMemoFrm, 1))
		cMemoFor := SubStr(cMemoFrm,1, Len(cMemoFrm) - 1 )

	//Se o último caractere for um espaço procurará o penúltimo e retirará o conteúdo desse intervalo.
	ElseIf ( nPosTok := RAT(Space(1), SubStr(cMemoFrm,1, Len(cMemoFrm) - 1 ) ) ) > 0
		cMemoFor := SubStr(cMemoFrm, 1, nPosTok - 1)

	Else
		cMemoFor := "" //Se não for nenhuma das outras condições limpará a variável.
	Endif

Return