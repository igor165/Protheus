Function Sg120Comb1(cTipo)

	Local aCombo :={}
	DEFAULT cTipo := '2' // Até implementação dos Pesos para Aspectos e LOcalização somente mostra Criterios de Impacto

	DbSelectArea('TA6')
	DbSetOrder(1)
	DbSeek(xFilial('TA6'))
	While !Eof() .and. xFilial('TA6') == TA6->TA6_FILIAL
	If TA6->TA6_TIPO == cTipo .and. TA6->TA6_PESO > 0
		Aadd( aCombo, AllTrim(TA6->TA6_DESCRI) )
		EndIf
	TA6->(DbSkip())
	End

Return aCombo

