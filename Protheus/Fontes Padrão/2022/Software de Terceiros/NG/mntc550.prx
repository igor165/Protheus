		M->TJ_PLANO := STJ->TJ_PLANO
	ElseIf !Empty((_cTrb)->TJ_ORDEM)
		M->TJ_ORDEM := (_cTrb)->TJ_ORDEM
		M->TJ_PLANO := (_cTrb)->TJ_PLANO
	EndIf

	dbSelectArea("STN")
