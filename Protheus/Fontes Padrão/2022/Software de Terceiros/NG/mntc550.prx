		bWHILE := {|| !EoF() .And. STQ->TQ_ORDEM == M->TJ_ORDEM .And. STQ->TQ_PLANO == M->TJ_PLANO}
		bFOR   := {|| TQ_FILIAL == xFilial("STQ") .And. TQ_ORDEM == M->TJ_ORDEM .And. TQ_PLANO  == M->TJ_PLANO}

