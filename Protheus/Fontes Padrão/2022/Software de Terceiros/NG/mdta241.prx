	If ExistBlock( "MDTA2411" ) //Adicionar botões na Rotina
		If ValType( aNovBot := ExecBlock( "MDTA2411", .F., .F.) ) == "A"
			aEval( aNovBot , { | x | aAdd( aRotina , x ) } )
		EndIf
	End

EndIf

Return aRotina
