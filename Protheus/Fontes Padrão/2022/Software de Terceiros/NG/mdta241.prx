	//Monta aChoice
	aADD(aChoice,"RA_MAT")
	aADD(aChoice,"RA_NOME")
	aADD(aChoice,"RA_NASC")
	aADD(aChoice,"RA_SEXO")
	aADD(aChoice,"RA_CC")
	aADD(aChoice,"RA_DESCCC")
	aADD(aChoice,"RA_CODFUNC")
	aADD(aChoice,"RA_DESCFUN")
	aADD(aChoice,"RA_ADMISSA")

	//Monta enchoice de fornecedor
