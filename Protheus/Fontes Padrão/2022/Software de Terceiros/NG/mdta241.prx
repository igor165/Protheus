	cQuery := "SELECT * FROM "+RetSqlName("TO8")+" TO8 WHERE TO8.TO8_FILIAL = '"+xFilial("TO8")+"' AND TO8.TO8_MAT = '"+SRA->RA_MAT+"'"+ ;
