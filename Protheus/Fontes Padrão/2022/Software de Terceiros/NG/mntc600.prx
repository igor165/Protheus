	DbSelectArea("ST5")
	Dbgobottom()
	Dbskip()

	If Empty(aCOLS) .OR. nOPCAO == 3
		aCOLS := BLANKGETD(aHEADER)
	Else
		aSt5Tar := aClone(aCols)
