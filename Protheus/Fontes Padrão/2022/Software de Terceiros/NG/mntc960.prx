				 {STR0003 ,"NGCAD01"  , 0 , 2}}  //"Visualizar"

	cCadastro := OemToAnsi(STR0018)   //"Custo MDO do Servico"

	aDBF2 := {{"CUSTO"    ,"C",Len(stj->tj_ccusto),0},;
			  {"VLRCUSTO" ,"N",12                 ,2},;
			  {"PERCENTO" ,"N",06                 ,2}}

	//Intancia classe FWTemporaryTable
