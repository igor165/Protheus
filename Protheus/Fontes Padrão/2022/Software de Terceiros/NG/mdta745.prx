	cFiltroTLR += " AND TLR_CC = '"+TLL->TLL_CC+"'"
	TLR->(dbGoTop())
	mBrowse( 6, 1,22,75,"TLR",,,,,,,,,,,,,,cFiltroTLR)
	
End Sequence

RestArea(aAreaTLL)
RestArea(aArea)
aRotina := aClone(_oldRotina)
Return

