/*/
Function MDT745VEQP(cTLR_CODEQU)
Local aArea := GetArea()

dbSelectArea("TNH")
dbSetOrder(1)
lRet := dbSeek(xFilial("TNH")+cTLR_CODEQU)
If lRet .and. NGCADICBASE("TNH_TIPOBJ","A","TNH",.F.)
	lRet := If( TNH->TNH_TIPOBJ <> "2" , .T. , .F. )
Endif
If lRet
	lRet := ExistChav("TLR",M->TLR_CC+cTLR_CODEQU)
Else
	Help(" ",1,"REGNOIS")
Endif

RestArea(aArea)
Return lRet