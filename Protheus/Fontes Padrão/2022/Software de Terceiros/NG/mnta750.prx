Function Mnt750Chk()
Local cMsgCiclo   := AllTrim(M->TR9_CICLO)
Local cMsgVeiculo := AllTrim(M->TR9_FROTA)
Local lEnd := .f.

If Empty(M->TR9_ORDEM)
	Help(" ",1,"OBRIGAT")
	Return .f.
EndIf

If !MsgYesNo(STR0008+cMsgCiclo+STR0009; //"Deseja iniciar a inspeção do "###" ciclo para o veículo "
   				+cMsgVeiculo,STR0010)   //"ATENÇÃO"
	Return .f.
EndIf
If MNTOPEN232(M->TR9_FROTA) .And.  NGCADICBASE("TR9_NUMCAL","D","TR9",.F.)
    Processa({ |lEnd| MNTA995Gra(,,1,M->TR9_FROTA, M->TR9_CICLO, M->TR9_ORDEM)}, STR0011)//"Processando inspeção" (Modo Gráfico)
Else
    Processa({ |lEnd| Mnt750Pro()}, STR0011)//"Processando inspeção"
EndIf
Return .t.

