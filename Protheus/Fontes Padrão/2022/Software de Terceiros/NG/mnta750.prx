/*/{Protheus.doc} PrepGraf
Prepara a rotina para o modo gráfico inicializando variáveis do modelo antigo

@author William Rozin Gaspar
@since 15/06/2014
@return Nil
/*/
//---------------------------------------------------------------------

Static Function PrepGraf()

Local nCnt
//Private cHora    := Time()
//Private nPoscont := 0
//Private dDtInsp := dDataBase,dDtDigit := dDataBase


Dbselectarea("TRC")
Dbsetorder(1)

aHeader := CabecGetD("TRC",{"TRC_ORDEM","TRC_PLANO","TRC_CICLO"})
ADHeadRec("TRC",aHeader)

nCnt := 0
DbSelectArea("TQ1")
DbSetOrder(1)
DbSeek(xFilial("TQ1")+M->TR9_CODFAM+M->TR9_TIPMOD)
While !Eof() .And. TQ1->TQ1_FILIAL == xFILIAL("TQ1");
    .And. TQ1->TQ1_DESENH == M->TR9_CODFAM;
    .And. TQ1->TQ1_TIPMOD == M->TR9_TIPMOD

    aAdd(aCols,{TQ1->TQ1_EIXO,;//Código do Eixo
                TQ1->TQ1_QTDPNE,;//Quantidade de Pneus
                Space(16),;//Número do Fogo 1
                TQ1->TQ1_LOCPN1,;//Localização 1
                0,;//Medida 1 - Pneu 1
                0,;//Medida 2 - Pneu 1
                0,;//Medida 3 - Pneu 1
                Space(16),;//Número do Fogo 2
                TQ1->TQ1_LOCPN2,;//Localização 2
                0,;//Medida 1 - Pneu 2
                0,;//Medida 2 - Pneu 2
                0,;//Medida 3 - Pneu 2
                Space(16),;//Número do Fogo 3
                TQ1->TQ1_LOCPN3,;//Localização 3
                0,;//Medida 1 - Pneu 3
                0,;//Medida 2 - Pneu 3
                0,;//Medida 3 - Pneu 3
                Space(16),;//Número do Fogo 4
                TQ1->TQ1_LOCPN4,;//Localização 4
                0,;//Medida 1 - Pneu 4
                0,;//Medida 2 - Pneu 4
                0,;//Medida 3 - Pneu 4
                Space(16),;//Número do Fogo 5
                TQ1->TQ1_LOCPN5,;//Localização 5
                0,;//Medida 1 - Pneu 5
                0,;//Medida 2 - Pneu 5
                0,;//Medida 3 - Pneu 5
                Space(16),;//Número do Fogo 6
                TQ1->TQ1_LOCPN6,;//Localização 6
                0,;//Medida 1 - Pneu 6
                0,;//Medida 2 - Pneu 6
                0,;//Medida 3 - Pneu 6
                Space(16),;//Número do Fogo 7
                TQ1->TQ1_LOCPN7,;//Localização 7
                0,;//Medida 1 - Pneu 7
                0,;//Medida 2 - Pneu 7
                0,;//Medida 3 - Pneu 7
                Space(16),;//Número do Fogo 8
                TQ1->TQ1_LOCPN8,;//Localização 8
                0,;//Medida 1 - Pneu 8
                0,;//Medida 2 - Pneu 8
                0,;//Medida 3 - Pneu 8
                Space(16),;//Número do Fogo 9
                TQ1->TQ1_LOCPN9,;//Localização 9
                0,;//Medida 1 - Pneu 9
                0,;//Medida 2 - Pneu 9
                0,;//Medida 3 - Pneu 9
                Space(16),;//Número do Fogo 10
                TQ1->TQ1_LOCPN0,;//Localização 10
                0,;//Medida 1 - Pneu 10
                0,;//Medida 2 - Pneu 10
                0,;//Medida 3 - Pneu 10
                "TRC",;//Alias
                TRC->(Recno()),;//Recno TRC
                .F.})//Não deletada
    nCnt++

    Dbselectarea("TQ1")
    DbSkip()
End

If Empty(aCOLS)//Caso aCols fique vazio, adiciona linha em branco
   aCols := BlankGetD(aHeader)
Endif
Return
