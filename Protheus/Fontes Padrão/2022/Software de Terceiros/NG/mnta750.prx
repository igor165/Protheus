Function Mnt750Cic()
nCiclo := 0
cAliasQry := GetNextAlias()

cQuery := " SELECT MAX(TR9_CICLO) CICLO "
cQuery += " FROM " + RetSQLName("TR9")
cQuery += " WHERE D_E_L_E_T_<>'*' and "
cQuery += " TR9_ORDEM = '"+M->TR9_ORDEM+"' and "
cQuery += " TR9_PLANO = '"+STJ->TJ_PLANO+"'"

