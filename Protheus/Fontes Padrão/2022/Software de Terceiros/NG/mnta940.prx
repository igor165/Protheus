/*/
Function ATUSTJ940()
Local lCustFer  := NGCADICBASE("TJ_CUSTFER","A","STJ",.F.)
Local nMDO,nMAT,nMAA,nMAS,nTER,nFER, nVALOR, nSEQUE := "stl->tl_sequenc"
DbSelectArea("SIX")
DbSetOrder(1)
If Dbseek("STL1")
   If "TL_SEQRELA"$SIX->CHAVE
       nSEQUE := "Val(stl->tl_seqrela)"
   Endif    
Endif

DbSelectArea("STJ")
DbSetOrder(1)
Dbseek(xFilial("STJ"))
ProcRegua(LastRec())

While !EOF() .and. STJ->TJ_FILIAL == xFILIAL("STJ")
   IncProc()

   If STJ->TJ_SITUACA = "L"
      Store 0.00 To nMDO,nMAT,nMAA,nMAS,nTER,nFER
      DbSelectArea("STL")
      Dbsetorder(1)
      DbSeek(xFilial("STL")+STJ->TJ_ORDEM+STJ->TJ_PLANO)
      While !EOF() .and. STL->TL_FILIAL == xFILIAL("STL") .and. ;
         STL->TL_ORDEM == STJ->TJ_ORDEM .and. STL->TL_PLANO == STJ->TJ_PLANO

         If &nSEQUE >  0
            nValor := stl->tl_custo
            If !Empty(stl->tl_numseq)
               DbselectArea('SD3')
               DbsetOrder(4)
               If Dbseek(xFILIAL("SD3")+stl->tl_numseq)
                  cOPSTL := Alltrim(stl->tl_ordem+"OS001")
                  If Alltrim(sd3->d3_op) = cOPSTL
                     If stl->tl_custo <> sd3->d3_custo1
                        DbselectArea('STL')
                        RecLock("STL",.F.)
                        stl->tl_custo := sd3->d3_custo1
                        MsUnlock("STL")
                        nValor := stl->tl_custo
                     Endif
                  Endif
               Endif
            Endif
            If STL->TL_TIPOREG == "T"
               nTER += nVALOR
            ElseIf STL->TL_TIPOREG == "M"
               nMDO += nVALOR
            ElseIf STL->TL_TIPOREG == "E"
               nMDO += nVALOR
            ElseIf STL->TL_TIPOREG == "P"
               If STL->TL_DESTINO == "T"
                  nMAT += nVALOR
               ElseIf STL->TL_DESTINO == "S"
                  nMAS += nVALOR
               Else
                  nMAA += nVALOR
               Endif
            ElseIf STL->TL_TIPOREG == "F"
            	nFER += nVALOR
            Endif
         Endif

         DbSelectArea("STL")
         DbSkip()
      End

      If STJ->TJ_TERMINO = "S"
         DbSelectArea("STJ")
         Reclock("STJ",.F.)
         STJ->TJ_CUSTMDO := nMDO
         STJ->TJ_CUSTMAT := nMAT
         STJ->TJ_CUSTMAA := nMAA
         STJ->TJ_CUSTMAS := nMAS
         STJ->TJ_CUSTTER := nTER
         If lCustFer
				STJ->TJ_CUSTFER := nFER
         EndIf
         MSUNLOCK("STJ")
      Endif
   Endif
   DbSelectArea("STJ")
   DbSkip()
End
DbSelectArea("STJ")
DbSetOrder(1)
DbSeek(xFIlial("STJ"))
Return nil