		Somalin()
		@ Li,000 Psay STR0015+" "+Alltrim(TP3->TP3_FORNEC)+" - "+;
		NGSEEK('SA2',TP3->TP3_FORNEC+TP3->TP3_LOJA,1,'A2_NOME') //"Fornecedor: "
