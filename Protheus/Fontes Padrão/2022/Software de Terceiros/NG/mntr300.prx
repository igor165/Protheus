			nTOTGMDOTER := nTOTGMDOTER+nTOTMDOTER
			nTOTGMATPRO := nTOTGMATPRO+nTOTMATPRO
			nTOTGMATTER := nTOTGMATTER+nTOTMATTER
			nTOTGOLEO   := nTOTGOLEO  +nTOTOLEO
			nTOTGVALR   := nTOTGVALR  +nTOTVALR
