Static Function NGRETVAL(nCAM)

	Local nVALORCAM := 0

	If nCAM = 1
		nVALORCAM := nREFORM
	ElseIf nCAM = 2
		nVALORCAM := nMDOPRO
	ElseIf nCAM = 3
		nVALORCAM := nMDOTER
	ElseIf nCAM = 4
		nVALORCAM := nMATPRO
	ElseIf nCAM = 5
		nVALORCAM := nMATTER
	ElseIf nCAM = 6
		nVALORCAM := nOLEO
	ElseIf nCAM = 7
		nVALORCAM := nVALR
	Else
		nVALORCAM := nTOTMOE
	EndIf
Return nVALORCAM

