				If (cTRB)->ANO == AllTrim(Str(nAno1))
					If MV_PAR09 == 2
						aDados[nx][6] := aDados[nx][3]  //total qtd
					Else
						aDados[nx][6] := aDados[nx][4]  //total custo
					EndIf
				EndIf
			EndIf
		EndIf

		DbSelectArea(cTRB)
		DbSetOrder(2)
		DbSkip()
	End
	RestArea(aArea)
Return .t.
