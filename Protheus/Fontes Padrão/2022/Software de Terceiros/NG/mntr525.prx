/*/
Function MNR525PROC()
	Local nCusto 	:= 0
	Local cNGSEREF	:= GetMv("MV_NGSEREF")

	nAnoAtu := Val(Substr(DtoS(dDataBase),1,4))
	dDtMax := CtoD('31/12/'+AllTrim(Str(nAnoAtu + 2)))
	nProxMes := Month(dDataBase)+1
	nAno := nAnoAtu
	If nProxMes > 12
		nProxMes := nProxMes - 12
		nAno := nAno + 1
	EndIf
	dDtMin  := CtoD('01/'+StrZero(nProxMes,2)+'/'+AllTrim(Str(nAno)))

	cAliasQry := GetNextAlias()

	cQuery := " SELECT tqs.tqs_filial,tqs.tqs_codbem,tqs.tqs_medida,tqs.tqs_desenh,tqs.tqs_numfog,tqs.tqs_sulcat,"
	cQuery += " tqs.tqs_bandaa,tqs.tqs_kmor,tqs.tqs_kmr1,tqs.tqs_kmr2,tqs.tqs_kmr3,tqs.tqs_kmr4,tqs.tqs_dot,"
	cQuery += " tqs.tqs_eixo,tqs.tqs_tipeix,tqs.tqs_posic,tqs.tqs_eixo,st9.t9_codesto,st9.t9_valcpa,st9.t9_codfami,"
	cQuery += " (SELECT TQT_DESMED FROM "+RetSqlName("TQT")+" TQT WHERE TQT.TQT_MEDIDA = TQS.TQS_MEDIDA AND " + NGMODCOMP("TQT","TQS") + "AND TQT.d_e_l_e_t_ <> '*')DESMED,"
	cQuery += " st9.t9_tipmod,(SELECT TQR_DESMOD FROM "+RetSqlName("TQR")+" TQR WHERE TQR.TQR_TIPMOD = ST9.T9_TIPMOD AND " + NGMODCOMP("TQR","ST9") + " AND TQR.d_e_l_e_t_ <> '*')DESMOD,"
	cQuery += " st9.t9_fabrica,st9.t9_nome,st9.t9_dtultac,st9.t9_poscont,st9.t9_contacu,st9.t9_vardia,"
	cQuery += " tqx.tqx_kmespo,tqx.tqx_sulcoo,tqx.tqx_kmespo,tqu.tqu_sulco,tqu.tqu_kmesp1,tqu.tqu_kmesp2,tqu.tqu_kmesp3,tqu.tqu_kmesp4 "
	cQuery += " FROM "+RetSqlName("TQS")+" TQS,"+RetSqlName("ST9")+" ST9,"
	cQuery +=   RetSqlName("TQX")+" TQX,"+RetSqlName("TQU")+" TQU "
	cQuery += " WHERE tqs.tqs_codbem = st9.t9_codbem "
	cQuery += " AND st9.t9_sitbem = 'A' "
	cQuery += " AND st9.t9_status = '"+GetMv("MV_NGSTAPL")+"' "
	cQuery += " AND st9.t9_codfami BETWEEN '"+MV_PAR01+"' AND '"+MV_PAR02+"' "
	cQuery += " AND st9.t9_tipmod BETWEEN '"+MV_PAR03+"' AND '"+MV_PAR04+"' "
	cQuery += " AND tqs.tqs_medida BETWEEN '"+MV_PAR05+"' AND '"+MV_PAR06+"' "
	cQuery += " AND tqs.tqs_bandaa <= '"+AllTrim(Str(MV_PAR08))+"'  "
	cQuery += " AND tqs.tqs_medida = tqx.tqx_medida "
	cQuery += " AND st9.t9_tipmod = tqx.tqx_tipmod "
	cQuery += " AND tqs.tqs_desenh = tqu.tqu_desenh "
	cQuery += " AND tqs.d_e_l_e_t_ <> '*' AND st9.d_e_l_e_t_ <> '*' "
	cQuery += " AND tqx.d_e_l_e_t_ <> '*' AND tqu.d_e_l_e_t_ <> '*' "
	cQuery := ChangeQuery(cQuery)
	dbUseArea( .T., "TOPCONN", TCGENQRY(,,cQuery),cAliasQry, .F., .T.)
	/*
	LEFT JOIN SZ1010 SZ1 ON sz1.z1_films=tqs.tqs_filial and sz1.d_e_l_e_t_<>'*'
	LEFT JOIN zzh010 zzh ON zzh.zzh_hub=sz1.z1_hub and zzh.d_e_l_e_t_<>'*',
	*/
	ProcRegua(LastRec())

	While !Eof()
		IncProc(STR0017,STR0018)       //"Aguarde..."###"Processando Registros..."

		DbSelectArea(cTRB)
		RecLock((cTRB), .T.)
		(cTRB)->FILIAL  := (cAliasQry)->TQS_FILIAL
		DbSelectArea("TSL")
		DbSetOrder(1)
		If DbSeek(xFilial("TSL")+(cTRB)->FILIAL)
			(cTRB)->CODHUB := TSL->TSL_HUB
		EndIf
		(cTRB)->FAMILIA := (cAliasQry)->T9_CODFAMI
		(cTRB)->MODELO  := (cAliasQry)->T9_TIPMOD
		(cTRB)->DESMOD  := (cAliasQry)->DESMOD
		(cTRB)->MEDIDA  := (cAliasQry)->TQS_MEDIDA
		(cTRB)->DESMED  := (cAliasQry)->DESMED
		(cTRB)->PNEU    := (cAliasQry)->TQS_CODBEM
		(cTRB)->NUMFOG  := (cAliasQry)->TQS_NUMFOG
		(cTRB)->DOT     := (cAliasQry)->TQS_DOT
		(cTRB)->SULCO   := (cAliasQry)->TQS_SULCAT
		(cTRB)->RENDOR  := (cAliasQry)->TQX_KMESPO / (cAliasQry)->TQX_SULCOO
		If (cAliasQry)->TQS_BANDAA = '1'
			(cTRB)->RENDREF := 0
			(cTRB)->VIDA   := "OR"
		ElseIf (cAliasQry)->TQS_BANDAA = '2'
			(cTRB)->RENDREF := ((cAliasQry)->TQX_KMESPO * ((cAliasQry)->TQU_KMESP1/100)) /	(cAliasQry)->TQU_SULCO
			(cTRB)->VIDA   := "R1"
		ElseIf (cAliasQry)->TQS_BANDAA = '3'
			(cTRB)->RENDREF := ((cAliasQry)->TQX_KMESPO * ((cAliasQry)->TQU_KMESP2/100)) /	(cAliasQry)->TQU_SULCO
			(cTRB)->VIDA   := "R2"
		ElseIf (cAliasQry)->TQS_BANDAA = '4'
			(cTRB)->RENDREF := ((cAliasQry)->TQX_KMESPO * ((cAliasQry)->TQU_KMESP3/100)) /	(cAliasQry)->TQU_SULCO
			(cTRB)->VIDA   := "R3"
		ElseIf (cAliasQry)->TQS_BANDAA = '5'
			(cTRB)->RENDREF := ((cAliasQry)->TQX_KMESPO * ((cAliasQry)->TQU_KMESP4/100)) /	(cAliasQry)->TQU_SULCO
			(cTRB)->VIDA   := "R4"
		EndIf
		(cTRB)->PREVROD := ((cAliasQry)->TQS_SULCAT - MV_PAR07) * If((cAliasQry)->TQS_BANDAA = '1',(cTRB)->RENDOR,(cTRB)->RENDREF)
		(cTRB)->VARDIA  := (cAliasQry)->T9_VARDIA
		(cTRB)->QTDDUR  := Round((((cTRB)->PREVROD / (cTRB)->VARDIA) / 30) ,2)
		(cTRB)->QTDARR  := If((cTRB)->QTDDUR < 1 .and. (cTRB)->QTDDUR > 0 ,1,Round(((cTRB)->QTDDUR),0))
		(cTRB)->DTTROCA := dDataBase + (30 * (cTRB)->QTDARR)
		(cTRB)->ANO := SubStr(DtoS((cTRB)->DTTROCA),1,4)
		cMES := SubStr(DtoS((cTRB)->DTTROCA),5,2)

		Do Case
			Case cMes = '01'
				(cTRB)->MES := STR0019 //"Janeiro"
			Case cMes = '02'
				(cTRB)->MES := STR0020 //"Fevereiro"
			Case cMes = '03'
				(cTRB)->MES := STR0021 //"MarÁo"
			Case cMes = '04'
				(cTRB)->MES := STR0022 //"Abril"
			Case cMes = '05'
				(cTRB)->MES := STR0023 //"Maio"
			Case cMes = '06'
				(cTRB)->MES := STR0024 //"Junho"
			Case cMes = '07'
				(cTRB)->MES := STR0025 //"Julho"
			Case cMes = '08'
				(cTRB)->MES := STR0026 //"Agosto"
			Case cMes = '09'
				(cTRB)->MES := STR0027 //"Setembro"
			Case cMes = '10'
				(cTRB)->MES := STR0028	//"Outubro"
			Case cMes = '11'
				(cTRB)->MES := STR0029 //"Novembro"
			Case cMes = '12'
				(cTRB)->MES := STR0030 //"Dezembro"
		End Do

		If ((cAliasQry)->TQS_BANDAA == '1' .OR. (cAliasQry)->TQS_BANDAA == AllTrim(STR(MV_PAR08))) .AND. (cAliasQry)->TQS_EIXO == '1'
			If !Empty((cAliasQry)->T9_CODESTO)
				DbSelectArea("SB2")
				DbSetOrder(1)
				If DbSeek(xFilial("SB2")+(cAliasQry)->T9_CODESTO)
					nCusto := SB2->B2_CM1
				EndIf
			Else
				nCusto := (cAliasQry)->T9_VALCPA
			EndIf
		Else
			cAliasQry2 := GetNextAlias()
			cQuery2 := " SELECT * FROM "+RetSqlName("TRA")
			cQuery2 += " WHERE TRA_SERVIC = '"+cNGSEREF+"' "
			cQuery2 += " AND TRA_MEDIDA = '"+(cAliasQry)->TQS_MEDIDA+"' AND "
			cQuery2 += " D_E_L_E_T_ <> '*' "
			cQuery2 += " ORDER BY TRA_BANDA "
			cQuery2 := ChangeQuery(cQuery2)
			dbUseArea( .T., "TOPCONN", TCGENQRY(,,cQuery2),cAliasQry2, .F., .T.)

			lCusto := .f.
			While !Eof()
				If Val((cAliasQry)->TQS_BANDAA) + 1 <= Val((cAliasQry2)->TRA_BANDA) .And. !lCusto
					nCusto := (cAliasQry2)->TRA_CUSTO
					lCusto := .T.
				EndIf
				If !lCusto
					nCusto := (cAliasQry2)->TRA_CUSTO
				EndIf
				DbSelectArea(cAliasQry2)
				DbSkip()
			End
			(cAliasQry2)->(DbCloseArea())
		EndIf
		(cTRB)->CUSTOTR := nCusto
		nCusto := 0
		(cTRB)->(MsUnLock())

		//deleta as trocas superiores ao final dos 2 proximos anos
		If (cTRB)->DTTROCA > dDtMax .Or. (cTRB)->DTTROCA < dDtMin
			RecLock((cTRB),.F.)
			dbDelete()
			(cTRB)->(MsUnLock())
		EndIf
		DbSelectArea(cAliasQry)
		DbSkip()
	End
	(cALIASQRY)->(dbCloseArea())

	DbSelectArea(cTRB)
	DbGoTop()
	If Eof()
		MsgInfo(STR0031,"ATEN«√O")  //"N„o existe dados para montar o relatÛrio."
		lImpRel := .F.
	EndIf

Return lImpRel
