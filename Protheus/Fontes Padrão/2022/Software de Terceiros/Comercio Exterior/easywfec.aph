<%
   Private oWorkFlow := EasyIniWF()
	if Valtype(cVrs) == "C"
		cVrs := "2"
	EndIf
%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
	"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
<head>
    <meta http-equiv='Content-Type' content='text/html; charset=iso-8859-1'>
    <title><%= oWorkFlow:RetDesc() %></title>
    <STYLE TYPE="text/css">
        <!-
        body {
        color: #000000;
        background-color: #FFFFFF;
        }
        td{
        width:50%;
        }
        h1 {
        font-family: arial;
        font-size: 19pt;
        color: #00008B;
        }
        h2 {
        font-family: arial;
        font-size: 11pt;
        text-align:left;
        }
        h3 {
        font-family: verdana;
        font-size: 9pt;
        text-align:left;
        background-color: #F8F8FF;
        font-weight: normal;
        margin:0px;
        }
        h4 {
        font-family: verdana;
        font-size: 9pt;
        font-weight: bold;
        text-align:left;
        background-color: #DCDCDC;                                          
        margin:0px;
        }
        hr {
        padding:0px;
        margin:0px;
        }
        ->
    </STYLE>	
</head>
<body style="font-family: verdana; font-size: 9pt; font-weight: bold; text-align: center">
    <div id="body">
        <table width=100% >
            <tr>
                <td><h1 align="Left">  WorkFlow            </h1></td>
                <td><h1 align="Right"> Easy Export Control </h1></td>
            </tr>
            <tr>
                <td colspan=2>
                    <hr>
                    <h2> Embarque </h2>
                    <hr>
                </td>
            </tr>
        </table>	
        <p> O Embarque abaixo foi incluído com sucesso: </p>
    </div>
    <div id ="table">
        <table align="center" width="60%" cellpadding="0px" >
            <tr>
                <td><h4> Embarque:                                </h4></td>
                <td><h3> %EECEMB%      </h3></td>
            </tr>
            <tr>
                <td><h4> Data:                                  </h4></td>
                <td><h3> %EECDAT%      </h3></td>
            </tr>
            <tr>
                <td><h4> Exportador:                            </h4></td>
                <td><h3> %EECNEX%      </h3></td>
            </tr>
            <tr>
                <td><h4> Importador:                            </h4></td>
                <td><h3> %EECNIM%      </h3></td>
            </tr>
            <% If !Empty(oWorkFlow:RetVal("RESPON")) %>
                <tr>
                    <td><h4> Analista Responsável:                 </h4></td>
                    <td><h3> %RESPON%      </h3></td>
                </tr>
            <% EndIf %>
            <tr>
                <td><h4> Usuário do Sistema:                    </h4></td>
                <td><h3> %NMUSER%      </h3></td>
            </tr>
            <tr>
                <td><h4> Incoterm:                              </h4></td>
                <td><h3> %INCOTE%      </h3></td>
            </tr>
            <tr>
                <td><h4> Modal:                                 </h4></td>
                <td><h3> %MODAL%      </h3></td>
            </tr>
            <tr>
                <td><h4> Origem:                                </h4></td>
                <td><h3> %ORIGEM%      </h3></td>
            </tr>
            <tr>
                <td><h4> Destino:                               </h4></td>
                <td><h3> %DESTINO%      </h3></td>
            </tr>
            <% If oWorkFlow:RetVal("MODAL") $ "1" %> //(NÃ£o exibir se for aÃ©reo)
                <tr>
                    <td><h4> Embarcação:                        </h4></td>
                    <td><h3> %NNAVIO%   </h3></td> //Nome do Navio
                </tr>
            <% EndIf %>
            <tr>
                <td><h4> Nro. Voo\Viagem:                     </h4></td>
                <td><h3> %VIAGEM%   </h3></td>
            </tr>
            <tr>
                <td><h4> Data ETA:                              </h4></td>
                <td><h3> %DTETA%   </h3></td>
            </tr>
            <tr>
                <td><h4> Data ETD:                              </h4></td>
                <td><h3> %DTEDT%   </h3></td>
            </tr>
            <tr>
                <td><h4> Valor Total:                           </h4></td>
                <td><h3> %VLRTOT%      </h3></td>
            </tr>
            <tr>
                <td><h4> Peso Total:                            </h4></td>
                <td><h3> %PESTOT%      </h3></td>
            </tr>
        </table>
    </div>
    <p> Produtos: </p>
    <div id ="table1">
        <table align="center" width="70%" cellpadding="0px" >
            <tr>
                <td><h4> Código:       </h4></td>
                <td><h4> Descrição:    </h4></td>
                <td><h4> Quantidade:   </h4></td>
                <td><h4> Peso:         </h4></td>
                <td><h4> Preço:        </h4></td>
                <td><h4> Pedido:        </h4></td>
            </tr>
            <tr>
                <td><h3> %i.PRODUTO% 		</h3></td>
                <td><h3> %i.DESCRI%  		</h3></td>
                <td><h3><u> %i.QUANTI% </u></h3></td>
                <td><h3><u> %i.PESOPR% </u></h3></td>
                <td><h3><u> %i.PRCTOT% </u></h3></td>
                <td><h3> %i.PEDIDO%        </h3></td>
            </tr>
        </table>
        <p> Workflow Enviado em: %DATA%   </p>
    </div>
<p>&nbsp;</p>
</body>
</html>  