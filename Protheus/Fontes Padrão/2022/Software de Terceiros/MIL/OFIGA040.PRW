dbgotop()
dbseek("VG3")
aCampos := {}

do While !eof() .and. x3_arquivo == "VG3"

   If X3USO(x3_usado).and.cNivel>=x3_nivel
       aadd(aCampos,x3_campo)
   EndIf
       
   dbskip()
