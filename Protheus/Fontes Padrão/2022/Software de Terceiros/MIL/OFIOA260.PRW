Function OA260I(cAlias,nReg,nOpc)

CAMPOA260()
If AxInclui(cAlias,nReg,nOpc,aCampos) == 1
	If FG_SEEK("VOF","VSN->VSN_CODCOR + VSN->VSN_PRISMA",3,.f.)
   	Return .t.
	Else
	   DbSelectArea("VOF")
	   RecLock("VOF",.t.)
	   VOF_FILIAL := xFilial("VOF")
	   VOF_CODCOR := VSN->VSN_CODCOR
	   VOF_PRISMA := VSN->VSN_PRISMA
	   VOF_SITBOX := "D"       
	   MsUnLock()
   	DbSelectArea("VSN")
	EndIf
EndIf
Return .t.

