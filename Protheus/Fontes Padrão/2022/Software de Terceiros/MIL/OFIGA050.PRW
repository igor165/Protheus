DbSelectArea("SX3")
DbSetOrder(1)
dbseek("VG4")
aCampos := {}

do While !eof() .and. x3_arquivo == "VG4"
   If X3USO(x3_usado).and.cNivel>=x3_nivel
      aadd(aCampos,x3_campo)
   EndIf
      
   dbskip()
   
Enddo

DbSelectArea("VG4")

Return

