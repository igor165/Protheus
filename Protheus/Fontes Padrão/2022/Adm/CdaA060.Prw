Local aArea		:=	GetArea()
Local aAreaAH8	:=	AH8->(GetArea())
DEFAULT lAlt := .F.

If !Empty(M->AH8_PRODUT) .And. !Empty(M->AH8_CODPRO) 
	dbSelectArea("AH8")
	AH8->(dbSetOrder(1))
	If AH8->(dbSeek(xFilial("AH8") + M->AH8_PRODUT)).and. !lAlt
		Help(" ",1,"CDAGRVMST")
		lRet := .F.
	EndIf	
Else
	lRet := .F.
EndIf

RestArea(aAreaAH8)
RestArea(aArea)

Return lRet


