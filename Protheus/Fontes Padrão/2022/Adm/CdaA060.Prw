Function CD060Del(cAlias,nReg,nOpc)

Local nLinhas := 0
Local nOpcA
Local lVar01  := 1
Local bCampo
Local oDlg 
Local aSize := MSADVSIZE()
Local aObjects := {}      
Local aInfo
Local aPosObj
//⁄ƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒø
//≥ Monta a entrada de dados do arquivo                          ≥
//¿ƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒŸ
PRIVATE aTELA[0][0]
PRIVATE aGETS[0]

bCampo := {|nCPO| Field(nCPO)}    



AAdd( aObjects, { 100, 100, .t., .t. } )
aInfo := { aSize[ 1 ], aSize[ 2 ], aSize[ 3 ], aSize[ 4 ], 3, 3 }
aPosObj := MsObjSize( aInfo, aObjects )



While .T.

	//⁄ƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒø
	//≥ Envia para processamento dos Gets          ≥
	//¿ƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒƒŸ
	nOpcA:=0
             


	DEFINE MSDIALOG oDlg TITLE cCadastro From aSize[7],0 To aSize[6],aSize[5] OF oMainWnd PIXEL //     

		nOpcA := EnChoice(cAlias,nReg,nOpc,,"AC",STR0007,,aPosObj[1]) //"Quanto Ö exclusÑo?"
		
		nOpc  := 1

	ACTIVATE MSDIALOG oDlg CENTERED ON INIT EnchoiceBar(oDlg,{|| nOpca := 2,oDlg:End()},{|| nOpca := 1,oDlg:End()}) 

	If nOpcA == 2

		dbSelectArea("AH1")
		dbSeek(xFilial("AH1") + AH8->AH8_PRODUT)

		If !Eof()
			Help(" ",1,"EXISTPRD")
			dbSelectArea(cAlias)
			MsUnLock()
			Exit
		Endif

		BEGIN TRANSACTION
			
			dbSelectArea(cAlias)
			RecLock(cAlias,.F.,.T.)
			dbDelete()	
			WriteSx2(cAlias)

		END TRANSACTION
	Else
		MsUnLock()
	EndIf
	Exit
End
dbSelectArea(cAlias)

Return (nOpca)

