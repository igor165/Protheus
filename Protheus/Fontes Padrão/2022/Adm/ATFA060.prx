oModel := MPFormModel():New('ATFA060',/*preValid */,{|oModel|AF060TDOK(oModel)}/*bPosValidacao*/,{ |oModel| af060Commit( oModel ) }, /*bCancel*/ )

//Criando os AddFields e AddGrid
