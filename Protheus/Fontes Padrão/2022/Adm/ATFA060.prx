			aEntidades[2] := IIf(oModFNS:GetValue("FNS_CCDDD")  == "*", oModFNS:GetValue("FNS_CCDDO") , oModFNS:GetValue("FNS_CCDDD"))
			aEntidades[3] := IIf(oModFNS:GetValue("FNS_ITDEDD") == "*", oModFNS:GetValue("FNS_ITDEDO"), oModFNS:GetValue("FNS_ITDEDD"))
