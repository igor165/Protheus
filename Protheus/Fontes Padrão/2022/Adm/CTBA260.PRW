	dbSeek(xFilial()+mv_par03)
	IF !Found() .Or. CT1->CT1_NCUSTO = 0 
		Help(" ",1,"NOGERACTB")
		dbGoto(nRec)
		RestArea(aSaveArea)
		Return
	EndIF
