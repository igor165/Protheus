Function Af280SN1()
Local lRet := .T. 
Local aArea := { Alias(), Indexord() }

If !Empty(m->nt_cbase) .And. !Empty(m->nt_item)  .Or. ;
	(Empty(m->nt_cbase) .and. Empty(m->nt_item) )
	dbSelectArea("SN1")
	dbSetOrder(1)
	If !dbSeek(xFilial("SN1")+m->nt_cbase+m->nt_item)
		Help(" ",1,"AF210SN1")   // Nao esta cadastrado no SN1		
		lRet := .F.
	EndIf
EndIf

dbSelectArea( aArea[1] )
dbSetOrder( aArea[2] )
Return lRet
