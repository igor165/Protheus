Function FA013NUM()
Local lRet 		:= .T.
Local nRecno 	:= SEY->(Recno())
Local aArea		:= GetArea()
Local aAreaSEY := SEY->(GetArea())

If !Empty(M->EY_COBRAD) .And. !Empty(M->EY_RECINI)
	SEY->(DbSetOrder(4))
	If SEY->(DbSeek(xFilial("SEY")+M->EY_SERIE+M->EY_RECINI+M->EY_COBRAD))
		Help(" ",1,"FA013NUM")
		SEY->(DbGoTo(nRecno))
		lRet := .F.	
	EndIf
EndIf

RestArea(aAreaSEY)
RestArea(aArea)
Return lRet