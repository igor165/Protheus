Private cProveei:= ''
Private cTiendai:= ''
Private cProveef:= ''
Private cTiendaf:= ''
Private dFechai := ''
Private dFechaf := ''
Private cOrdeni := ''
Private cOrdenf := ''
Private cUbicac := ''
Private cNombre := ''
Private cTipoOr := ''

If Pergunte(cPerg,.T.)
	If TodoOk(cPerg)
		dbSelectArea("SA6")
		SA6->(dbSetOrder(1))
		SA6->(dbSeek(xFilial("SA6")+cBanco))
		cLayout := SA6->A6_CNAB2
		cNomBan := SA6->A6_NOME
		GeraHeader()
		MsAguarde({|| GeraDetail()}, STR0001, , .T.)//"Seleccionando Información"
		If nCount > 0
			GeraTraill()
			FN883DOC()()
		EndIf
	Else
		FINA883()
	EndIf
EndIf
Return

