Function FINA883()
Private cPerg := "FINA883"
Private aHeader := {}
Private aDetail := {}
Private aTraill := {}
Private aOrden  := {}
Private cEOL    := CHR(13)+CHR(10)
Private nLinea  := 500+Len(cEOL)
