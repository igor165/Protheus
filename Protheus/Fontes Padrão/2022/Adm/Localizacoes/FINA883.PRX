Static Function TodoOK(cPerg)
Local lRet := .F.

Pergunte(cPerg,.F.)
cBanco  := MV_PAR01
cSucurs := MV_PAR02
cCuenta := MV_PAR03
cProveei:= MV_PAR04
cTiendai:= MV_PAR05
cProveef:= MV_PAR06
cTiendaf:= MV_PAR07
dFechai := MV_PAR08
dFechaf := MV_PAR09
cOrdeni := MV_PAR10
cOrdenf := MV_PAR11
cUbicac := MV_PAR12
cNombre := MV_PAR13
cTipoOr := MV_PAR14

If Len(Alltrim(cBanco)) == 0
	Aviso(STR0002, STR0004, {STR0003})//"Ingresa el código de banco"
ElseIf Len(Alltrim(cSucurs)) == 0
	Aviso(STR0002, STR0005, {STR0003})//"Ingresa el código de sucursal"
ElseIf Len(Alltrim(cCuenta)) == 0
	Aviso(STR0002, STR0006, {STR0003})//"Ingresa el número de cuenta"
ElseIf Len(Alltrim(cProveef)) == 0
	Aviso(STR0002, STR0007, {STR0003})//"Ingresa el código de proveedor final"
ElseIf Len(Alltrim(cTiendaf)) == 0
	Aviso(STR0002, STR0008, {STR0003})//"Ingresa el código de tienda final"
ElseIf Len(Alltrim(DTOS(dFechaf))) == 0
	Aviso(STR0002, STR0009, {STR0003})//"Ingresa la fecha final"
ElseIf Len(Alltrim(cOrdenf)) == 0
	Aviso(STR0002, STR0010, {STR0003})//"Ingresa orden de pago final"
ElseIf Len(Alltrim(cUbicac)) == 0
	Aviso(STR0002, STR0011, {STR0003})//"Ingresa la ruta donde se guardará el archivo"
ElseIf !ExistDir(Alltrim(cUbicac))
	Aviso(STR0002, STR0012, {STR0003})//"El directorio seleccionado no existe"
ElseIf Len(Alltrim(cNombre)) == 0
	Aviso(STR0002, STR0013, {STR0003})//"Ingresa el nombre del archivo"
Else
	lRet := .T.
EndIf
Return lRet

