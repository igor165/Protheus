function formataData (cData) {
  var cResultado;
  if (cData != "" && cData != undefined) {
	var cParte = cData.match(/\d+/g),
	cAno = cParte[0].substring(0,4);
	cMes = cParte[1];
	cDia = cParte[2];
	cResultado = cDia+'/'+cMes+'/'+cAno;
  } else {
 	cResultado = ""
  }
  return cResultado
}

