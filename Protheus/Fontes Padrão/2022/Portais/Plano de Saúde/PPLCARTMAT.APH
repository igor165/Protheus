	if (aResult[0] == 'S') {
	    aDig = aResult[2].split("@");
		cDig = aDig[0];
		cPar = 'Z';
		document.TotvsBioForm.digital.value = aResult[2]+aResult[3]+'@'+aResult[4];
		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
