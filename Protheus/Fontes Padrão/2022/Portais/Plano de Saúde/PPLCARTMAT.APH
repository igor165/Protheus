function CarregaVldTri(v) {
    
	var aResult
	var critica = ""

	if (v.length == 1) {
		aResult = v.split(" ");
    	cCapIde		= aResult[1];
	
   }else {
		aResult = v.split("|");
		cCapIde		= aResult[1];
   }  
	
	if (aResult[0] == "T") {
		
		for (var i=1;i<aResult.length;i++)
		{ 
			critica += aResult[i]			
		}
		
		alert(critica)
	}

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
