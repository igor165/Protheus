function ExibeErro(v) {                   
	var aResult = v.split("|"); 
	
	if (aResult[0] != "true" && aResult[0] != "false")
	    alert("Erro: " + aResult[0])
	else {   
		if (aResult[0] == "false") {
		  alert(aResult[1]);          
