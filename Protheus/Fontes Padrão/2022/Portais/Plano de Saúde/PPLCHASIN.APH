function fCheckMat(oObj) {

	var lRet = true;
	
	if ( !isEmpty( getField('Field_MATUSU') ) )	 {
		if (lRet)
			 return getGatCmp('PLGATBA1','Field_MATUSU',null,1);
	 	else return lRet;	
	} else {
		setField("Field_NOMUSR","");
	}
}	                 
