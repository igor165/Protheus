    if ( !validaFieldOb() ) return;                       

	ChamaPoP('W_PPLRELGEN.APW?cFunName=PPRELSIN&Field_MATUSU=' + clearMark(getField("Field_MATUSU"))+'&Field_ANO=' + clearMark(getField("Field_ANO"))+'&Field_MES=' + clearMark(getField("Field_MES"))+"&cRotinLGPD=PPLCHASIN",'bol','yes',0,925,605)
}
//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
