			function dinMenu(link){
				link.parentNode.parentNode.parentNode.childNodes[1].click();
				if($('#btn-toggle').css('display') == "block")
					$('#btn-toggle').click();
				return true;
			}

			function fSearchMenu(){
				var obj = document.querySelector('.navbar-form').menuSelect;
				if(obj.value != ""){	
					 $('#btnSelMenu').popover('destroy');
					window.frames[0].location = obj.value;
				}
				else{
					 $("#btnSelMenu").popover();
					 $('#btnSelMenu').popover('show');
				}
			}

			//Valida o browser utilizado pelo usuário
			//Documentação sobre browser homologados no TDN: http://tdn.totvs.com/display/PROT/Navegadores+homologados+para+o+Portal+PLS
			function checkBrowser() {
				var cPortal = "";
				var cSessionMportal = "<%= HttpSession->MPortal %>";
				var nSessionTpPor = <%= nTpPor %>;

				if (cSessionMportal == "000010"){
					if (nSessionTpPor == 2){
						cPortal = "empresa";
					} else {
						cPortal = "beneficiario";
					}
				} else {
					cPortal = "prestador";
				}

				if (typeof navigator.userAgent !== 'undefined'){
					var b = detect.parse(navigator.userAgent);
					
					if (cPortal == "beneficiario"){
						//Portal do beneficiario homologado no IE 11, Edge, Firefox e Chrome
						if ((b.browser.family == "IE" && parseInt(b.browser.version) < 11) || (b.browser.family == "Chrome" && parseInt(b.browser.version) < 48)){
							modalBS('<i class="fa fa-exclamation-triangle" style="font-weight: bold; color: #000; text-shadow: 0 1px 0 #fff; filter: alpha(opacity=20); opacity: .2;"></i>&nbsp;&nbsp;&nbsp;Atenção', '<p>Esta versão do navegador não está homologada para utilização do Portal do Beneficiário.<br>Navegador: ' + b.browser.family + ' - Versão: ' + b.browser.version + '<br>Consulte os navegadores homologados no TDN clicando <a href="http://tdn.totvs.com/display/PROT/Navegadores+homologados+para+o+Portal+PLS" target="_blank">aqui.</a></p>', "@Voltar~javascript:location.href='W_PPLSOUT.APW'", "white~#960000", "large", "N");
						}
					} else if (cPortal == "prestador") {
						//Navegadores serão homologados no portal do prestador futuramente
						if ((b.browser.family == "IE" && parseInt(b.browser.version) < 11) || (b.browser.family == "Chrome" && parseInt(b.browser.version) < 48)){
							modalBS('<i class="fa fa-exclamation-triangle" style="font-weight: bold; color: #000; text-shadow: 0 1px 0 #fff; filter: alpha(opacity=20); opacity: .2;"></i>&nbsp;&nbsp;&nbsp;Atenção', '<p>Esta versão do navegador não está homologada para utilização do Portal do Beneficiário.<br>Navegador: ' + b.browser.family + ' - Versão: ' + b.browser.version + '<br>Consulte os navegadores homologados no TDN clicando <a href="http://tdn.totvs.com/display/PROT/Navegadores+homologados+para+o+Portal+PLS" target="_blank">aqui.</a></p>', "@Voltar~javascript:location.href='W_PPLSOUT.APW'", "white~#960000", "large", "N");
						}
					} else if (cPortal == "empresa") {
						//Navegadores serão homologados no portal da empresa futuramente
						if (b.browser.family == "IE"){
							modalBS('<i class="fa fa-exclamation-triangle" style="font-weight: bold; color: #000; text-shadow: 0 1px 0 #fff; filter: alpha(opacity=20); opacity: .2;"></i>&nbsp;&nbsp;&nbsp;Atenção', '<p>Este navegador não está homologado para utilização do Portal da Empresa. <br>Navegador: ' + b.browser.family + ' - Versão: ' + b.browser.version + '<br>Consulte os navegadores homologados no TDN clicando <a href="http://tdn.totvs.com/display/PROT/Navegadores+homologados+para+o+Portal+PLS" target="_blank">aqui.</a></p>', "@Voltar~javascript:location.href='W_PPLSOUT.APW'", "white~#960000", "large", "N");
						}			
					}
				}
			}
		</script>
		
