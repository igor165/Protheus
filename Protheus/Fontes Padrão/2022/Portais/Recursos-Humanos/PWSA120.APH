		<tr>
			<th><%=OemToAnsi(STR0009)%></th><!-- Matricula -->
			<th><%=OemToAnsi(STR0004)%></th><!-- Nome -->
			<th><%=OemToAnsi(STR0005)%></th><!-- Admissao -->
			<th>
				<% If HttpSession->lGSP %>
                	<%=OemToAnsi(STR0036)%> <!-- Lotacao -->				
            	<% Else %>				
    				<%=OemToAnsi(STR0007)%> <!-- Departamento -->
            	<% EndIf %>
