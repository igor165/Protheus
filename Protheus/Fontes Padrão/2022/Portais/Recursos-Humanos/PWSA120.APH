			<option value="5" <%=IIF(HttpGet->FilterField == "5", "selected", '')%>><%=STR0010%></option><!-- Situacao -->
		</select>
		<input name="txtfiltro" type="text" id="txtfiltro" size="50" maxlength="50" class="Texto" value="<%=HttpGet->FilterValue%>">
