<%      
#INCLUDE "PWSC079.CH"
Local nI 			:= 0
Local nJ 			:= 0
Local nTam 			:= Len( HttpSession->PWSC079INFO )
Local nTamHeader 	:= Len( HttpSession->PWSC079INFO[1] )
%>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
<link href="<%=GetPrtSkin()%>/estilo.css" rel="stylesheet" type="text/css">
<script language="Javascript">
<!--
function fDetalhes()
{
	document.forms[0].action = "";
	document.forms[0].submit();
}
//-->
</script>
</head>
<body bgcolor="#FFFFFF" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0" class="FundoClaro">
<form name="form1" method="post" action="">
  <table height="100%" border="0" cellpadding="0" cellspacing="0">
    <tr> 
      <td width="6" valign="top" class="FundoPontilhadoVertical"><img src="<%=GetPrtSkin()%>/transparente.gif" width="6" height="6"></td>
      <td valign="top" class="FundoClaro"> 
        <table border="0" align="center" cellpadding="10" cellspacing="0">
          <tr> 
            <td><p align="justify" class="titulo">&raquo; <%=HttpSession->PWSC079APH[1]%></p>
              <table width="<%=80*nTamHeader%>" border="1" align="center" cellpadding="0" cellspacing="0" bordercolor="#E5E5E5">
                <tr> 
                  <td bordercolor="#FFFFFF" class="titulo">
                    <table border="0" align="center" cellpadding="2" cellspacing="1" bgcolor="#EDEDED">
                      <% For nI := 1 To nTam %>
                      <tr>               
                        <td bgcolor="#EDEDED" width="5">&nbsp;</td>
                        <% For nJ := 1 To nTamHeader %>
                          <% If nI == 1 %>
                        <td bgcolor="#EDEDED" class="TituloMenorBold" width="80" valign="top"><%=HttpSession->PWSC079INFO[nI][nJ]%></td>
                          <% Else %>
                        <td bgcolor="#EDEDED" class="TituloMenor" width="80" valign="top"><%=HttpSession->PWSC079INFO[nI][nJ]%></td>
                          <% EndIf %>
                        <% Next nJ %>
                      </tr>
                      <% Next nI %>
                    </table>
                  </td>
                </tr>
              </table>
            </td>
          </tr>
          <tr>
            <td>
              <table align="center">
                <tr>
                  <td><input type="button" name="button" value=<%= STR0001 /*"voltar"*/%> class="Botoes" onClick="window.location='W_PWSC076.APW?cOP=<%=HttpPost->cOP%>'"></td>
                </tr>
              </table>
            </td>
          </tr>
        </table>
      </td>
    </tr>
  </table>
</form>
</body>
</html>