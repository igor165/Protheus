Function CRDA190TudoOk(cSerie,cNota,cPDV)
Local lRet  	 := .T.
Local aArea 	 := GetArea() 
Local nTamL1_DOC := TamSx3("L1_DOC")[1]         //(Tratamento tamanho Nota Fiscal)   

cNota := PadR(cNota,nTamL1_DOC)                                      


// So permitir que o conteudo das variaveis MAH_TRANS e MAH_STATUS sejam = "1"
// ---------------------------------------------------------------------------
If M->MAH_STATUS <> "1" .or. M->MAH_TRANS <> "1"
	MsgStop("Os campos MAH_STATUS e MAH_TRANS devem estar preenchidos com o conteudo 1.")
	M->MAH_STATUS := "1"
	M->MAH_TRANS  := "1"
	Return .F.
EndIf

// Zerar algumas variaveis
// -----------------------
M->MAH_DTCANC := ctod("")
M->MAH_HRCANC := ""
M->MAH_USERCA := ""
M->MAH_LJTRN  := ""

DbSelectArea( "SL1" )
DbSetOrder( 2 )	// Filial + Série + NF +PDV

If SL1->( DbSeek( xFilial( "SL1" ) + cSerie + cNota + cPDV ) )
	RecLock("SL1",.F.)
	SL1->L1_CONTRA := M->MAH_CONTRA
	MsUnlock()
Else
	lRet := .F.
Endif

RestArea(aArea)

Return(lRet)