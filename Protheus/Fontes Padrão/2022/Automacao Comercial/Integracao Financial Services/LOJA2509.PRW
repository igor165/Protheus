Method VerifEnvio(nVlrMin) Class LJClTitCAP

	Local lRet := .F.  
	Local aArea := GetArea() 
    Local cOperac := PadR("1", TamSX3("MEQ_OPERAC")[1])
    
    DEFAULT nVlrMin := 0 //Valor Mínimo para envio

  	MEQ->(DbSetOrder(2)) 		//MEQ_FILIAL+MEQ_PREFIX+MEQ_NUM+MEQ_PARCEL+MEQ_TIPO+MEQ_OPERAC                                                                                      
    
    If ( nVlrMin = 0 .OR. (Self:cTitAtras)->SLDDEVCTT >=  nVlrMin ) .AND.   MEQ->(!DbSeek(xFilial("MEQ")+(Self:cTitAtras)->(E1_PREFIXO+E1_NUM+E1_PARCELA+E1_TIPO+cOperac)))
       	lRet := .T.	   		
	EndIf   
      	    			
	RestArea(aArea)

Return lRet
                                              
