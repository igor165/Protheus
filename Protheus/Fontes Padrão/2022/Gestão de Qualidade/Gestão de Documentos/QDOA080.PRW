			cDepto := Right(ALLTRIM(aConteudo[1][1]),Len(ALLTRIM(aConteudo[1][1])) - AT("-",ALLTRIM(aConteudo[1][1])))
			// Remoo nome do usurio
			cNome  := Alltrim(Substring(cDepto,aConteudo[1][3][2][3]+1,aConteudo[1][3][3][3]))

